@Article{Desbiens2005,
  Title                    = {Water condensation in hydrophobic silicalite-1 zeolite: a molecular simulation study},
  Author                   = {N. Desbiens and A. Boutin and I. Demachy},
  Journal                  = {J. Phys. Chem. B},
  Year                     = {2005},
  Number                   = {50},
  Pages                    = {24071–-24076},
  Volume                   = {109},

