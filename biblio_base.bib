@Article{Stahl2002,
  Title                    = {Reactions of ethynyl radicals as a source of {C}$_4$ and {C}$_5$ hydrocarbons in {T}itan's atmosphere},
  Author                   = {F. Stahl and P. v. R. Schleyer and H. F. Schaefer III and R. I. Kaiser},
