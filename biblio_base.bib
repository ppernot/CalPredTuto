@Article{Chassefiere91,
  Title                    = {Stratospheric depletion of carbon monoxide on Titan},
  Author                   = {Chassefiere, Eric and Cabane, Michel},
  Journal                  = {Geophysical Research Letters},
  Year                     = {1991},
  Number                   = {3},
  Pages                    = {467-70},
  Volume                   = {18},

  Abstract                 = {The stratospheric depletion of CO obsd. in Titan's atm. is attributed to the adsorption of CO on aerosols in the region of Titan's atm. where CH4 is expected to condense. Particulate CH4 condensates could transport enough CO to generate the obsd. tropospheric enrichment provided the sticking coeff. (fraction of collisions producing adsorption) is 6 * 10-4, assuming the same eddy diffusion coeff. as that of the Earth's troposphere. If Titan's troposphere has a lower eddy diffusion coeff. than that obsd. for Earth, even smaller values of the sticking coeff. (e.g., approx. 10-6) would be required. This hypothesis, if confirmed by lab. measurements, would provide indirect evidence for the presence of large amts. of CH4 ice in Titan's atm. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, stratosphere of, carbon monoxide depletion in, from hypothetical adsorption on condensing methane aerosols) methane carbon monoxide Titan atm},
