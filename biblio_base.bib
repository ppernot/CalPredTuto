@Article{Lellouch03a,
  Title                    = {Titan's 5-[mu]m window: observations with the Very Large Telescope},
  Author                   = {Lellouch, E. and Coustenis, A. and Sebag, B. and Cuby, J. G. and Lopez-Valverde, M. and Schmitt, B. and Fouchet, T. and Crovisier, J.},
  Journal                  = {Icarus},
