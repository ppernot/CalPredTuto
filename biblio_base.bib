@Article{Zubrin92,
  Title                    = {Nuclear thermal rocketry using indigenous Martian propellants},
  Author                   = {Zubrin, Robert M.},
  Journal                  = {Space Nuclear Power Systems},
  Year                     = {1992},
  Note                     = {CAN 118:262627 71-13 Nuclear Technology Martin Marietta Astronaut.,Denver,CO,USA. Journal 1041-2824 written in English. 74-82-8 (Methane); 124-38-9 (Carbon dioxide); 630-08-0 (Carbon monoxide); 7440-37-1 (Argon); 7727-37-9 (Nitrogen); 7732-18-5 (Water) Role: USES (Uses) (propellant of, for nuclear thermal rocket for Mars mission)},
  Pages                    = {131-40},
  Volume                   = {10},

  Abstract                 = {This paper presents a preliminary examn. of a novel concept for a Mars and outer solar system exploratory vehicle. Propulsion is provided by using a nuclear thermal reactor to heat a propellant volatile indigenous to the destination world to form a high thrust rocket exhaust. Candidate propellants whose performance, materials compatibility, and ease of acquisition are examd. include carbon dioxide, water, methane, nitrogen, carbon monoxide, and argon. Ballistic and winged supersonic configuration are discussed. The use of this method of propulsion potentially offers high payoff to a manned Mars mission, both by sharply reducing the initial mission mass required in low Earth orbit, and by providing Mars explorers with greatly enhanced mobility in traveling about the planet through the use of a vehicle that can refuel itself each time it lands. Using the nuclear landing craft in combination with a hydrogen fueled nuclear thermal interplanetary vehicle and a heavy lift booster, it is possible to achieve a manned Mars mission in one launch. Using such a system in the outer solar system, it is found that a low level aerial reconnaissance of Titan combined with a multiple sample return from nearly every satellite of Saturn can be accomplished in a single launch of a Titan IV or STS. [on SciFinder (R)]},
  Keywords                 = {Propellants (indigenous Martian) nuclear thermal rocket propellant Mars mission},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

