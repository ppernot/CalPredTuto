@Article{McLain2009a,
  Title                    = {Flowing afterglow studies of temperature dependencies for electron dissociative recombination of {HCNH$^+$}, {CH$_3$CNH$^+$} and {CH$_3$CH$_2$CNH$^+$} and their symmetrical proton-bound dimers},
  Author                   = {J.L. McLain and N.G. Adams},
