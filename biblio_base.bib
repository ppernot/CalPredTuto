@Article{Guez97,
  Title                    = {Importance of phase changes in Titan's lower atmosphere. Tools for the study of nucleation},
  Author                   = {Guez, L. and Bruston, P. and Raulin, F. and Regnaut, C.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1997},
  Note                     = {CAN 127:250863 53-9 Mineralogical and Geological Chemistry Laboratoire Interuniversitaire des Systemes Atmospheriques,Universities Paris 7 and Paris 12, CNRS-U.R.A. 1404,Creteil,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (importance of phase changes in Titan's (satellite of Saturn) lower atm. and the study of nucleation)},
  Number                   = {6},
  Pages                    = {611-625},
  Volume                   = {45},

  Abstract                 = {The uncertainty about possible supersatn. of methane, condensation of volatile species, and the existence of clouds in Titan's lower atm. affects our understanding of photochem., the nature of the surface and the atm. thermal structure. Indeed, photochem. depends on the depth of penetration of energetic photons, affected by methane abundance. Radar and IR observations of bright surface regions have been explained by rain washing of highlands. As for the thermal profile, it is sensitive to CH4-N2 gas opacity, cloud opacity and could be influenced by latent heat exchange. A rudimentary model with no methane supersatn. and gas transport by eddy diffusion indicates a methane latent heat release of 0.2 W m-2 between 20 and 30 km altitude for a surface mole fraction of 4.4% and an eddy diffusion coeff. of 0.2 m2s-1. Description of nucleation seems to be one of the first improvements which should be included in a model of phase changes. The suspicion of difficult methane nucleation comes from anal. of Voyager IRIS spectra. Moreover, species are expected to condense to the solid phase, which excludes very efficient nucleation and condensation processes assocd. with the presence of a liq. phase, such as deliquescence. The classical theory of heterogeneous nucleation, despite its deficiencies, is employed in atm. models, owing to its general nature and relative simplicity. Yet, it requires phys. quantities for which exptl. values do not exist. It is shown how surface free enthalpies of solids and contact angles may be linked to other material properties which are within reach of lab. expts., mainly UV absorption spectra of solid phases. It is found that a value of 10-9-10-7s-1 for the \"crit. nucleation rate\" (per nucleus) is adapted to the case of Titan, though we question the ability of the crit. rate concept to make predictions for the condensation altitudes. A possible consequence of difficult methane nucleation is periodic evolution of the lower atm., on a timescale of the order of 102 years. [on SciFinder (R)]},
  Keywords                 = {Atmosphere; Clouds; Rainwater; Titan; UV absorption (importance of phase changes in Titan's (satellite of Saturn) lower atm. and the study of nucleation) planet Saturn Titan satellite atm nucleation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

