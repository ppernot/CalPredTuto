@Article{Bar-Nun85,
  Title                    = {Trapping and release of gases by water ice and implications for icy bodies},
  Author                   = {Bar-Nun, A. and Herman, G. and Laufer, D. and Rappaport, M. L.},
  Journal                  = {Icarus},
  Year                     = {1985},
  Number                   = {3},
  Pages                    = {317-32},
  Volume                   = {63},

  Abstract                 = {The trapping and release of H2, CO, CO2, CH4, Ar, Ne, and N2 by amorphous water ice was studied exptl. under dynamic conditions, at low temps. starting at 16 K, with gas pressures of 5 * 10-8 to 10-6 torr. CO, CH4, Ar, and N2 were released in 3 or 4 distinct temp. ranges, each resulting from a different trapping mechanism: (1) 30-55 K, where the gas frozen on the water ice evaps.; (2) 135-155 K, where gas is squeezed out of the water ice during the transformation of amorphous ice to cubic ice; (3) 165-190 K, where gas and water are released simultaneously, probably by the evapn. of a clathrate hydrate and, occasionally, (4) 160-175 K, where deeply buried gas is released during the transformation of cubic ice to hexagonal ice. If the 3rd range is indeed due to clathrate formation, CO was found to form this compd. CO2 did not form a clathrate under the exptl. conditions. Excess H did not affect the occlusion of other gases. H itself was trapped only at 16 K. Ne was not trapped at 25 K. With cubic ice, the only trapping mechanism is freezing of gas on the ice surface. No fractionation between the gas phase and the ice was obsd. with a mixt. of CO and Ar. Massive ejection of ice grains was obsd. during the evapn. of the gas in 3 (1, 3, 4) out of the 4 ranges. The exptl. results are used to explain several cometary phenomena, esp. those occurring at large heliocentric distances, and are applied also to Titan's atm. compn. and to the possible ejection of ice grains from Enceladus. [on SciFinder (R)]},
  Keywords                 = {Comets; Space (icy bodies of, implications of trapping and release of gases by water ice for); Planets (Saturn, Titan satellite, atm. of, icy bodies in, implications of trapping and release of gases by water ice for) water ice gas release trapping; Titan atm Enceladus ice},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Bar-Nun79,
  Title                    = {The photochemistry of hydrocarbons in Titan's atmosphere},
  Author                   = {Bar-Nun, Akiva and Podolak, Morris},
  Journal                  = {Icarus},
  Year                     = {1979},
  Number                   = {1},
  Pages                    = {115-22},
  Volume                   = {38},

  Abstract                 = {Results of the far UV photolysis of CH4 dild. 1000 times with Ar are presented for temps. of 300 and 200 K. C2H4 and C2H2 reached a temp.-dependent steady state, while C2H6 and C3H8 continued to rise linearly with time. The exptl. C2H4 column abundance in a photochem. steady state was extrapolated to the temps. expected in the CH4 photolysis layer on Titan. This, together with prior observations, make it possible to set a lower limit to the altitude of the top of Titan's aerosol layer, below which the C2H4 is protected from photolysis. This is 4.8 or 7.2 scale heights above the surface, for mean temps. in the photolysis layer of 130 and 160 K, resp. For 130 K, the C2H4 abundance above the inversion base agrees well with Gillett's observations, and leads to an aerosol cloud top coincident with the inversion base. In order to transport C2H4 to the cold (78 K) \"surface\" with a flux sufficiently large so that the rate of condensation equals the rate of prodn., an eddy diffusion coeff. at the surface of at least 102 cm2 s-1 is required. The exptl. results lead to an C2H2 abundance in a photochem. steady state which is .apprx.2 orders of magnitude smaller than the one suggested earlier. Since C3H8 is one of the major products, the possibility of observing it at 13.4 mm is suggested. [on SciFinder (R)]},
  Keywords                 = {Photolysis (of hydrocarbons, Titan atm. in relation to); Hydrocarbons Role: RCT (Reactant), RACT (Reactant or reagent) (photochem. of, in Titan's atm.); Planets (Saturn, photochem. of hydrocarbons in atm. of satellite Titan) Titan atm methane},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

