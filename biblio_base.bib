@Article{Pollack86,
  Title                    = {Estimates of the bolometric albedos and radiation balance of Uranus and Neptune},
  Author                   = {Pollack, James B. and Rages, Kathy and Baines, Kevin H. and Bergstralh, Jay T. and Wenkert, Daniel and Danielson, G. Edward},
  Journal                  = {Icarus},
  Year                     = {1986},
  Note                     = {CAN 104:233549 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Space Sci. Div.,NASA/Ames Res. Cent.,Moffett Field,CA,USA. Journal 0019-1035 written in English. 74-82-8; 1333-74-0 Role: PRP (Properties) (spectral lines of, in spectra of Neptune and Uranus)},
  Number                   = {2-3},
  Pages                    = {442-66},
  Volume                   = {65},

  Abstract                 = {An attempt was made to bound the wavelength-averaged phase integrals and bolometric albedos of Uranus and Neptune by fitting a wide range of aerosol model atmospheres to their obsd. geometric albedo spectra. These models are characterized by an upper haze layer of finite optical depth and a lower cloud layer of infinite optical depth at discrete altitudes. Alternative models differ in the assumed value of the particles' single scattering phase function and the wavelength dependence of the haze optical depth. Phase functions ranging from isotropic to those characteristic of particles in the atmospheres of Titan, Jupiter, and Saturn are considered. The models of Uranus were partially tested by comparing the dependence of their disk-integrated brightness on phase angle with that derived from a combination of ground-based and Voyager 1 data that span phase angles of 0-85 Deg and by comparing their predicted shapes of several H2 quadrupole lines with obsd. shapes. Predictions of the Neptune models were compared with detns. of the planet's disk-integrated brightness at 0-48 Deg phase angle. The derived model parameters lie within useful bounds. In the case of Uranus, the cloud pressure for all 7 models considered falls between 2.2 and 2.5 bars, implying CH4 mixing ratios in the deeper portion of the atm. that are >=30 times higher than expected from solar elemental abundances if the cloud is interpreted as being a CH4 condensation cloud. The range of haze pressure (?0.5 bars) and optical depth (0.06-0.6 at 0.6435 mm) imply that haze aerosols are a significant absorber of sunlight and hence constitute a significant heating source in Uranus's upper troposphere and stratosphere. The results imply that the meteor. regimes in the observable atmospheres of Uranus and Neptune may be very different, with internal heat flux playing a more important role for Neptune than for Uranus. [on SciFinder (R)]},
  Keywords                 = {Planets (Neptune, bolometric albedos and radiation balance of); Planets (Uranus, bolometric albedos and radiation balance of) aerosol atm Uranus Neptune methane},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
