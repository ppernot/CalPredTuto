@Article{Lunine85,
  Title                    = {Thermodynamics of clathrate hydrate at low and high pressures with application to the outer solar system},
  Author                   = {Lunine, Jonathan I. and Stevenson, David J.},
  Journal                  = {Astrophysical Journal, Supplement Series},
  Year                     = {1985},
  Note                     = {CAN 103:145139 53-9 Mineralogical and Geological Chemistry Div. Geol. Planet. Sci.,California Inst. Technol.,Pasadena,CA,USA. Journal 0067-0049 written in English. 630-08-0D Role: OCCU (Occurrence) (dissocn. pressure-temp. curve for, entrapment of volatiles, in solar gaseous nebula in relation to); 7732-18-5DP; 7732-18-5DP; 7732-18-5DP Role: FORM (Formation, nonpreparative), PREP (Preparation) (formation of, kinetics and thermodn. of, outer Solar System in relation to); 1333-74-0 Role: USES (Uses) (in clathrate hydrates, compn. of outer Solar system satellites in relation to); 14798-03-9 Role: USES (Uses) (in nitrogen-contg. clathrate hydrate, clathrate stability under outer Solar system conditions in relation to); 74-82-8D Role: PRP (Properties) (phase diagram of, in presence of ammonia, outer Solar system evolution in relation to); 74-82-8D; 7727-37-9D Role: OCCU (Occurrence) (phase diagram, outer Solar system evolution in relation to)},
  Number                   = {3},
  Pages                    = {493-531},
  Volume                   = {58},

  Abstract                 = {The thermodn. stability of clathrate hydrate is calcd. under a wide range of temp. and pressure conditions applicable to Solar system problems, using a statistical mech. theory developed by J. H. van der Waals and J. C. Platteeuw (1959) and existing exptl. data on properties of clathrate hydrates and their components. At low pressure, dissocn. pressures and partition functions (Langmuir consts.) for CO clathrate (hydrate) were predicted, using the properties of clathrate contg. (as guests) mols. similar to CO. The comparable or higher propensity of CO to incorporate in clathrate relative to N2 is used to argue for high CO-to-N2 ratios in primordial Titan if N2 was accreted as clathrate. The relative incorporation of noble gases in clathrate from a solar compn. gas at low temps. is calcd. and applied to the case of giant-planet atmospheres and icy satellites. Nonsolar but well-constrained noble gas abundances will be measured by Galileo in the Jovian atm. if the obsd. C enhancement is due to the bombardment of the atm. by clathrate-bearing planetesimals sometime after planetary formation. The noble-gas abundances in Titan's atm. are also predicted under the hypothesis that much of the satellite's CH4 accreted as clathrate. Double occupancy of clathrate cages by H2 and CH4 in contact with a solar-compn. gas is examd., and it is concluded that potentially important amts. of H2 may have been incorporated in satellites as clathrate. The kinetics of clathrate formation is also examd., and it is suggested that, under thermodn. appropriate conditions essentially complete clathration of water ice could have occurred in high-pressure nebulae around giant planets by probably not in the outer solar nebula; comets probably did not aggregate as clathrate. At moderate pressures, the phase diagram for CH4 clathrate hydrate in the presence of 15% NH3 (relative to water) is constructed, and application to the early Titan amt. compn. is described. The high-pressure stability of CH4, N2, and mixed CH4-N2 clathrate hydrate is calcd.; conversion back to water and CH4 and/or N2 fluids or solids is predicted for pressures .gtorsim. 12 kilobars (independent of temp.) and temps. .gtorsim. 320 K (independent of pressure. The effect of NH3 is to shrink the T-P stability field of clathrate with increasing NH3 concn. These results imply that (1) clathrate is stable throughout the interior of Oberon- and Rhea-sized icy satellites, and (2) clathrate incorporated in the innersmost icy regions of Tian would have decompd., perhaps allowing buoyant CH4 to rise. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, compn. evolution of, clathrate hydrates in); Planets (outer, atms. and satellites of, compn. evolution and origin of, clathrate hydrates in) clathrate hydrate thermodn Solar system},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Lunine85a,
