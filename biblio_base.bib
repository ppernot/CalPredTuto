@Article{Tokano01a,
  Title                    = {Modelling of thunderclouds and lightning generation on Titan},
  Author                   = {Tokano, T. and Molina-Cuberos, G. J. and Lammer, H. and Stumptner, W.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2001},
  Number                   = {6},
  Pages                    = {539-560},
  Volume                   = {49},

  Abstract                 = {The likelihood of lightning generation in Titan's troposphere is investigated by theor. consideration and a numerical one-dimensional time-dependent thundercloud model. The main cloud electrification hypotheses proposed for terrestrial thunderstorms are examd. taking into account recent knowledge concerning Titan's atm. state, esp. condensation, and the lower ionosphere. Titan's thunderclouds may be quite rare because of the difficult methane nucleation and low convection energy, but once moist convection is triggered, a massive cloud contg. slowly falling solid and liq. methane particles may be expected. In this case the cloud rapidly attaches a large amt. of free electrons, which are abundant in Titan's troposphere due to the low abundance of electrophilic species. The entirely neg. space charge in the cloud causes within a few minutes a temporary max. elec. field of 2*106 V m-1 which is sufficient to initiate 20 km long neg. cloud-to-ground lightning strokes in Titan's lower troposphere. The collisional charging, on the other hand, appears to be less efficient since the charge transfer itself may be limited at Titan's cold temps. and no substantial charge redistribution takes place in the cloud due to the weak updraft and gravitation. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; modeling of methane thunderclouds and lightning generation on Titan); Clouds (methane thunderclouds; modeling of methane thunderclouds and lightning generation on Titan); Lightning (modeling of methane thunderclouds and lightning generation on Titan); Weather (storm, thunderstorm; modeling of methane thunderclouds and lightning generation on Titan) methane thundercloud lightning Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Tokano01,
  Title                    = {Three-dimensional modeling of the tropospheric methane cycle on Titan},
  Author                   = {Tokano, Tetsuya and Neubauer, Fritz M. and Laube, Manfred and McKay, Christopher P.},
  Journal                  = {Icarus},
  Year                     = {2001},
  Note                     = {CAN 136:40425 53-9 Mineralogical and Geological Chemistry Institut fuer Geophysik und Meteorologie,Universitaet zu Koeln,Cologne,Germany. Journal 0019-1035 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (three-dimensional general circulation model of methane in the troposphere of Titan)},
  Number                   = {1},
  Pages                    = {130-147},
  Volume                   = {153},

  Abstract                 = {The tropospheric methane cycle on Titan including advection, diffusion, condensation, pptn., evapn., and surface source is simulated by a three-dimensional general circulation model. The model takes into account the different nucleation of methane in Titan's troposphere, and assumes condensation whenever the satn. ratio (relative humidity) exceeds a crit. value of either 100 or 150%. The simulation with the latter crit. satn. is shown to be more compatible with the Voyager data. The meridional circulation accounts for a seasonal methane transport from the winter to summer hemisphere in the mid-troposphere, causing a higher supersatn. in late summer and early autumn. Condensation preferentially occurs in this season at 15 km altitude at low latitudes. The occasional character may be explained by the high crit. satn. ratio, which cannot be often exceeded. Falling rain evaps. during the descent, so no rain arrives at the surface. The hydrol. cycle of methane on Titan takes place mostly within the atm. The latitudinal methane distribution near the surface cannot be reproduced by transport, evapn., condensation, and combinations of them, indicating some undetd. surface processes affecting the methane distribution. The zonal and meridional circulation is sensitive to the methane distribution. (c) 2001 Academic Press. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; three-dimensional general circulation model of methane in the troposphere of Titan); Titan (three-dimensional general circulation model of methane in the troposphere of Titan) circulation methane Titan troposphere},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

