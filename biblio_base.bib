@Article{Mousis02a,
  Title                    = {The D/H Ratio in methane in Titan: Origin and history},
  Author                   = {Mousis, Olivier and Gautier, Daniel and Coustenis, Athena},
  Journal                  = {Icarus},
  Year                     = {2002},
  Note                     = {CAN 138:6617 53-9 Mineralogical and Geological Chemistry LESIA, CNRS-FRE 2461, Observatoire de Paris,Meudon,Fr. Journal 0019-1035 written in English. 14476-19-8 (Methane hydrate) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in interplanetary ices that were incorporated into planetesimals that formed Titan's core and were subsequently decompd. and methane outgassed to Titan's atm.); 12385-13-6; 16873-17-9 Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (origin and history of D/H Ratio in methane in Titan atm.); 74-82-8 (Methane) Role: GPR (Geological or astronomical process), PRP (Properties), PROC (Process) (origin and history of D/H Ratio in methane in Titan atm.); 676-49-3 (Methane-d) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (origin of D/H Ratio in methane in Titan atm. from cooling solar nebula by isotopic thermal exchange of H with CH3D originating from interstellar methane D-enriched ices vaporized in nebula)},
  Number                   = {1},
  Pages                    = {156-165},
  Volume                   = {159},

  Abstract                 = {A new interpretation of the D/H ratio in CH4 obsd. in the atm. of Titan is proposed. Using a turbulent evolutionary model of the subnebula of Saturn (O. Mousis et al. 2002), the authors show that in contrast to the current scenario, the deuterium enrichment with respect to the solar value obsd. in Titan cannot have occurred in the subnebula. Instead, values of the D/H ratio measured in Titan were obtained in the cooling solar nebula by isotopic thermal exchange of hydrogen with CH3D originating from interstellar methane D-enriched ices that vaporized in the nebula. The rate of the isotopic exchange decreased with temp. and became fully inhibited around 200 K. Methane was subsequently trapped in cryst. ices around 10 AU in the form of clathrate hydrates formed at 60 K, and incorporated into planetesimals that formed the core of Titan. The nitrogen-methane atm. was subsequently outgassed from the decompn. of the hydrates (Mousis et al. 2002). By use of a turbulent evolutionary model of the solar nebula (O. Mousis et al. 2000), the entire story of D/H in CH4 was reconstructed, from its high value in the early solar nebula (acquired in the presolar cloud) down to the value measured in Titan's atm. today. Considering the two last detns. of the D/H ratio in Titan [D/H = (7.75+-2.25) * 10-5 obtained from ground-based observations (Orton 1992) and D/H = (8.75+3.25-2.25) * 10-5, obtained from IR Space Observatory observations (Coustenis et al. 2002, submitted for publication)] an inferred upper limit of the D/H ratio in methane in the early outer solar nebula is taken as about 3 * 10-4. The approach used is consistent with the scenario advocated by several authors in which the atm. methane of Titan is continuously replenished from a reservoir of clathrate hydrates of CH4 at high pressures, located in the interior of Titan. If this scenario is correct, observations of the satellite to be performed by the radar, the imaging system, and other remote sensing instruments aboard the spacecraft of the Cassini-Huygens mission from 2004 to 2008 should reveal local disruptions of the surface and other signatures of the predicted outgassing. [on SciFinder (R)]},
  Keywords                 = {Titan (atm.; origin and history of D/H Ratio in methane in Titan atm.); Solar nebula (origin of D/H Ratio in methane in Titan atm. from cooling solar nebula by isotopic thermal exchange of H with CH3D originating from interstellar methane D-enriched ices vaporized in nebula) deuterium hydrogen atomic ratio methane Titan satellite atm},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

