@Article{Thompson92,
  Title                    = {Vapor-liquid equilibrium thermodynamics of nitrogen + methane: model and Titan applications},
  Author                   = {Thompson, W. Reid and Zollweg, John A. and Gabis, David H.},
  Journal                  = {Icarus},
  Year                     = {1992},
  Note                     = {CAN 117:154844 53-9 Mineralogical and Geological Chemistry Lab. Planet. Stud.,Cornell Univ.,Ithaca,NY,USA. Journal 0019-1035 written in English. 7727-37-9 (Nitrogen) Role: PRP (Properties) (system, methane-, calcns. of vapor-liq. equil. thermodn. of, simulation of Titan's atm. in relation to); 74-82-8 (Methane) Role: PRP (Properties) (system, nitrogen-, calcns. of vapor-liq. equil. thermodn. of, simulation of Titan's atm. in relation to)},
  Number                   = {2},
  Pages                    = {187-99},
  Volume                   = {97},

  Abstract                 = {Calcns. of the vapor-liq. equil. thermodn. of the N2 + CH4 system show that the tropospheric clouds of Titan are not pure CH4, but solns. of CH4 contg. substantial quantities of N2. The conditions for satn., latent heat of condensation, and droplet compn. all depend on this equil. This study presents a thermodn. model for vapor-liq. equil. in the N2 + CH4 system which, by its structure, places strong constraints on the consistency of exptl. equil. data, and confidently embodies temp. effects by also including enthalpy (heat of mixing) data. Selected equil. and enthalpy data are used in a max. likelihood detn. of model parameters. The model can be readily evaluated to compute the satn. criteria, compn. of condensate, and latent heat in Titan's atm. for a given pressure-temp. (P-T) profile. For a nominal P-T profile, the partial pressure of CH4 required for formation of CH4 + N2 condensate is .apprx.20% lower than that required to sat. pure CH4, and .apprx.25% higher than that which would be computed by Raoult's law. N2 constitutes 16-30% of the cloud condensate, and higher altitude clouds are generally more N2-rich. The N2 content of condensate is .apprx.1/2 of that computed from Raoult's law and about 30% greater than that computed from Henry's law. Heats of condensation are .apprx.10% lower than for pure CH4. Above 14 km altitude, the liq. soln. becomes metastable with respect to a solid soln. contg. less N2: freezing of liq. droplets will be accompanied by the exsoln. of about 30% of the dissolved N2, probably leading to an underdense, porous texture. The refractive index, single-scattering albedo, and d. of CH4 + N2 cloud droplets of the appropriate compn. and phase should be used in modeling and spacecraft planning studies for Titan. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, compn. and phase distribution in atm. of, calcns. of vapor-liq. equil. thermodn. of nitrogen-methane system in relation to) nitrogen methane system equil thermodn Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

