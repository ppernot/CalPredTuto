@Article{Dimitrov04a,
  Title                    = {Kinetic pathways in the atmospheric chemistry of Titan - A generalized analysis},
  Author                   = {Dimitrov, Vasili and Bar-Nun, Akiva},
  Journal                  = {Progress in Reaction Kinetics and Mechanism},
  Year                     = {2004},
  Number                   = {1},
  Pages                    = {1-41},
  Volume                   = {29},

  Abstract                 = {Titan, the sixth Saturnine moon, is a unique celestial body in many respects, including the existence of a high-d. atm. over a relatively small astrophys. object, chem. activity in the low-potential reducing medium, the presence of an extensive aerosol domain, etc. Despite many observations, simulation expts. and theor. models, the general picture of Titan's atm. photochem. is still imprecise. This study of the most general features of chem. activity in Titan's atm. by means of Generalized Kinetic Anal. (GKA) is based on the point that both the probability and efficiency of kinetic trends are estd. solely on the basis of energy/material restrictions and general kinetic laws. Only the quantity (intensity) and quality (spectrum) of the external driving force are considered closely, while both the particular kinetic demands and low internal energy resources of Titan's background are discounted. What this means is that the main inferences of GKA should be valid for any given kinetic model. Only a small part Lch of the total external energy flux Labs.apprx.12.6 W m-2 is photochem. active Lch = (L1ion + L2ion + L1dis) + L2ch = (1.5 * 10-3 + 0.22 * 10-3 + 10.6 * 10-3) + 0.69 W m-2. The secondary energy L2ch (1440<l<3500.ANG.) meets the common energy requirements, while the primary energies L1ion, L2ion and L1dis define kinetic pathways of the chem. process, i.e. L1ion (790<l<980.ANG.) and L2ion (l<790.ANG.) initiate ionic photochem. via ionization of CH4 and N2, resp., while L1dis (980<l<1440.ANG.) provides photodissocn. of CH4 to neutral species. Because of severe energy/material restrictions, the general chem. process proceeds in the form of a self-sustaining Diels-Alder diene low-temp. synthesis to give telomerization and polymn. GKA proves that the main kinetic pathways (photodissocn. to neutrals and charged photoionization) play different roles with respect to the quant. and qual. formation of the final stable products of Titan's atm. photochem. The neutral pathway governs the bulk (overall yield) of the final products while ionic chem. is responsible for its wide chem. compn. (variety of chem. species). Species identification in terms of hydrocarbon type content results in the following wt. ratio compn. of the final products: dienes (0.60-0.65) + satd./unsatd. acyclic pure hydrocarbons (0.16-0.19) + tholins (0.07-0.08) + isocyclics (0.03-0.05) + misc. (0.05). The elemental compn. of this bulk material is (C/H/N) .apprx. 1.00/1.12/0.08. [on SciFinder (R)]},
  Keywords                 = {Polymerization (Diels-Alder; general features of chem. activity in Titan's atm. by generalized kinetic anal. and predicted thermodn. equil. compn. of final condensed products); Titan (atm.; general features of chem. activity in Titan's atm. by generalized kinetic anal. and predicted thermodn. equil. compn. of final condensed products); Diels-Alder reaction; Photoionization; Photolysis (general features of chem. activity in Titan's atm. by generalized kinetic anal. and predicted thermodn. equil. compn. of final condensed products); Alkadienes; Aromatic hydrocarbons; Nitriles; Polyacetylenes; Vinyl compounds Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (general features of chem. activity in Titan's atm. by generalized kinetic anal. and predicted thermodn. equil. compn. of final condensed products) Titan atm generalized kinetic model photodissocn photoionization},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

