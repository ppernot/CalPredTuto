@Article{McCord71,
  Title                    = {Saturn and its satellites: narrow-band spectrophotometry (0.3-1.1 m)},
  Author                   = {McCord, Thomas B. and Johnson, Torrence V. and Elias, Jonathan H.},
  Journal                  = {Astrophysical Journal},
  Year                     = {1971},
  Note                     = {CAN 75:12656 73 Spectra by Absorption, Emission, Reflection, or Magnetic Resonance, and Other Optical Properties Dep. Earth Planet. Sci.,Massachusetts Inst. Technol.,Cambridge,MA,USA. Journal 0004-637X written in English. 74-82-8; 1333-74-0; 7664-41-7; 7732-18-5 Role: PRP (Properties) (spectral bands of, in planet Saturn satellites)},
  Number                   = {2)(Pt. 1},
  Pages                    = {413-24},
  Volume                   = {165},

  Abstract                 = {Spectrophotometric observations at uv, visible, and near-ir wavelengths of 5 of Saturn's satellites, Tethys, Dione, Rhea, Titan, and Iapetus, and the equatorial band of Saturn were made during the 1968 and 1969 apparitions. Spectral resoln., achieved by using a series of narrow-band interference filters, ranged from 0.02 m in the uv to 0.05 m in the ir. The 60-in. (152 cm) and 100-in. (254 cm) telescopes on Mount Wilson were used. All satellites except Titan show evidence of synchronous rotation. Tethys, Dione, and Rhea all show some variation in brightness with orbital phase, their leading sides being 10-20% brighter than their trailing sides. No change in Titan's brightness with phase was obsd. The large change in Iapetus's brightness with orbital phase was confirmed. Iapetus shows significant variation in its spectrum with orbital phase, with its darker side \"redder\" than the brighter side by at least 15% between 0.4 and 0.8 m. All of the satellites except Titan have similar reflection spectra, relatively const. from 0.4 to 0.8 m and then decreasing from 0.8 to 1.1 m. Dione shows the largest decrease in the ir with a decrease of .apprx.15% from 0.8 to 1.0 m. The reflection spectra of these 4 satellites are consistent with what is known of H2O and NH3 frost spectra in the spectral range 0.4-1.1 m. The satellite spectra are not similar to spectra for most ferrosilicates; nor are they similar to spectra of the Moon, Mars, or the Galilean satellites. More surprisingly, the satellite spectra in this spectral region are quite different from the spectrum of Saturn's rings, where water ice has been identified in ir spectra. This difference implies the presence of some materials in the rings which are absent on the satellite surfaces. Titan's spectrum is also different from the spectra of the other satellites, but it is similar to the spectrum of Saturn's equatorial belt. This suggests that material present in the optical surface of Saturn's belt is similar to materials on Titan, perhaps on Titan's surface. CH4 absorptions affect both the belt spectrum and Titan's spectrum beyond 0.6 m. The Saturn and Titan spectra differ in the uv. Rayleigh scattering in gases above an uv haze layer (about 10 km-atm H2) can account for the uv turnup in Saturn's spectrum. There is much less Rayleigh scattering on Titan, which suggests a thin (10-1000 m-atm) atm., through which the surface may be visible. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn and its satellites, spectra of) Saturn satellites spectrophotometry; Tethys spectrophotometry; Dione spectrophotometry; Rhea spectrophotometry; Titan spectrophotometry; Iapetus spectrophotometry},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{McCullough2008a,
  Title                    = {Microsoft Excel's [`]Not The Wichmann-Hill' random number generators},
  Author                   = {McCullough, B.D.},
  Journal                  = {Computational Statistics \& Data Analysis},
  Year                     = {2008},

  Month                    = jun,
  Number                   = {10},
  Pages                    = {4587--4593},
  Volume                   = {52},

  Abstract                 = {Microsoft attempted to implement the Wichmann-Hill RNG in Excel 2003 and failed; it did not just produce numbers between zero and unity, it would also produce negative numbers. Microsoft issued a patch that allegedly fixed the problem so that the patched Excel 2003 and Excel 2007 now implement the Wichmann-Hill RNG, as least according to Microsoft. We show that whatever RNG it is that Microsoft has implemented in these versions of Excel, it is not the Wichmann-Hill RNG. Microsoft has now failed twice to implement the dozen lines of code that define the Wichmann-Hill RNG.},
  File                     = {:home/plessis/article/reference/McCullough2008a.pdf:},
  Owner                    = {plessis},
  Timestamp                = {2008.06.18},
  Url                      = {http://www.sciencedirect.com/science/article/B6V8V-4S21TGC-2/1/42bf1a71f933456465d5c161cdf162c5}
}

@Article{McCullough2008,
  Title                    = {On the accuracy of statistical procedures in Microsoft Excel 2007},
  Author                   = {McCullough, B.D. and Heiser, David A.},
  Journal                  = {Computational Statistics \& Data Analysis},
  Year                     = {2008},

  Month                    = jun,
  Number                   = {10},
  Pages                    = {4570--4578},
  Volume                   = {52},

  Abstract                 = {Excel 2007, like its predecessors, fails a standard set of intermediate-level accuracy tests in three areas: statistical distributions, random number generation, and estimation. Additional errors in specific Excel procedures are discussed. Microsoft's continuing inability to correctly fix errors is discussed. No statistical procedure in Excel should be used until Microsoft documents that the procedure is correct; it is not safe to assume that Microsoft Excel's statistical procedures give the correct answer. Persons who wish to conduct statistical analyses should use some other package.},
  File                     = {:home/plessis/article/reference/McCullough2008.pdf:},
