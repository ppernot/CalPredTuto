@Article{Smith93,
  Title                    = {Dissociative recombination of {H$_3^+$} and some other interstellar ions: a controversy resolved},
  Author                   = {David Smith and Patrik Spanel},
  Journal                  = {International Journal of Mass Spectrometry and Ion Processes},
  Year                     = {1993},
  Pages                    = {163 - 182},
  Volume                   = {129},

  Abstract                 = {Prompted by some recenst measurements of the recombination coefficient for H+3, i.e. [alpha](H+3), which indicate a value of [approximate]2×10-7 cm3 s-1 at 300 K for ground vibrational state ions, i.e. about a factor of ten greater than our previous FALP determination [less-than-or-equals, slant]2×10-8 cm3 s-1, we have made a further detailed study of this very important interstellar recombination reaction using our improved FALP apparatus. Thus we have shown that [alpha](H3+(v[greater-or-equal, slanted]3)) ions do indeed recombine quite efficiently, a mean recombination coefficient at 300 K being [approximate]1.3×10-7 cm3 s-1, but that vibrationally-relaxed ions recombine much more slowly, [alpha](H3+(v=1,0)) being [approximate]3×10-8 cm3 s-1 and [alpha](H3+ (v=0)) being somewhat smaller at (1-2)×10-8 cm3 s-1 at 300 K. Measurements at 210 and 140 K indicated a slow increase of [alpha](H3+) with decreasing temperature, the increase being less than a factor of 2 for both [alpha](H3+(v[greater-or-equal, slanted]3)) and [alpha](H3+(v=1,0)) between 300 and 140 K. Values of [alpha](D3+) were also obtained at 300 K only. The values obtained for both the rapidly-recombining and the slowly-recombining D3+ ions were somewhat smaller than the corresponding values for H3+. In addition to these studies, we have also re-measured the [alpha] values at both 300 and 100 K for HCO+, N2H+ and CH5+ which are also important interstellar ions. The values obtained are in reasonable agreement with our previous FALP measurements, but our [alpha](HCO+) and [alpha](N2H+) are lower by factors of 2 and 3 respectively than the recent measurements made in a pulsed afterglow experiment.},
  Doi                      = {DOI: 10.1016/0168-1176(93)87040-Y},
  ISSN                     = {0168-1176},
  Keywords                 = {Dissociative recombination},
  Owner                    = {plessis},
  Timestamp                = {2009.12.15},
  Url                      = {http://www.sciencedirect.com/science/article/B6TG6-44WCWCN-7D/2/b4631d4b797e4eef87e00f54594640e7}
}

