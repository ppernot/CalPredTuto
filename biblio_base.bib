@Article{Borucki85,
  Title                    = {Spectra of simulated lightning on Venus, Jupiter, and Titan},
  Author                   = {Borucki, W. J. and McKenzie, R. L. and McKay, C. P. and Duong, N. D. and Boac, D. S.},
  Journal                  = {Icarus},
  Year                     = {1985},
  Number                   = {2, pt. 1},
  Pages                    = {221-32},
  Volume                   = {64},

  Abstract                 = {Laser-induced plasmas in various gas mixts. were used to simulate lightning in other planetary atmospheres. This method of simulation has the advantage of producing short-duration, high-temp. plasmas free from electrode contamination. The laser-induced plasma discharges in air accurately simulate terrestrial lightning and can be expected to simulate lightning spectra in other planetary atmospheres. Spectra from 240 to 880 nm are presented for simulated lightning in the atmospheres of Venus, Earth, Jupiter, and Titan. The spectra of lightning on the other giant planets are expected to be similar to that ot Jupiter because the atmospheres of these planets are composed mainly of H and He. The spectra of Venus and Titan show substantial amts. of radiation due to the presence of C atoms and ions and show CN Violet radiation. Although small amts. of CH4 and NH3 are present in the Jovian atm., only emission from H and He was obsd. Most difference in the spectra can be understood in terms of the elemental ratios of the gas mixts. Consequently, observations of the spectra of lightning on other planets should provide in situ ests. of the atm. and aerosol compn. in the cloud layers in which lightning is occurring. In particular, the detection of inert gases such as He should be possible and the relative abundance of these gases compared to major constituents might be detd. [on SciFinder (R)]},
  Keywords                 = {Planets (spectra of simulated lightning on); Lightning (spectra of simulated, on planets); Plasma (laser-induced, in spectral studies of simulated lightning) planet atm spectra simulated lightning},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

