@Article{Griffith92,
  Title                    = {Observing the surface of Titan through near-infrared windows in its atmosphere},
  Author                   = {Griffith, Caitlin A. and Owen, Tobias},
  Journal                  = {European Space Agency, [Special Publication], SP},
  Year                     = {1992},
  Note                     = {CAN 118:172831 53-9 Mineralogical and Geological Chemistry Phys. Dep.,State Univ. New York,Stony Brook,NY,USA. Journal 0379-6566 written in English. 74-82-8 (Methane) Role: PRP (Properties) (near-IR windows of, in Titan's atm., surface in relation to)},
  Number                   = {Proc. - Symp. Titan, 1991},
  Pages                    = {199-204},
  Volume                   = {ESA SP-338},

  Abstract                 = {Ground-based observations suggest that the near-IR will be an important spectral region for exploring the compn. of Titan's surface with the Cassini mission. The principal source of opacity in Titan's atm., CH4, has transmission windows at 1.1, 1.3, 1.6, 2.0, 3.0 and 5.0 mm, which are transparent enough to allow visibility of the surface. Recent studies of the particle distribution in Titan's atm. suggest that haze and clouds also do not obscure the surface from view. A preliminary anal. of Titan's albedo at 1.3, 1.6 and 2.0 mm indicates that the wavelength dependence of the surface albedo is consistent with a surface composed mainly of dirty water ice. Prospects seem good for detailed observations of the surface by VIMS, DISR and possibly ISS. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, surface studies of, near-IR windows of methane in atm. in relation to); Infrared spectra (near-IR, methane windows of, in Titan's atm., surface in relation to) Titan surface near IR window atm},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

