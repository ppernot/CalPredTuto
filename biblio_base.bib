@Article{McEwan1998,
  Title                    = {Ion-molecule reactions relevant to Titan's ionosphere},
  Author                   = {McEwan, Murray J. and Scott, Graham B. I. and Anicich, Vincent G.},
  Journal                  = {International Journal of Mass Spectrometry and Ion Processes},
