@Article{Bird97a,
  Title                    = {Atmospheric attenuation of the Huygens S-band radio signal during the Titan descent},
  Author                   = {Bird, M. K.},
  Journal                  = {European Space Agency, [Special Publication] ESA SP},
  Year                     = {1997},
  Number                   = {Huygens: Science. Payload and Mission},
  Pages                    = {321-335},
  Volume                   = {ESA SP-1177},

  Abstract                 = {Attenuation of radio signals from absorption in Titan's atm. is computed for various radio ray path geometries applicable to the Huygens Probe Relay Link (PRL). The absence of strong radio absorbers such as ammonia or water in Titan's neutral atm. results in rather weak attenuation at the S-band wavelengths used for Huygens at l = 14.7 cm (f = 2040 MHz) and l = 14.3 cm (f = 2098 MHz). Attenuation in the neutral atm. is assumed to arise mainly (.simeq. 80%) from collision-induced absorption by mol. nitrogen. The remaining 20% is attributed to the same absorption process but via collisions of N2 with CH4. Defocusing losses due to atm. refraction, which is quite severe for radio occultation conditions, is negligible for the geometry applicable to the Huygens PRL. Tropospheric methane clouds are also not expected to have any measurable effect on the PRL. If the fractional ionization in the lower atm. is significantly enhanced by cosmic rays, absorption by free electron could become the dominant source of S-band attenuation. The predicted attenuation loss for the nominal baseline link at Probe touchdown (altitude z0 = 0 km; Probe Aspect Angle PAA = 45 Deg) is .simeq.-0.02 dB. If the cosmic ray source of ionization is detd. to be unimportant, the attenuation at this PAA will be more like -0.004 dB. Finally, applying these attenuation calcns. to the 15 GHz signal of the Huygens Radar Altimeter, it is found that the two-way (up and down ray path) vertical attenuation will not exceed -0.281 dB. [on SciFinder (R)]},
  Keywords                 = {Space vehicles (Huygens Probe; atm. attenuation of the Huygens S-band radio signal during the Titan descent); Radio wave (S-band; atm. attenuation of the Huygens S-band radio signal during the Titan descent); Titan; Troposphere (atm. attenuation of the Huygens S-band radio signal during the Titan descent) Huygens probe radio signal atm attenuation; Titan descent Huygens probe radio attenuation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Blagojevic03,
  Title                    = {Gas-phase syntheses for interstellar carboxylic and amino acids},
  Author                   = {Blagojevic, Voislav and Petrie, Simon and Bohme, Diethard K.},
  Journal                  = {Monthly Notices of the Royal Astronomical Society},
  Year                     = {2003},
  Pages                    = {L7--L11},
  Volume                   = {339},

  Abstract                 = {We report experimental results that demonstrate gas-phase, ionic syntheses of glycine and
β-alanine, as well as acetic and propanoic acid, from smaller molecules found in space; in
doing so, we infer the formation of these acids in the interstellar environment. We show that
ionized glycine and β-alanine are produced in the reactions of hydroxylamine ions, NH2OH+,
with acetic and propanoic acid respectively. Even more promising in the context of interstellar
synthesis are our results that demonstrate the corresponding production of the protonated
amino acids from analogous reactions with protonated hydroxylamine. The striking specificity
of these syntheses for the β-isomer of alanine suggests that the amino acids of CI (Carbonaceous
Ivona) chondrite meteorites are products of interstellar chemistry and supports the hypothesis
that these meteorites are of cometary origin.},
  File                     = {:home/plessis/article/reference/Blagojevic2003.pdf:},
