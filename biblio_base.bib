@Article{Kondrat'ev82,
  Title                    = {Optical properties of a Titan atmosphere and radiant heat transfer},
  Author                   = {Kondrat'ev, K. Ya and Moskalenko, N. I. and Parzhin, S. N.},
  Journal                  = {Doklady Akademii Nauk SSSR},
  Year                     = {1982},
  Note                     = {CAN 97:226688 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Gl. Geofiz. Obs.,Leningrad,USSR. Journal 0002-3264 written in Russian. 74-82-8; 74-84-0; 74-85-1; 74-86-2; 1981-80-2; 2229-07-4; 2465-56-7; 7783-07-5 Role: PRP (Properties) (absorption spectrum of, in Titan atm.)},
  Number                   = {2},
  Pages                    = {309-12 [Astron ]},
  Volume                   = {266},

  Abstract                 = {During remote probing of the Titan atm., a sharp increase in temp. with height is obsd. in the stratosphere which indicates the presence of components, absorbing short-wave solar radiation above the level of the tropopause (>30 km). This absorption is attributed to IR bands of CH4 and UV bands of CH2, CH3 which are photodissocn. products of other hydrocarbons. The Titan cloud consists, most probably, of a rather dispersed and vertically elongated haze, consisting of small fractions of solid CH4 particles. The 3.5- and 8-mm absorption bands of the haze are overlapped by absorption bands of the atm. gas components, and therefore have no significant effect on the radiative properties of the atm. Heating of the below-cloud layer of the Titan atm. is caused by absorption of solar radiation by numerous CH4 bands in the visible and near-IR region. The absorption spectra indicate the presence of CH4, C2H2, C2H4, C2H6, HCN, and C3H4. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, satellites, Saturn, Titan satellite, absorption spectra and radiant heat transfer in); Radiation (solar, absorption of, by compds. in Titan atm., radiative heat transfer during) Titan atm absorption spectra; methane Titan atm spectra; solar radiation Titan atm heating},
  Owner                    = {pernot},
