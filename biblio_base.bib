@Article{Kouvaris91,
  Title                    = {Phase equilibrium of methane and nitrogen at low temperatures: application to Titan},
  Author                   = {Kouvaris, Louis C. and Flasar, F. M.},
  Journal                  = {Icarus},
  Year                     = {1991},
  Note                     = {CAN 115:96122 53-9 Mineralogical and Geological Chemistry Appl. Res. Corp.,Landover,MD,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 7727-37-9 (Nitrogen) Role: OCCU (Occurrence) (in atm. of Titan, phase equil. of, at low temps.)},
  Number                   = {1},
  Pages                    = {112-24},
  Volume                   = {91},

  Abstract                 = {CH4 and N2 are partially miscible in the condensed phase. Although there are exptl. data on the vapor-liq. equil. of CH34 + N2 mixts. at the low temps. characterizing much of Titan's lower atm., in many cases only the temp., total vapor pressure, and compn. of the liq. phase have been measured. The compn. of the vapor phase is uniquely detd. as a consequence of the Gibbs phase rule, and the vapor compn. is computed for these data by integration of the Gibbs-Duhem equation. The thermodn. consistency of published measurements and calcns. of the vapor phase is examd. Using the profile of Titan's atm. temp. derived from Voyager radio occultation measurements, the satd. mole fraction of gaseous CH4 is computed as a function of altitude ? the 700-mbar level. It lies approx. halfway between that computed from Raoult's Law for a gas in equil. with an ideal soln. of liq. Ni and CH4 and that for a gas in equil. with pure liq. CH4. Above this level, the equil. condensed phase will freeze. If, however, a supercooled liq. phase is assumed the exptl. measurements can be extrapolated to a lower pressure and temp., and the vertical profile of gaseous CH4 can be extended to the cold trap near 344 mbar, where the satd. mole fraction of tropospheric CH4 is a min. If uniform mixing of CH4 above this level is assumed, the occultation measurements may be adjusted for the presence of this constituent in the stratosphere. At the cold trap the mole fraction of CH4 in equil. with supercooled droplets in a N + CH4 atmosphere is 0.017 +- 0.001. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, atm. of, methane and nitrogen in) planet Titan atm methane nitrogen},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

