@Article{Karkoschka92,
  Title                    = {Saturn's upper troposphere 1986-1989},
  Author                   = {Karkoschka, Erich and Tomasko, Martin G.},
  Journal                  = {Icarus},
  Year                     = {1992},
  Note                     = {CAN 117:154843 53-9 Mineralogical and Geological Chemistry Lunar Planet. Lab.,Univ. Arizona,Tucson,AZ,USA. Journal 0019-1035 written in English. 1333-74-0 (Hydrogen) Role: OCCU (Occurrence) (in upper troposphere, near-IR and visible absorption spectra in relation to, of Saturn); 74-82-8 (Methane); 7664-41-7 (Ammonia) Role: OCCU (Occurrence) (mixing ratio of, in upper troposphere, near-IR and visible absorption spectra in relation to, of Saturn)},
  Number                   = {2},
  Pages                    = {161-81},
  Volume                   = {97},

  Abstract                 = {Observations are made of Saturn's atm. in the visible and near-IR (460-940 nm) including 4 hydrogen quadrupole lines, 17 methane absorption bands ranging over 3 orders of magnitude in absorption strength, an ammonia absorption band, and the abs. calibrated continuum spectrum. All observations have complete coverage of Saturn's disk, in latitude as well as in center-to-limb position. A new method describing center-to-limb information is presented. The accuracy of the data is comparable to or better than that of previous data. This data set gives a quite complete description of Saturn's atm. in the visible and near IR at the spatial resoln. of ground-based observations. The main data were acquired in 1988, but small changes between 1986 and 1989 were detd. also. Weak absorption features of hydrogen, methane, and ammonia show a significant enhancement in the North Polar Region compared to the rest of the planet. An atm. model is given which fits all observations within estd. errors. It has clear gas at the top of the atm., an extended haze layer, and a reflective cloud at the bottom. Pressure levels and the haze optical depth were detd. as a function of latitude. The single-scattering albedo spectrum of the particles (most likely ammonia ice crystals) is also given for each latitude. The methane mixing ratio is (3.0 +- 0.6) * 10-3, the ammonia mixing ratio is (1.2 + 0.8/-0.6) * 10-3 below the ammonia condensation level. A cold temp. methane absorption spectrum is detd. under the assumption that methane band strengths are temp. invariant. It indicates that the absorption coeffs. in band centers are typically 20-30% stronger than at room temp. This spectrum should be useful in the interpretation of methane observations of all the giant planets and Titan. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, upper troposphere of, ammonia and hydrogen and methane in, near-IR and visible absorption spectra in relation to) Saturn troposphere near IR visible spectrum; cloud atm Saturn absorption spectrum; methane mixing ratio atm Saturn; ammonia hydrogen troposphere spectra Saturn},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

