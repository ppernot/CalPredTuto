@Article{Balucani01,
  Title                    = {Laboratory studies on the formation of CN-containing molecules in the atmosphere of Titan and Prebiotic Earth},
  Author                   = {Balucani, N. and Cartechini, L. and Bergeat, A. and Casavecchia, Piero and Volpi, G. G.},
  Journal                  = {European Space Agency, [Special Publication] SP},
  Year                     = {2001},
  Number                   = {Exo-/Astro-Biology},
  Pages                    = {159-162},
  Volume                   = {SP-496},

  Abstract                 = {The reactions of excited nitrogen atoms, N(2D), with methane, ethene and ethyne, have been studied under single collision conditions in order to identify possible reaction products. In all cases, the nitrogen/hydrogen exchange channel was found to be a significant reaction pathway, leading to nitrogen contg. mols. or radicals. The present lab. studies are helpful in modeling the chem. of the upper atm. of Titan, since they confirm that reactions of N(2D) are effective in generating new C-N bonds and therefore represent potential first steps towards the formation of nitriles. Relevant to the org. chem. of Titan atm. are the following findings: (i) CH2NH is the main reaction product from the reaction N(2D)+CH4; (ii) the products of formula C2H3N generated by the N(2D)+C2H4 reaction are formed with enough internal energy to easily rearrange to the most stable isomer CH3CN; (iii) the main product from N(2D)+C2H2 reaction is HCCN. We recall that both CH2NH and CH3CN have been obsd. in the atm. of Titan, while the HCCN radical has been suggested to be a key intermediate towards C2N2/C2N4 formation. [on SciFinder (R)]},
  Keywords                 = {Titan (atm.; lab. studies on formation of cyanide-contg. mols. in atm. of Titan and Prebiotic Earth); Titan (lab. studies on formation of cyanide-contg. mols. in atm. of Titan and Prebiotic Earth); Nitriles Role: FMU (Formation, unclassified), FORM (Formation, nonpreparative) (lab. studies on formation of cyanide-contg. mols. in atm. of Titan and Prebiotic Earth); Earth (prebiotic; lab. studies on formation of cyanide-contg. mols. in atm. of Titan and Prebiotic Earth) cyanide formation atm Titan prebiotic Earth},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

