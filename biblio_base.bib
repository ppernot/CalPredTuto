@Article{Kasprzak96,
  Title                    = {Cassini orbiter ion and neutral mass spectrometer instrument},
  Author                   = {Kasprzak, Wayne and Niemann, H. and Harpold, D. and Richards, J. and Manning, H. and Patrick, E. and Mahaffy, P.},
  Journal                  = {Proceedings of SPIE-The International Society for Optical Engineering},
  Year                     = {1996},
  Note                     = {CAN 126:21145 53-9 Mineralogical and Geological Chemistry NASA/Goddard Spaceflight Center,Greenbelt,MD,USA. Journal 0277-786X written in English. 74-82-8 (Methane); 74-90-8 (Hydrocyanic acid); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (the Cassini orbiter ion and neutral mass spectrometer instrument for studying the atm. of Titan)},
  Number                   = {Cassini/Huygens: A Mission to the Saturnian Systems},
  Pages                    = {129-140},
  Volume                   = {2803},

  Abstract                 = {The Cassini Orbiter Ion and Neutral Mass Spectrometer (INMS) is designed to measure the compn. and d. variations of (low energy) ions and neutral species in the upper atm. of Titan, in the vicinity of the icy satellites and in the inner magnetosphere of Saturn where densities are sufficiently high for measurement. The sensor utilizes a dual radio frequency quadrupole mass analyzer with a mass range of 1-99 amu, two electron multipliers operated in pulse-counting mode to cover the dynamic range required and two sep. ion sources. A closed ion source measures non-surface reactive neutral species which have thermally accommodated to the inlet walls such as N2 and CH4. An open ion source allows direct beaming ions or chem. active neutral species such as N and HCN to be measured without surface interaction. The instrument can alternate between these three different modes (closed neutral, open ion and open neutral). Characterization and calibration of each of these three modes is done using a low energy ion beam, a neutral mol. beam and a neutral thermal gas source. An onboard flight computer is used to control instrument operating parameters in accordance with pre-programmed sequences and to package the telemetry data. The sensor is sealed and maintained in a vacuum prior to launch to provide a clean environment for measurement of neutral species when it is opened to the ambient atm. after orbit insertion. The instrument is provided by NASA/Goddard Spaceflight Center, Code 915. Operation of the instrument and data anal. will be carried out by a Science Team. [on SciFinder (R)]},
  Keywords                 = {Planets (outer; the Cassini orbiter ion and neutral mass spectrometer instrument for studying the atm. of Titan); Magnetosphere; Saturn; Titan (the Cassini orbiter ion and neutral mass spectrometer instrument for studying the atm. of Titan) planet Saturn Titan atm spectrometry Cassini},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

