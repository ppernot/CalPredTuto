@Article{Smith99b,
  Title                    = {A combined experimental and theoretical study of the catalytic dissociation of methane by the photolysis of acetylene at 185 nm},
  Author                   = {Smith, N. S. and Gazeau, M. C. and Khelifi, A. and Raulin, F.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1999},
  Note                     = {CAN 131:21445 53-9 Mineralogical and Geological Chemistry Laboratoire Interuniversitaire des Systemes Atmospheriques (LISA),Creteil,Fr. Journal 0032-0633 written in English. 74-86-2 (Ethyne) Role: CAT (Catalyst use), GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PROC (Process), USES (Uses) (Titan's stratosphere; a combined exptl. and theor. study of the catalytic dissocn. of methane by the photolysis of acetylene at 185 nm); 74-82-8 (Methane) Role: GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PROC (Process) (Titan's stratosphere; a combined exptl. and theor. study of the catalytic dissocn. of methane by the photolysis of acetylene at 185 nm)},
  Number                   = {1/2},
  Pages                    = {3-10},
  Volume                   = {47},

  Abstract                 = {Exptl. irradiations of CH4-C2H2 (1000: 1) gas mixts., at 185 nm, have been carried out in order to test the catalytic scheme of dissocn. of methane, via the photolysis of acetylene, that is supposed to occur in Titan's stratosphere. The theor. evolution of such a system has been investigated using three chem. models, and the predictions compared with our exptl. data. Our expts. confirm that dissocn. of methane can be catalyzed by the photolysis of acetylene at 185 nm. The quant. comparison with theor. results reveals an exptl. effect, which we explain as a heterogeneous process occurring on the walls of the reactor. We propose a mechanism for this wall effect that is in qual. agreement with our observations. We are currently developing an approach that will allow quantification of this effect, with the aim of extrapolating lab. results to an open system. We will therefore be able to perform a crit. comparison with existing gas-phase schemes to improve the chem. modeling of Titan's atm. [on SciFinder (R)]},
  Keywords                 = {Titan (stratosphere process; combined exptl. and theor. study of the catalytic dissocn. of methane by the photolysis of acetylene at 185 nm) catalytic dissocn methane photolysis acetylene Titan atm exptl simulation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Smith1999,
  Title                    = {Modeling of methane photolysis in the reducing atmospheres of the outer solar system},
  Author                   = {Smith, Nicholas S. and Raulin, François},
  Journal                  = {J. Geophys. Res.},
  Year                     = {1999},
  Number                   = {E1},
  Pages                    = {1873--1876},
  Volume                   = {104},

  Abstract                 = {It is important that methane photolysis is accurately treated in models of planetary atmospheres of the outer solar system and of the interstellar medium. Recent measurements of the H atom and H2 yields in the photolysis of methane have been considered and appear to be consistent with quantum yields, at Lyman &#945; (121.6 nm), of (R1) CH3 + H = 0.41, (R2) 1CH2 + H2 = 0.53, (R3) 1CH2 + H + H = 0.0, (R4) 3CH2 + H + H = 0.0, and (R5) CH + H2 + H = 0.06. At shorter wavelengths it is likely that the production of CH will increase due to the decomposition of excited CH3 molecules, although further data are required to be able to accurately model the wavelength dependence of methane photolysis. At the low temperatures of Titan's atmosphere, for example, the possible temperature dependence of CH4 absorption cross sections may need to be considered. By reference to other hydrocarbons, the absorption of methane may increase by 30% or more at 200 K with respect to room temperature values. These dependences, not previously considered in photochemical models of Titan, could significantly change the predicted composition of Titan's atmosphere and the chemical pathways implied.},
  ISSN                     = {0148-0227},
  Owner                    = {pernot},
  Publisher                = {AGU},
  Timestamp                = {2012.05.02},
  Url                      = {http://dx.doi.org/10.1029/1998JE900027}
}

