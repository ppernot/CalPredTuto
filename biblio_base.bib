@Article{Hartung04,
  Title                    = {A new VLT surface map of Titan at 1.575 microns},
  Author                   = {Hartung, M. and Herbst, T. M. and Close, L. M. and Lenzen, R. and Brandner, W. and Marco, O. and Lidman, C.},
  Journal                  = {Astronomy and Astrophysics},
  Year                     = {2004},
  Note                     = {CAN 141:353013 53-9 Mineralogical and Geological Chemistry European Southern Observatory,Santiago,Chile. Journal 0004-6361 written in English.},
  Number                   = {1},
  Pages                    = {L17-L20},
  Volume                   = {421},

  Abstract                 = {We present a first high contrast 1.575 m surface map of Titan, that is haze cor. by simultaneously imaging the stratospheric layer. At visible and most near-IR wavelengths, the methane-rich atm. completely obscures the surface; only in a few narrow wavelength windows does the atm. become optically thin. One of the most convenient windows lies at 1.58 mm, adjacent to the methane absorption feature at 1.62 mm. Our data span 7 consecutive nights, resulting in phase coverage of 275 Deg in longitude. The images were taken with NAOS-CONICA adaptive optics system at the VLT, using the recently commissioned Simultaneous Differential Imager mode (SDI). The combination of adaptive optics and simultaneous imaging through 3 filters sampling the methane absorption at 1.6 m reveals extraordinary details of Titan's surface. Providing views of Titan's surface at high resoln. (60 mas) is of particular topical importance, since the Cassini-Huygens mission is currently approaching the Saturn system and the Huygens probe will enter Titan's atm. in early 2005. [on SciFinder (R)]},
  Keywords                 = {IR absorption; Titan (high contrast 1.575 m surface map of Titan) Titan satellite surface map adaptive optics imaging},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

