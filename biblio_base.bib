@Article{Lecluse96,
  Title                    = {Deuterium enrichment in giant planets},
  Author                   = {Lecluse, C. and Robert, F. and Gautier, D. and Guiraud, M.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1996},
  Note                     = {CAN 126:174471 53-9 Mineralogical and Geological Chemistry Laboratoire de Mineralogie (URA CNRS no 736), Museum National d'Histoire Naturelle,Paris,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 1333-74-0 (Hydrogen) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (deuterium enrichment in giant planets); 7732-18-5 (Water) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (ice; deuterium enrichment in giant planets)},
  Number                   = {12},
  Pages                    = {1579-1592},
  Volume                   = {44},

  Abstract                 = {Recently published lab. measurements of the isotopic exchange rate const. k-(T) between CD4 and H2 are used to calc. the f(z) value (the isotopic enrichment factor between CH4 and H2) at every level in the outer atm. of the giant planets. The variation of f(z) with local vertical velocity, temp. and pressure has been calcd. under the assumption that atmospheres are convective and uncertainties have been calcd. by error propagation. Considering only the random errors, mainly the uncertainty on k-(T), the f values in the observable upper atmospheres of giant planets (i.e at z = 0, P = 1 bar) are f(0) = 1.25 +- 0.05, 1.38 +- 0.06, 1.68 +- 0.09, and 1.61 +- 0.08 for Jupiter, Saturn, Uranus, and Neptune, resp. Addnl. systematic errors due to the uncertainty in calcg. the vertical velocity in the framework of the mixing length Prandtl theory lead to an overall uncertainty on f(0) +- 0.12, +- 0.15, +- 0.23, and +- 0.21 for each planet, resp. The D/H ratios in H2 derived from the measured CH3D/CH4 ratios in the upper atm. of the four giant planets are then recalcd. Uranus and Neptune seem to be enriched in deuterium with respect to the protosolar nebula but depleted relative to the Std. Mean Oceanic Water on the Earth (SMOW). However calcns. based on current interior models of Neptune suggest that ices which formed the core of the planet had a D/H ratio of the order of the SMOW. The deuterium abundance in proto-Uranian ices remains uncertain. The case where water is a major constituent of the fluid envelope of Neptune is discussed. It is shown that the D/H ratio of the planet would then be higher than the value measured in hydrogen. Even in this case, the D/H ratio in proto-Neptunian ices is less than the recently revised value in P/Halley and less than the value measured in water of the Semarkona meteorite. These results suggest that the ices which formed the core of Neptune did not have an interstellar origin. Similarly, the comparison of the most recent detn. of the D/H ratio in the atm. of Titan with the value of D/H in P/Halley suggests that this atm. was not formed by infalling comets but more likely from grains embedded in the sub-nebula of Saturn. [on SciFinder (R)]},
  Keywords                 = {Earth; Halley's comet; Ice; Jupiter; Meteorites; Neptune; Saturn; Titan; Uranus (deuterium enrichment in giant planets); Planets (giant; deuterium enrichment in giant planets) planet giant deuterium isotope enrichment factor},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Lecointre09,
  Title                    = {Absolute cross-sections and kinetic-energy-release distributions for electron-impact ionization and dissociation of {CD}},
  Author                   = {Lecointre, J. and Belic, D. S. and Jureta, J. J. and Janev, R. K. and Defrance, P.},
