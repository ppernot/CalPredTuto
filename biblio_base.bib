@Article{Gladstone93,
  Title                    = {A search for argon in the atmosphere of Titan},
  Author                   = {Gladstone, Randy and Bowyer, Stuart and Owen, Tobias and Hurwitz, Mark},
  Journal                  = {UV X-Ray Spectrosc. Astrophys. Lab. Plasmas, Proc. Int. Colloq., 10th},
  Year                     = {1993},
  Note                     = {CAN 122:14057 53-9 Mineralogical and Geological Chemistry Space Sciences Laboratory,University of California,Berkeley,CA,USA. Conference written in English. 7440-37-1 (Argon) Role: ANT (Analyte), GOC (Geological or astronomical occurrence), ANST (Analytical study), OCCU (Occurrence) (in Titan atm.; model simulation of detn. of argon in Titan atm. using extreme UV emission features in Titan spectrum)},
  Pages                    = {293-6},

  Abstract                 = {The origin of Titan's thick nitrogen and methane atm. is an enigma. Detection of argon would indicate that the N2 and CO now found in the atm. probably came in with ice during Titan's accretion. If there is very little argon, then we have to turn to models starting with frozen ammonia, methane and water ice, indicating a more important role for the Saturn sub-nebula, and requiring subsequent modification by photochem. Current ests. on the fraction of argon in Titan's atm. are based on indirect evidence and range from 0 to 25%. We have modeled Titan's atm. with argon as an added constituent. Using a photoelectron transport code, we detd. the photoelectron flux in a std. model Titan atm. with energy degrdn. and transport of photoelectrons due solely to N2. The calcd. photoelectron flux was then used to excite the N2, Ar, and CH4 in the upper atm. of Titan using appropriate electron impact EUV emission cross sections. Photodissociative excitation of N2, resulting in emissions by N and N+ fragments, were also included. The resulting emission profiles were used to simulate the expected brightnesses of several features in the EUV dayglow spectrum of Titan. In particular, the bright resonance line of argon at 1048 .ANG. is predicted to be present at a level of 5-10 R, depending on the assumed abundance of argon in the lower atm. of Titan. We then consider observability of the argon dayglow emission by the Berkeley spectrometer to be flown as part of the German/US Shuttle-borne ORFEUS mission. This spectrometer employs four novel spherically figured varied line-space gratings which will provide high resoln. (l/Dl > 6000) spectra from 390 to 1200 .ANG.. The calcd. integration time required for detection of argon in the atm. of Titan is about 20 h, which is, unfortunately, too long to be attempted during the 5-day ORFEUS mission. An accurate detn. of the abundance of argon on Titan will likely have to wait for the Cassini orbiter and Huygens probe to arrive at the Saturn system. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, atm.; model simulation of detn. of argon in Titan atm. using extreme UV emission features in Titan spectrum) Titan atm argon extreme UV spectrometry},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Glandorf00,
  Title                    = {Nucleation properties of aerosols in the atmospheres of Mars and Titan},
  Author                   = {Glandorf, David L. and Curtis, Daniel B. and Colaprete, Tony and Toon, Owen B. and Tolbert, Margaret A.},
  Journal                  = {AIP Conference Proceedings},
  Year                     = {2000},
  Note                     = {CAN 133:298985 53-9 Mineralogical and Geological Chemistry Department of Chemistry and Biochemistry and CIRES,University of Colorado,Boulder,USA. Journal 0094-243X written in English. 124-38-9 (Carbon dioxide) Role: GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PRP (Properties), PROC (Process) (on Mars; exptl. study of nucleation properties of aerosols in the atmospheres of Mars and Titan); 74-82-8 (Methane); 74-84-0 (Ethane) Role: GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PRP (Properties), PROC (Process) (on Titan; exptl. study of nucleation properties of aerosols in the atmospheres of Mars and Titan)},
  Number                   = {Nucleation and Atmospheric Aerosols 2000},
  Pages                    = {661-664},
  Volume                   = {534},

  Abstract                 = {Mars, with a rich CO2 atmosphere and Titan, with an abundance of hydrocarbons, have both been proposed as models for the atm. of early Earth. In the current atmospheres of Mars and Titan, CO2 and hydrocarbons, resp., may condense to form clouds. In this paper, we explore cloud formation processes in these two very different planetary atmospheres. Under early Martian conditions, IR scattering by CO2 clouds could have warmed the planet's surface above freezing. However, the radiative effect of the clouds depends strongly on the nucleation and growth kinetics of the cloud particles. We exptl. examine the nucleation and growth of CO2 on water ice under Martian conditions. We find that a crit. satn. of S = 1.3 is required for nucleation, corresponding to a contact parameter of m = 0.95. After nucleation, growth of CO2 proceeds rapidly without a surface kinetic barrier. Using a microphys. cloud model, our data suggest that CO2 clouds are best described as \"snow\", having a small no. of very large particles. Titan's atm. may have clouds of ethane and methane and may even support a cycle analogous to the hydrol. cycle on Earth. Alternatively, Titan's atm. may be highly supersatd. in orgs. if no suitable particles are available for nucleation. Titan's org. haze particles, dubbed tholins, may provide a suitable nucleation surface. We have prepd. a lab. sample of tholins and will be performing nucleation expts. of ethane on the tholins. [on SciFinder (R)]},
  Keywords                 = {Clouds; Mars; Nucleation (exptl. study of nucleation properties of aerosols in the atmospheres of Mars and Titan); Atmospheric aerosols (on Mars and Titan; exptl. study of nucleation properties of aerosols in the atmospheres of Mars and Titan); Hydrocarbons Role: GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PRP (Properties), PROC (Process) (on Titan; exptl. study of nucleation properties of aerosols in the atmospheres of Mars and Titan) carbon dioxide hydrocarbon cloud nucleation aerosol atm Mars Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Glosik01,
  Title                    = {Experimental study of recombination of {H$_3^+$} ions with electrons relevant for interstellar and planetary plasmas},
  Author                   = {J. Glos\'ik and R. Pla\v{s}il and V. Poterya and P. Kudrna and M. Tich\'y and A. Pysanenko},
  Journal                  = {Journal of Physics B: Atomic, Molecular and Optical Physics},
  Year                     = {2001},
  Number                   = {15},
  Pages                    = {L485-L494},
  Volume                   = {34},

  Abstract                 = {The H3+ is a key ion in the interstellar chemistry. The rate of recombination of H3+ with electrons, a, plays a pivotal role in the level of ionization of interstellar plasmas. Over the past 50 years a number of studies of this recombination have been carried out, yet they did not lead to an unambiguous value of a. We report observation of the rate of de-ionization of H3+ as low as aeff \~{} 3x10-9 cm3 s-1 at hydrogen density decreasing down to 5x1010 cm-3. This implies that the dissociative recombination of H3+ under interstellar conditions is very slow with aDR$<$$<$10-8 cm3 s-1. This value is in agreement with theoretical predictions and astronomical observations, and provides an explanation of the `enigma of H3+'.},
  File                     = {:home/plessis/article/reference/Glosik2001.pdf:},
  Owner                    = {plessis},
  Timestamp                = {2009.12.01},
  Url                      = {http://stacks.iop.org/0953-4075/34/L485}
}

@Article{Glosik05,
