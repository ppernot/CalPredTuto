@Article{Barbieri01,
  Title                    = {Phanerozoic submarine cold vent biota and its exobiological potential},
  Author                   = {Barbieri, Roberto and Ori, Gian Gabriele and Taviani, Marco},
  Journal                  = {European Space Agency, [Special Publication] SP},
  Year                     = {2001},
  Number                   = {Exo-/Astro-Biology},
  Pages                    = {295-298},
  Volume                   = {SP-496},

  Abstract                 = {A brief review. Modern and fossil chemosynthetic communities based on the consumption of gases (carbon dioxide, methane, hydrogen sulfide, and others) are widespread on the Earth surface, where they have produced geol. bodies distributed over a wide time slice. These communities are entirely sustained by gas-bearing cold fluids, coming to the surface from the upper crust, around which they grow. This kind of outgassing is not linked to volcanic activity and there is not hydrothermal influence. Cold outgassing is probably present on Mars, it has been obsd. on Ganymede, Europa, and other icy satellites (ice volcanism), and is theor. considered to be present on Titan. This possible extensive occurrence of outgassing sites on the planetary surface suggests that chemosynthesis may be an important player in the biol. of the Solar System. [on SciFinder (R)]},
  Keywords                 = {Life (extraterrestrial; phanerozoic submarine cold vent biota and exobiol. potential); Biota (marine, cold vent assocd., chemosynthetic; phanerozoic submarine cold vent biota and exobiol. potential); Earth (phanerozoic submarine cold vent biota and exobiol. potential); Landforms (submarine cold vents; phanerozoic submarine cold vent biota and exobiol. potential) review phanerozoic cold vent biota exobiol extraterrestrial life},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

