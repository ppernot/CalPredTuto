@Article{Margulis77,
  Title                    = {Limitations to growth of microorganisms on Uranus, Neptune, and Titan},
  Author                   = {Margulis, Lynn and Halvorson, Harlyn O. and Lewis, John and Cameron, A. G. W.},
  Journal                  = {Icarus},
  Year                     = {1977},
  Note                     = {CAN 86:184882 6-13 General Biochemistry Dep. Biol.,Boston Univ.,Boston,MA,USA. Journal 0019-1035 written in English.},
  Number                   = {4},
  Pages                    = {793-808},
  Volume                   = {30},

  Abstract                 = {A wide range of physical, chem., and thermal models of the atm. of Uranus was examd. Even in the most favorable model of S.J. Weidenschilling and J.S. Lewis (1973), the probability of growth of a contaminant terrestrial microorganisms is nil. If, as is likely, conditions are even more extreme on Neptune, the probability of contamination of both of the outer planets Uranus and Neptune is nil. The probability of growth was estd. with respect to the following factors: the presence of stable liq. water, water layer, org. mol. prodn. by solar UV light, NH3 concn. at the water cloud level, ionic species distribution, and concns. at the water cloud level. The evaluation of these factors suggests that most terrestrial life as we known it would be excluded on the basis of any one of them. No known organism would be adapted to all the stringent Uranus conditions simultaneously. Titan, the methane-rich moon of Saturn, may be more hospitable for terrestrial organisms than any of the other objects of the outer solar system. Even there there are formidable barriers to the growth of an Earth organism i titan's atm. [on SciFinder (R)]},
  Keywords                 = {Microorganism (in outer planet atm., probability of growth of); Planets (Neptune, atm. models of, microorganism growth in); Planets (Saturn, Titan satellite, atm. models of, microorganism growth in); Planets (Uranus, atm. models of, microorganism growth in) Uranus atm microorganism growth; Neptune atm microorganism growth; Titan atm microorganism growth},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

