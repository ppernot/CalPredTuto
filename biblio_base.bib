@Article{Kobayashi01,
  Title                    = {Formation of bioorganic compounds in simulated planetary atmospheres by high energy particles or photons},
  Author                   = {Kobayashi, Kensei and Masuda, Hitomi and Ushio, Ken-Ichiro and Ohashi, Akihiro and Yamanashi, Hiroto and Kaneko, Takeo and Takahashi, Jun-Ichi and Hosokawa, Teruo and Hashimoto, Hirofumi and Saito, Takeshi},
  Journal                  = {Advances in Space Research},
  Year                     = {2001},
  Note                     = {CAN 135:268784 6-0 General Biochemistry Department of Chemistry and Biotechnology,Yokohama National University,Yokohama,Japan. Journal; General Review 0273-1177 written in English.},
  Number                   = {2},
  Pages                    = {207-215},
  Volume                   = {27},

  Abstract                 = {A review and discussion, with 29 refs. Various types of org. compds. have been detected in Jupiter, Titan, and cometary coma. It is probable that org. compds. were formed in primitive Earth and Mars atmospheres. Cosmic rays and solar UV are believed to be two major energy sources for org. formation in space. The authors examd. energetics of org. formation in simulated planetary atmospheres. Gas mixts. including a C-source (carbon monoxide or methane) and a N-source (nitrogen or ammonia) were irradiated with the following: High energy protons or electrons from accelerators, gamma-rays from 60Co, UV light from a deuterium lamp, and soft X-rays or UV light from an electron synchrotron. Amino acids were detected in the products of particles, gamma-rays and soft X-rays irradn. from each gas mixt. examd. UV light gave, however, no amino acid precursors in the gas mixt. of carbon monoxide, nitrogen and nitrogen. It gave only a trace of them in the gas mixt. of carbon monoxide, ammonia and water or that of methane, nitrogen and water. Yield of amino acid precursors by photons greatly depended on their wavelength. These results suggest that nitrogen-contg. org. compds. like amino acid precursors were formed chiefly with high energy particles, not UV photons, in Titan or primitive Earth/Mars atmospheres where ammonia is not available as a predominant N-source. [on SciFinder (R)]},
  Keywords                 = {Planets (high energy particles in formation of amino acids and other org. compds. in gas mixt. models planetary atmospheres); Amino acids; Organic compounds Role: FMU (Formation, unclassified), GFM (Geological or astronomical formation), FORM (Formation, nonpreparative) (high energy particles in formation of amino acids and other org. compds. in gas mixt. models planetary atmospheres) review planet atm org compd formation high energy particle},
  Owner                    = {pernot},
