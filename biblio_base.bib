@Article{Vacher00,
  Title                    = {Clustering reactions of HCNH+, HCNH+(N2) and HCNH (CH4) with ethane: application to Titan atmosphere},
  Author                   = {Vacher, J. R. and Le Duc, E. and Fitaire, M.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2000},
  Note                     = {CAN 132:281940 53-9 Mineralogical and Geological Chemistry Laboratoire de Physique des Gaz et des Plasmas, (associated to C. N. R. S.), Universite Paris XI,Orsay,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 74-84-0 (Ethane); 7727-37-9 (Nitrogen); 21107-92-6 (Methylium, imino-) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (clustering reactions of HCNH+, HCNH+(N2) and HCNH (CH4) with ethane and their application to Titan atm.)},
  Number                   = {2/3},
  Pages                    = {237-247},
  Volume                   = {48},

  Abstract                 = {Thermochem. data for several ion-mol. clustering reactions of HCNH+, HCNH+(N2), and HCNH+(CH4) with C2H6 were detd. by high-pressure mass spectrometry in order to better understand the role of ethane in the chem. of Titan's atm. Clustering equil. were obtained in a gas mixt. (pure N2 with a small amt. (10-5 of the total pressure) of C2H6) irradiated by alpha particles in the temp. range 150-320 K. The enthalpy and entropy changes for the reactions HCNH+(C2H6)n + C2H6 + N2 .tautm. HCNH+(C2H6)n+1 + N2 (n = 0, 1), HCNH+(C2H6)(N2)n + N2 + N2 .tautm. HCNH+(C2H6)(N2)n+1 + N2 (n = 0-2), HCNH+(CH4) + C2H6 + N2 .tautm. HCNH+(CH4)(C2H6) + N2 and HCNH+(N2) + C2H6 + N2 .tautm. HCNH+(N2)(C2H6) + N2 have been measured. For the HCNH+(C2H6)n series, the abs. enthalpy change for the trimer formation (4.5 kcal mol-1) is found to be much smaller than the relatively large enthalpy for the dimer formation (11.6 kcal mol-1). For the HCNH+(N2)n(C2H6) series, the enthalpy change appears to decrease monotonically from n = 1 to 3, showing no discontinuity in the attachment energy of the ligand mol. The measured low enthalpy changes suggest that the nature of the bonding remains mainly electrostatic for these cluster's formations as it is for the formation of HCNH+(CH4)(C2H6). The free energy changes DG Deg for the substitution of N2 or CH4 by C2H6 in HCNH+(N2)n (n = 1, 2), HCNH+(CH4) and HCNH+(N2)(C2H6) are neg. for T = 90 K and T = 165 K. The ligand-exchange reaction leading to HCNH+(C2H6) and HCNH+(C2H6)2 is possible in Titan's lower atm. (100-150 km). These processes could lead to the formation of heavier cyanides and eventually large aerosols. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; clustering reactions of HCNH+, HCNH+(N2) and HCNH (CH4) with ethane and their application to Titan atm.); Atmospheric aerosols; Entropy; Free energy (clustering reactions of HCNH+, HCNH+(N2) and HCNH (CH4) with ethane and their application to Titan atm.) planet Saturn Titan atm clustering reaction ethane},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Vacher99,
  Title                    = {Titan's atmosphere: possible clustering reactions of HCNH+, HCNH+(N2) and HCNH+(CH4) ions with acetylene},
  Author                   = {Vacher, J. R. and Le Duc, E. and Fitaire, M.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1999},
  Note                     = {CAN 131:34001 53-9 Mineralogical and Geological Chemistry Laboratoire de Physique des Gaz et des Plasmas,Universite Paris,Orsay,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 74-86-2 (Ethyne); 7727-37-9 (Nitrogen); 21107-92-6 Role: GOC (Geological or astronomical occurrence), RCT (Reactant), OCCU (Occurrence), RACT (Reactant or reagent) (thermodn. parameters for possible clustering reactions of HCNH+, HCNH+(N2) and HCNH+(CH4) ions with acetylene simulating reactions in Titan atm.)},
  Number                   = {1/2},
  Pages                    = {151-162},
  Volume                   = {47},

  Abstract                 = {The thermochem. of several ion-mol. clustering reactions of HCNH+, HCNH+(N2) and HCNH+(CH4) ions with C2H2 was detd. from chem. equil. measurements. In order to better understand the chem. of Titan, a gas mixt. contg. 97% N2, 3% CH4 and a small amt. of 0.005% C2H2 was irradiated by a-particles in the temp. range 140-320K and analyzed by high-pressure mass spectrometry. The enthalpy and entropy changes for the reactions HCNH+(C2H2)n+C2H2+N2 .rdblhar. HCNH+(C2H2)n+1+N2 (n=0-2), HCNH+(C2H2)n+N2+N2 .rdblhar. HCNH+(C2H2)n(N2)+N2 (n=1, 2), HCNH+(C2H2)n+CH4+N2 .rdblhar. HCNH+(C2H2)n(CH4)+N2 (n=1, 2), HCNH+(C2H2)n(N2)+C2H2+N2 .rdblhar. HCNH+(C2H2)n+1(N2)+N2 (n=0, 1) have been detd. For the HCNH+(C2H2)n series, the abs. enthalpy change decreases monotonically from 12.1 to 2.8 kcal mol-1 for n=1 to 3, showing no noticeable effect concerning the filling of shells. The nature of the bonding remains mainly electrostatic. For the HCNH+(C2H2)n(X) series with X=N2 or CH4, the free energy changes DG Deg were calcd. for T=90 K and 165 K. For both temps., DG Deg~0 for replacement of N2 by CH4 for n=0 to 2 whereas DG Deg for the substitution of N2 or CH4 by C2H2 are neg. for n=0, 1. The ligand-exchange reaction leading to the formation of HCNH+(C2H2) and HCNH+(C2H2)2 could exist in the lower Titan's atm. Paths involving HCNH+(C2H2)(N2) or HCNH+(C2H2)(CH4) and yielding to HCNH+(C2H2)2 ions could also be possible and thus could lead to heavier org.-nitrogen compds. and eventually large aerosols. [on SciFinder (R)]},
  Keywords                 = {Entropy; Free energy; Titan (thermodn. parameters for possible clustering reactions of HCNH+, HCNH+(N2) and HCNH+(CH4) ions with acetylene simulating reactions in Titan atm.) Titan atm exptl simulation enthalpy cluster ion formation; methylium imino cluster ion ethyne methane nitrogen Titan atm},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Vacher97,
  Title                    = {Ion-molecule clustering reactions of HCNH+ ion with N2 and/or CH4 in N2-CH4 mixtures},
  Author                   = {Vacher, J. R. and Le Duc, E. and Fitaire, M.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1997},
  Note                     = {CAN 128:207045 53-9 Mineralogical and Geological Chemistry Laboratoire de Physique des Gaz et des Plasmas,(associated to C. N. R. S.), Universite Paris-Sud,Orsay,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 7727-37-9 (Nitrogen); 15845-29-1 (Methylene Amidogen) Role: GPR (Geological or astronomical process), RCT (Reactant), PROC (Process), RACT (Reactant or reagent) (ion-mol. clustering reactions of HCNH+ ion with N2 and/or CH4 in N2-CH4 mixts.)},
  Number                   = {11},
  Pages                    = {1407-1416},
  Volume                   = {45},

  Abstract                 = {Thermochem. data for several ion-mol. clustering reactions of HCNH+ ion with N2 and (or) CH4 have been studied by high-pressure mass spectrometry in order to better understand the chem. of the atm. of Titan. Clustering equil. were obtained in gas mixts. irradiated by a-particles in the temp. range 145-300 K. The enthalpy and entropy changes for the reactions HCNH+(N2)n+CH4+N2.tautm.HCNH+(N2)n(CH4)+n2 (n = 0-2), HCNH+(N2)n-1(CH4) + N2 + N2.tautm.HCNH+(N2)n + (CH4)+N2 (n = 1, 2), HCNH+(CH4)+CH4+N2 .tautm. HCNH+(CH4)+N2 have been measured. The low enthalpy changes suggest that the nature of the bonding remains mainly electrostatic for these reactions. Enthalpy changes as a function of n for X+. (N2)n and X+(CH4)n ions (X+ = CH5+, C2H5+ and C3H7+) were compared with those for HCNH+(N2)n and HCNH+(CH4)n ions. The free energy changes for the step-wise replacement of N2 by CH4 in HCNH+(N2)m(CH4)n clusters (n and m = 0, 1) have been detd. The ligand-exchange reaction HCNH+(N2)+CH4.tautm.HCNH+(CH4)+N2, the first step of a possible path for the formation of org.-nitrogen compds., could exist for T .apprx. 160 K in Titan's atm. (.apprx. 150 km). Switching reactions from HCNH+(N2)(CH4) involving N2, CH4 and C2H2 and leading to HCNH+(C2H2)2 could also be a path yielding heavier org.-nitrogen compds. in the lower Titan's atm. [on SciFinder (R)]},
  Keywords                 = {Molecular association; Titan (ion-mol. clustering reactions of HCNH+ ion with N2 and/or CH4 in N2-CH4 mixts.) Titan ionosphere methane nitrogen clustering reaction; methylene amidogen clustering reaction Titan ionosphere},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

