@Article{Wong02,
  Title                    = {Evolution of CO on Titan},
  Author                   = {Wong, Ah-San and Morgan, Christopher G. and Yung, Yuk L. and Owen, Tobias},
  Journal                  = {Icarus},
  Year                     = {2002},
  Note                     = {CAN 137:111946 53-9 Mineralogical and Geological Chemistry Division of Geological and Planetary Sciences,California Institute of Technology,Pasadena,CA,USA. Journal 0019-1035 written in English. 14762-74-4 (Carbon-13); 14797-71-8 (Oxygen-18) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (geol. indicator, in Titan's atm. CO; massive escape of CO from the early Titan atm. produced heavy isotopomer enrichments, which are preserved for heavy O but have been lost for heavy C through CH2-mediated photochem. C exchange with CH4); 630-08-0 (Carbon monoxide); 4906-87-0 (Carbon monoxide-18O); 13311-49-4 (Carbon monoxide-17O) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in Titan atm.; massive escape of CO from the early Titan atm. produced heavy isotopomer enrichments, which are preserved for heavy O but have been lost for heavy C through CH2-mediated photochem. C exchange with CH4); 74-82-8 (Methane); 1641-69-6 (Carbon-13C monoxide); 2465-56-7 (Methylene) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), RCT (Reactant), OCCU (Occurrence), PROC (Process), RACT (Reactant or reagent) (in Titan atm.; massive escape of CO from the early Titan atm. produced heavy isotopomer enrichments, which are preserved for heavy O but have been lost for heavy C through CH2-mediated photochem. C exchange with CH4)},
  Number                   = {2},
  Pages                    = {382-392},
  Volume                   = {155},

  Abstract                 = {The early evolution of Titan's atm. is expected to produce enrichment in the heavy isotopomers of CO, 13CO and C18O, relative to 12C16O; however, the original isotopic signatures may be altered by photochem. reactions. This paper explains why there is no isotopic enrichment in C in Titan's atm., despite significant enrichment of heavy H, N, and O isotopes. We show that there is a rapid exchange of C atoms between the CH4 and CO reservoirs, mediated by the reaction 1CH2 + *CO -> 1*CH2 + CO, where *C is 13C. Based on recent lab. measurements, we est. the rate coeff. for this reaction to be 3.2 * 10-12 cm3 s-1 at the temp. appropriate for the upper atm. of Titan. We investigate the isotopic diln. of CO using the Caltech/JPL one-dimensional photochem. model of Titan. Our model suggests that the time const. for isotopic exchange through the above reaction is about 800 Myr, which is significantly shorter than the age of Titan, and therefore any original isotopic enhancement of 13C in CO may have been dild. by the exchange process. In addn., a plausible model for the evolution history of CO on Titan after the initial escape is proposed. [on SciFinder (R)]},
  Keywords                 = {Titan (massive escape of CO from the early Titan atm. produced heavy isotopomer enrichments, which are preserved for heavy O but have been lost for heavy C through CH2-mediated photochem. C exchange with CH4) Titan atm carbon monoxide isotope fractionation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Wong2003,
