@Article{Lunine97,
  Title                    = {Moist convection model for Titan},
  Author                   = {Lunine, J. I. and Awal, M.},
  Journal                  = {European Space Agency, [Special Publication] ESA SP},
  Year                     = {1997},
  Note                     = {CAN 128:104540 53-9 Mineralogical and Geological Chemistry Lunar and Planetary Lab.,Univ. Arizona,Tucson,AZ,USA. Journal 0379-6566 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (atm. condensable; math. modeling of occurrence and properties of moist convective clouds in the troposphere of Titan, based on momentum balance considerations)},
  Number                   = {Huygens: Science. Payload and Mission},
  Pages                    = {313-320},
  Volume                   = {ESA SP-1177},

  Abstract                 = {We present a model that predicts the occurrence and properties of moist convective clouds in the troposphere of Titan, based on momentum balance considerations. Unlike the giant planets of the outer solar system, the primary atm. condensable (methane) is lighter than the background air (nitrogen), so that as on Earth clouds are buoyant due both to the larger amt. of condensable in the column (relative to the environment) and the release of latent heat. Upward velocities of the order of 10 m/s are possible in the plumes up to altitudes of roughly 30 km. However, such 'methane-nitrogen' thunderstorms must be rare in Titan's atm., based on the available convective flux: no more than perhaps 10-5 of the globe might be covered by deep moist convection at any one time. The results suggest that both Probe icing and wind shears assocd. with thunderstorms are of little concern for Huygens, but such storms might well be visible to the Descent Imager/Spectral Radiometer (DISR). [on SciFinder (R)]},
  Keywords                 = {Space vehicles (Huygens Probe; math. modeling of likelihood of Huygens Probe experiencing icing and wind shears); Simulation and Modeling; Titan; Troposphere (math. modeling of occurrence and properties of moist convective clouds in the troposphere of Titan, based on momentum balance considerations); Clouds (moist convective; math. modeling of occurrence and properties of moist convective clouds in the troposphere of Titan, based on momentum balance considerations) Titan moist convection model},
  Owner                    = {pernot},
