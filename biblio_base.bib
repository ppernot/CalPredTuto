@TechReport{Mansour94,
  Title                    = {Cosmic and solar radiation induced suprathermal processes in Titan's atmosphere},
  Author                   = {Mansour, A. M. El-Sagheer and Abd Elkhalk, H. and Roessler, K.},
  Institution              = {Sci. Fac.,Suez Canal Univ.,Ismailiya,Egypt.},
  Year                     = {1994},
  Note                     = {CAN 122:295625 53-9 Mineralogical and Geological Chemistry Report 0366-0885 written in English. 12586-59-3 (Proton); 12587-46-1 (Helium, ion(2+); 14067-02-8 (Iron, ion(1+); 14067-05-1 (Carbon, ion(1+) Role: PEP (Physical, engineering or chemical process), PROC (Process) (from solar wind; computer simulation of cosmic and solar radiation induced suprathermal processes in Titan's atm. using a modified MARLOWE program)},

  Abstract                 = {A theor. study was made on the interaction of energetic H, He, C, and Fe ions from solar wind and cosmic rays with the atm. of Titan, a satellite of planet Saturn. It was based on computer simulation of binary collisions with the program MARLOWE in a kind of Monte Carlo calcn. Two main issues were discussed. Firstly, the prodn. of energetic secondary atoms with energies exceeding 0.5 eV by knock on processes and the range of the primary ions in a model lattice contg. the atm. components nitrogen, argon, and methane in 3 mixts. was modeled. Secondly, the model calcn. was applied to Titan's atm. using the range of primaries and the barometric formula in a 10 km bin arrangement. A distribution profile of secondary N, C, and H atoms resulted, depending on the kind and energy of primary ions. The MARLOWE program had to be modified for the application to a gas phase and high energy collisions. The latter was achieved by introducing the Bethe-Bloch formalism and relativistic corrections to inelastic loss calcn. Results show that heavy primary particles (C, Fe) produce very high nos. of secondaries. This counterbalances their low abundance in space radiation in view of chem. effects. An interesting result was that light primaries with 108 eV and heavy ones even up to 1010 eV dissipate their energy mainly within Titan's atm. and do not reach the solid surface. For a 98% N2 and 2% CH4 mixt. the column d. of all hot atoms generated by space particle radiation from H to Fe was calcd. to about 2*109 cm-2s-1. An important contribution of these hot atoms can be expected locally in the actual atm. On a global scale in the change from a primordial to the existing atm. during the lifetime of the solar system, the no. d. of these suprathermal atoms amounted to ~ 2*1026 cm-2, i.e. the total column d. of the atm., which implies that each atom once had been a suprathermal atom. The study was complemented by an exptl. approach. Hot chem. reactions of secondary atoms were studied by 32 MeV 3He2+ ions from Juelich compact cyclotron impinging onto a gas target with pure methane and mixts. of N2 and CH4 at 1.6 bar. The nuclear reaction 12C(3He,4He)11C created radioactive 11C recoils which labeled typical hot products. These could be analyzed by radiogaschromatog. Results show that the formation of carbon-nitrogen compds. is not favored, a result which is reflected in the atm. compn. of Titan. [on SciFinder (R)]},
  Keywords                 = {Cosmic ray; Solar flares and wind (computer simulation of cosmic and solar radiation induced suprathermal processes in Titan's atm. using a modified MARLOWE program); Simulation and Modeling (of cosmic and solar radiation induced suprathermal processes in Titan's atm. using a modified MARLOWE program); Particles (suprathermal; computer simulation of cosmic and solar radiation induced suprathermal processes in Titan's atm. using a modified MARLOWE program); Planets (Saturn, Titan satellite, computer simulation of cosmic and solar radiation induced suprathermal processes in Titan's atm. using a modified MARLOWE program) Titan atm suprathermal reaction computer simulation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Marcano01,
  Title                    = {An experimental approach to production of peptide-like compounds in the early terrestrial planets},
