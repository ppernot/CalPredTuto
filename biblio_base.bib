}

@Article{Lunine96,
  Title                    = {The surface of Titan revealed by Cassini/Huygens},
  Author                   = {Lunine, Jonathan I. and Lorenz, Ralph D.},
  Journal                  = {Proceedings of SPIE-The International Society for Optical Engineering},
  Year                     = {1996},
  Note                     = {CAN 126:34575 53-9 Mineralogical and Geological Chemistry Lunar and Planetary Laboratory,University Arizona,Tucson,AZ,USA. Journal 0277-786X written in English. 74-82-8 (Methane) Role: PRP (Properties), RCT (Reactant), RACT (Reactant or reagent) (in haze over surface of Titan; data on the surface of Titan obtained by the Cassini/Huygens payload)},
  Number                   = {Cassini/Huygens: A Mission to the Saturnian Systems},
  Pages                    = {45-54},
  Volume                   = {2803},

  Abstract                 = {Titan is Saturn's largest moon, and the second-largest natural satellite in the solar system. The surface is obscured from view by suspended haze layers, which are the products of methane photochem., within an atm. composed largely of mol. nitrogen and with a d. exceeding that of Earth's. Titan thus affords a remarkable opportunity to understand the evolution of an org.-rich, planet- sized world with chem. cycles powered over geol. time by sunlight. Because of the difficulties of viewing the surface remotely, a full understanding of the nature of this complex world requires a campaign of in-situ and close-flyby observations. The Cassini/Huygens payload is uniquely designed to conduct such an exploration from Saturn orbit and within Titan's atm. Direct sampling of the atm. chem. by gas-chromatog. and mass spectroscopy will be complemented by global remote spectra collected in the UV through the IR. Probe images in the optical and near-IR right up to the point of impact will be complemented by Orbiter imagery in the near-IR and through active radar sounding. The synergy between Orbiter and Probe observations required to do a first comprehensive exploration of Titan is a uniquely powerful capability of this mission. [on SciFinder (R)]},
  Keywords                 = {Saturn; Solar system; Titan (data on the surface of Titan obtained by the Cassini/Huygens payload) planet Saturn Titan satellite surface Cassini},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Lunine98,
  Title                    = {Some speculations on Titan's past, present and future},
  Author                   = {Lunine, Jonathan I. and Lorenz, Ralph D. and Hartmann, William K.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1998},
  Note                     = {CAN 130:198860 53-0 Mineralogical and Geological Chemistry Lunar and Planetary Laboratory,University of Arizona,Tucson,AZ,USA. Journal; General Review 0032-0633 written in English.},
  Number                   = {9/10},
  Pages                    = {1099-1107},
  Volume                   = {46},

  Abstract                 = {A review, with 54 refs., is given. The solar system's 2nd largest natural satellite is shrouded by a thick nitrogen atm., rich in methane, within which sunlight- and cosmic-ray-driven org. chem. has gone on for some 4.5 billion years. The earliest history of Titan's atm., and specifically its origin, remains unclear until the Cassini-Huygens probe measures the ratio of argon to nitrogen and the abundances of other noble gases and isotopes. However, the abundance of deuterated methane in the atm. today is consistent with an atm. that originated in the chem.-processed sub-nebula around Saturn, rather than in cometary material. Titan's overall atm. history is driven by the depletion of methane, and the mechanisms by which methane might be resupplied from surface on external sources. Remote sensing data mitigate against a large reservoir of methane at Titan's surface, leaving open the possibility that methane is periodically depleted from Titan's atm. on timescales of 107-108 years; under such conditions Titan might oscillate between thin and thick atm. epochs. Titan's surface may provide a repository for complex org. mols. that were synthesized during times when liq. water was temporarily available on the surface, such as after impacts or cryo-volcanic eruptions. Such mols. might provide clues to the resoln. of some difficult issues assocd. with the origin of life. Identifying the presence and nature of such mols. is a difficult exploration problem that must be left to missions which follow-on from the Cassini-Huygens exploration of Titan. [on SciFinder (R)]},
  Keywords                 = {Atmosphere (origin and evolution of atm. on Titan and implications for interior); Titan (speculations on Titan's past, present and future) review Titan atm origin evolution},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

