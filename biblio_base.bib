@Article{Bernard03,
  Title                    = {Experimental simulation of Titan's atmosphere: Detection of ammonia and ethylene oxide},
  Author                   = {Bernard, J. M. and Coll, P. and Coustenis, A. and Raulin, F.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2003},
  Number                   = {14-15},
  Pages                    = {1003-1011},
  Volume                   = {51},

  Abstract                 = {For several years now, an exptl. simulation of Titan's atm. has been on going at LISA. A cold plasma is established in a gas mixt. representative of the atm. of the satellite. In these expts., more than 70 org. compds. have been identified, including the first identification in this type of exptl. simulation of C4N2 already detected in its solid form on Titan, which suggests that the setup correctly mimics the chem. of Titan's atm. We have carried out the first exptl. simulation including O-contg. compds. in order to study the influence of the presence of CO on the chem. behavior of Titan's atm. With the help of gas chromatog.-mass spectrometry (GC-MS) and IR spectroscopy (IRS) we can thus det. which minor species still undetected in Titan's atm. are likely to be present and understand the complex chem. of the atm. of this satellite. Surprisingly we have identified unpredicted O-contg. gaseous compds., mainly ethylene oxide (also named oxirane, C2H4O). This mol. has been obsd. in the interstellar medium by observation in the millimeter region (Astrophys. J. 489 (1997) 553; Astron. Astrophys. 337 (1998) 275). On the contrary, the predicted O-compds. (formaldehyde and methanol) have not been identified in this expt. Furthermore, we have identified NH3 in the gaseous products with an initial mixt. of N2 (98%) and CH4 (2%). The paper describes the exptl. device used in this work, in particular the IRS and GC-MS techniques. We also comment the results related to the detection of the O-contg. compds. and NH3 and their implications on our knowledge of the chem. of Titan's atm. and on the retrieval of the future Titan data expected from Cassini-Huygens. [on SciFinder (R)]},
  Keywords                 = {Titan (ammonia and ethylene oxide detected in exptl. simulations of Titan's atm.) Titan simulated atm carbon monoxide oxirane ammonia},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

