}

@Article{Fortes00,
  Title                    = {Exobiological implications of a possible ammonia-water ocean inside Titan},
  Author                   = {Fortes, A. D.},
  Journal                  = {Icarus},
  Year                     = {2000},
  Number                   = {2},
  Pages                    = {444-452},
  Volume                   = {146},

  Abstract                 = {Models of Titan's thermal history indicate that an ocean consisting of an ammonia-water soln., as much as 200 km deep, is presently concealed beneath a crust of water ice. Conditions within this hypothetical ocean are discussed here and compared with conditions in terrestrial biomes. These conditions, while extreme by terrestrial stds., are such that life could indeed survive. This is in stark contrast with conditions on the surface where even the simplest prebiotic reactions have half-lives of the order 107 years, and free water would only infrequently be available to participate in chem. reactions. The plausible existence of life within Titan's subterranean ocean is discussed in terms of the effects that gaseous waste products of such indigenous organisms could have on the chem. and isotopic compn. of the atm. The possibility that all of Titan's current atm. CH4 and N2 is of biogenic origin is explored from a consideration of the potential productivity of oceanic microbes. While microbial life appears to be possible, it is unlikely that the Cassini/Huygens mission will be able to distinguish any unambiguous biomarkers. The existence and possible depth of the ocean may be constrained from Cassini flyby data, but detection of life in that ocean calls for a closer examn. than the Huygens lander will afford us. (c) 2000 Academic Press. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; possible microbial life in ammonia-water ocean at depth inside Titan being responsible for current chem. of Titan's atm. through gaseous waste products); Life (origin, exobiol.; possible microbial life in ammonia-water ocean at depth inside Titan being responsible for current chem. of Titan's atm. through gaseous waste products); Microorganism; Titan (possible microbial life in ammonia-water ocean at depth inside Titan being responsible for current chem. of Titan's atm. through gaseous waste products) exobiol microbial life ammonia water ocean Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Fouchet00,
