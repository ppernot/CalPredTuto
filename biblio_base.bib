@Article{Lorenz93,
  Title                    = {The life, death and afterlife of a raindrop on Titan},
  Author                   = {Lorenz, Ralph D.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1993},
  Note                     = {CAN 121:87918 53-9 Mineralogical and Geological Chemistry Unit Space Sci.,Univ. Kent,Canterbury,UK. Journal 0032-0633 written in English. 74-82-8 (Methane) Role: MSC (Miscellaneous) (raindrops, descent rates and evapn. and sizes of, model predictions for, in Titan atm.)},
  Number                   = {9},
  Pages                    = {647-55},
  Volume                   = {41},

  Abstract                 = {A model is presented which describes the descent rate and evapn. rate of methane raindrops on Titan. The model, using conventional aerodynamics, with raindrop distortion parameterized by the Weber no., gives excellent agreement with terrestrial raindrop data. Terminal descent velocities for drops of various sizes at different altitudes are presented, and it is found that the largest raindrops may be larger than those on Earth (9.5 mm diam. vs 6.5 mm diam.) yet fall much more slowly (1.6 m s-1 vs 9.2 m s-1). Under std. conditions on Titan, raindrops evap. before they reach the ground : profiles showing the shrinking of drops due to evapn. during their descent are shown for various values of relative humidity. A 500 m increase in elevation can lead to a tenfold increase in rain mass flux, leading to increased \"washing\" of highland terrain. It is pointed out that evapg. raindrops will leave behind their condensation nuclei: fall times for these are presented, and it is noted that they may significantly affect visibility in the troposphere. The effects of addnl. factors on raindrop behavior, such as the nonideal soly. of nitrogen in methane, are briefly considered. [on SciFinder (R)]},
  Keywords                 = {Simulation and Modeling (of behavior of methane raindrops in Titan atm.); Planets (Saturn, Titan satellite, atm. of, methane raindrops in, modeling of behavior of) methane raindrop fall evapn atm Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

