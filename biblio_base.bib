@Article{Trafton75a,
  Title                    = {Morphology of Titan's methane bands. I. Comparison with a reflecting layer model},
  Author                   = {Trafton, L.},
  Journal                  = {Astrophysical Journal},
  Year                     = {1975},
  Note                     = {CAN 82:148248 73-7 Spectra by Absorption, Emission, Reflection, or Magnetic Resonance, and Other Optical Properties McDonald Obs.,Univ. Texas,Austin,TX,USA. Journal 0004-637X written in English. 74-82-8 Role: PRP (Properties) (spectral lines of, in spectrum of Titan satellite of planet Saturn)},
  Number                   = {3, Pt. 1},
  Pages                    = {805-14},
  Volume                   = {195},

  Abstract                 = {Titan's spectrum at moderate resoln. is compared with and contrasted to Saturn's spectrum from 5800 to 12,000 .ANG.. The filling-in of Titan's bands arises partly from a curve-of-growth effect caused by the different pressures and visible-methane abundances in the 2 atms. Most of Titan's bands are satd. in the sense that the absorption of both their stronger and weaker lines is affected by the pressure. On the other hand, the weaker lines of Saturn's bands lie essentially on the linear part of the curve of growth. In principle, this permits us to evaluate the parameters of a band model describing the mean transmission vs. wavelength for a given band by using the spectra of both planets simultaneously. The reflecting layer model, however, cannot be reconciled with the observations of Titan. This is true whether or not results from an anal. of the R(5) manifold of Titan's 3n3 CH4 band are considered. This implies that aerosol scattering plays a significant role in Titan's band formation. The most likely candidate for such an aerosol is frozen CH4 particles. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite of, methane spectral bands in) methane spectra Titan; visible spectra methane Titan; IR spectra methane Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
