@Article{Coustenis97a,
  Title                    = {ISO observations of Titan with SWS/grating},
  Author                   = {Coustenis, A. and Encrenaz, Th and Salama, A. and Lellouch, E. and Gautier, D. and Kessler, M. F. and De Graauw, Th and Samuelson, R. E. and Bjoraker, G. and Orton, G.},
  Journal                  = {European Space Agency, [Special Publication] ESA SP},
  Year                     = {1997},
  Number                   = {First ISO Workshop on Analytical Spectroscopy, 1997},
  Pages                    = {255-258},
  Volume                   = {ESA SP-419},

  Abstract                 = {IR Space Observatory (ISO) observations of Titan in the thermal IR were performed around Jan. 10, using the Grating mode of the Short Wavelength Spectrometer (SWS) in the 2-45 mm region. Titan was at Eastern Elongation with respect to Saturn, showing its leading hemisphere as viewed from the Earth. Strong emission bands of hydrocarbons, nitriles and CO2, seen by Voyager/IRIS near Titan's equator, are all found in the ISO/SWS data. Hydrogen cyanide, acetylene, and propane show strong emission bands in the 680-780 cm-1 spectral region. Benzene, predicted to exist in Titan's atm. from lab. simulations and photochem. considerations, is not obsd. at 673.5 cm-1. The region from 780 to 1000 cm-1 exhibits the emission lines of the n9 ethane band at 821 cm-1 and of the n7 ethylene band at 949 cm-1. With respect to Voyager-inferred modeling of a Titan disk-av., we find that the differences with the Voyager stratospheric abundances are very slight. [on SciFinder (R)]},
  Keywords                 = {IR spectra; Titan (IR emission spectra as obsd. by ISO, using SWS/grating, as indicators of hydrocarbon abundances in stratosphere of Titan); Hydrocarbons Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (IR emission spectra as obsd. by ISO, using SWS/grating, as indicators of hydrocarbon abundances in stratosphere of Titan) IR spectrum hydrocarbon Titan stratosphere},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Coustenis01,
  Title                    = {Images of Titan at 1.3 and 1.6 mm with Adaptive Optics at the CFHT},
  Author                   = {Coustenis, Athena and Gendron, Eric and Lai, Olivier and Veran, Jean-Pierre and Woillez, Julien and Combes, Michel and Vapillon, Loic and Fusco, Thierry and Mugnier, Laurent and Rannou, Pascal},
  Journal                  = {Icarus},
  Year                     = {2001},
  Number                   = {2},
  Pages                    = {501-515},
  Volume                   = {154},

  Abstract                 = {Titan was obsd. with the Adaptive Optics Bonette at the Canada-France-Hawaii Telescope during Oct. 27th 1998 (UTC), when the satellite was at greatest eastern elongation (GEE) with respect to Saturn and its leading hemisphere was seen from the Earth. The seeing was excellent during these observations (with peaks at 0.3'' in the visible), and this allowed us to successfully image Titan for the 1st time in the J band, where the adaptive optics correction is highly dependent on the atm. conditions. Images were obtained in the center of the so-called \"methane windows,\" where the absorption is weak (at 1.29 -J1- and 1.6 mm -H1-) and also in the wings of the CH4 bands (at 1.18 mm -J2- and 1.64 mm -H2-) with narrowband filters. The latter wavelengths yield information on Titan's stratosphere and allow us, by subtracting its contribution from the J1 and H1 images, to infer furthermore direct information on the surface properties. The resoln. on the J and H diffraction-limited images is .apprx.0.08-0.1'', translating into .apprx.10 independent resoln. elements or 20 pixels on the Titan disk. We have obtained reconstructed PSFs of very high signal-to-noise ratio, with assocd. Strehl ratios of .apprx.35% in J and 50% in H. Hence, new and more efficient deconvolution methods (such as MISTRAL) were applied to the images, reducing ringing defects and restoring the initial photometry while enhancing the contrast on the obsd. features. Thus, the main features of the Titan atm. (bright south pole) and surface (large bright equatorial region and darker areas) - as previously obsd. with ADONIS/ESO, as well as with the HST and the Keck telescopes-are apparent, but with a much higher level of detail and contrast (.apprx.30%) at these wavelengths. In addn., on the J1 and J2 images, along with the north-south asymmetry (which is probably due to seasonal effects), another bright feature is reported for the 1st time seen on Titan's morning limb (anti-Saturn during GEE). This feature may be diagnostic of diurnal effects (such as morning fog) at altitudes of 70-90 km (due to the cycle of the condensable species), but requires further investigation before its origin can be firmly identified. On the surface images (cor. for limb effects and atm. contribution), the large equatorial feature is found to be bright also at 1.3 mm (thus further constraining models of the surface compn.). The high quality of the data allows us to resolve this area into 3 or more individual peaks, possibly towering over a mountainous plateau covered with ice (a plausible candidate being ethane ice). In any event, our images show this part of Titan's leading hemisphere to be more complex than previously suggested by models of Titan's surface. From our albedo maps, it appears that the darker areas have reflectances that are .apprx.3 times lower than the bright equatorial region. (c) 2001 Academic Press. [on SciFinder (R)]},
  Keywords                 = {Titan (images of Titan at 1.3 and 1.6 mm with adaptive optics at Canada-France-Hawaii Telescope during Oct. 27th 1998); IR spectra (near-IR; images of Titan at 1.3 and 1.6 mm with adaptive optics at Canada-France-Hawaii Telescope during Oct. 27th 1998) Titan satellite atm surface IR observation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Coustenis97,
  Title                    = {Titan's atmosphere and surface from infrared spectroscopy and imagery},
  Author                   = {Coustenis, Athena and Lellouch, Emmanuel and Combes, Michel and Wittemberg, Renald and McKay, Christopher P. and Maillard, Jean-Pierre},
  Journal                  = {Astronomical and Biochemical Origins and the Search for Life in the Universe, Proceedings of the International Conference on Bioastronomy, 5th, Capri, Italy, July 1-5, 1996},
  Year                     = {1997},
  Pages                    = {227-234},

  Abstract                 = {We have performed observations of Titan in the near-IR since 1991, using both spectroscopy and imagery. A higher geometric albedo is obsd. near the Greatest Eastern Elongation than near the Greatest Western Elongation or Conjunctions. The surface is within reach for telescopes at 1.075, 1.28, 1.6 and 2.0 mm, so we simulated Titan's geometric albedo in order to derive the surface albedo at all longitudes. Titan's leading hemisphere appears brighter than the trailing one by 20-25% at 1.08; 1.28 and 1.6 mm, and by 35% at 2.0 mm, with spectra compatible with H2O everywhere. We have then simulated the presence of a mountain on Titan covering 50% of the leading hemisphere disk, varying its height and introducing a higher surface albedo when required. We find that a relief can not reproduce the whole geometric albedo differences obsd. between the two hemispheres. Some other addnl. component bright at 1.0 and 2.0 mm must then be invoked. Candidates are tholins or CH4, which can exist on the top of a mountain if the temp. is lower than 90 K. [on SciFinder (R)]},
  Keywords                 = {Water vapor (Titan atm.; Titan's atm. and surface from IR spectroscopy and imagery); Titan (Titan's atm. and surface from IR spectroscopy and imagery); Organic compounds Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (tholins, Titan atm.; Titan's atm. and surface from IR spectroscopy and imagery) Titan atm surface IR spectroscopy imagery},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Coustenis95,
  Title                    = {Titan's surface: composition and variability from the near-infrared albedo},
  Author                   = {Coustenis, A. and Lellouch, E. and Maillard, J. P. and McKay, C. P.},
  Journal                  = {Icarus},
  Year                     = {1995},
  Number                   = {1},
  Pages                    = {87-104},
  Volume                   = {118},

  Abstract                 = {A report observations is made of Titan covering the 1.075-, 1.28-, 1.6-, and 2.0-mm windows which show variations with the orbital phase, implying variability of the satellite's geometric albedo, as found earlier by other authors. The observations cover the region 4400 to 9600 cm-1 and were taken in June 1991, August 1993, and Sept. 1994, using the Fourier transform spectrometer at the CFH Telescope (Hawaii). The resolving power was 10000, 1000, and 300 for the 1991, 1993, and 1994 spectra, resp. The data sets cover Titan's Greatest Western Elongation and opposite Conjunctions. The Titan's geometric albedo is about the same (0.29) at 1.28 and 1.075 mm; on the other hand, observations of Uranus show the 1.28-mm albedo to be lower by 30% than that in the 1.075-mm window. At the four wavelengths, a noticeable decrease in geometric albedo is obsd. near Greatest Western Elongation with respect to Inferior and Superior Conjunctions. The sum of the existing Titan data point to a bright leading side and a darker trailing one, with a max. brightness near 120 Deg (+-20 Deg) orbital longitude and a min. near 230 Deg (+-20 Deg). Despite important uncertainties in the value of the CH4 absorption coeffs. in the window regions, modeling Titan's obsd. albedos strongly suggests the existence of a surface material with a comparable albedo at 1.075 and 1.28 mm (0.4-0.5) but much darker at 1.6 and 2.0 mm (about 0.2). Titan's surface spectrum appears consistent with that of Hyperion and we suggest that this behavior may be due to the dominant presence of water ice on Titan's surface. The inhomogeneity of the surface implies that some other material (e.g., hydrocarbon lakes) is mixed with water ice in variable proportions. [on SciFinder (R)]},
  Keywords                 = {Ice (surface: compn. of Tian satellite and variability from the near-IR albedo); Hydrocarbons Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (surface: compn. of Tian satellite and variability from the near-IR albedo); Planets (Saturn, Titan satellite, surface: compn. of Tian satellite and variability from the near-IR albedo); Planets (Uranus, surface: compn. of Tian satellite and variability from the near-IR albedo) planet Saturn Titan surface compn IR},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Coustenis06,
  Title                    = {Titan's 3-micron spectral region from ISO high-resolution spectroscopy},
  Author                   = {Coustenis, Athena and Negrao, Alberto and Salama, Alberto and Schulz, Bernhard and Lellouch, Emmanuel and Rannou, Pascal and Drossart, Pierre and Encrenaz, Therese and Schmitt, Bernard and Boudon, Vincent and Nikitin, Andrei},
  Journal                  = {Icarus},
  Year                     = {2006},

  Month                    = jan,
  Number                   = {1},
  Pages                    = {176--185},
  Volume                   = {180},

  Abstract                 = {The near-infrared spectrum of Titan, Saturn's largest moon and one of the Cassini/Huygens' space mission primary targets, covers the 0.8 to 5 micron region in which it shows several weak CH4 absorption regions, and in particular one centered near 2.75 micron. Due to the interference of telluric absorption, only part of this window region (2.9-3.1 [mu]m) has previously been observed from the ground [Noll, K.S., Geballe, T.R., Knacke, R., Pendleton, F., Yvonne, J., 1996. Icarus 124, 625-631; Griffith, C.A., Owen, T., Miller, G.A., Geballe, T., 1998. Nature 395, 575-578; Griffith, C.A., Owen, T., Geballe, T.R., Rayner, J., Rannou, P., 2003. Science 300, 628-630; Geballe, T.R., Kim, S.J., Noll, K.S., Griffith, C.A., 2003. Astrophys. J. 583, L39-L42]. We report here on the first spectroscopic observations of Titan covering the whole 2.4-4.9 [mu]m region by two instruments on board the Infrared Space Observatory (ISO) in 1997. These observations show the 2.75-[mu]m window in its complete extent for the first time. In this study we have also used a high-resolution Titan spectrum in the 2.9-3.6 [mu]m region taken with the Keck [Geballe, T.R., Kim, S.J., Noll, K.S., Griffith, C.A., 2003. Astrophys. J. 583, L39-L42; Kim, S.J., Geballe, T.R., Noll, K.S., Courtin, R., 2005. Icarus 173, 522-532] to infer information on the atmospheric parameters (haze extinction, single scattering albedo, methane abundance, etc.) by fitting the methane bands with a detailed microphysical model of Titan's atmosphere (updated from Rannou, P., McKay, C.P., Lorenz, R.D., 2003. Planet. Space Sci. 51, 963-976). We have included in this study an updated version of a database for the CH4 absorption coefficients [STDS, Wenger, Ch., Champion, J.-P., 1998. J. Quant. Spectrosc. Radiat. Transfer 59, 471-480. See also http://www.u-bourgogne.fr/LPUB/TSM/sTDS.html for latest updates; Boudon, V., Champion, J.-P., Gabard, T., Loete, M., Michelot, F., Pierre, G., Rotger, M., Wenger, Ch., Rey, M., 2004. J. Mol. Spectrosc. 228, 620-634]. For the atmosphere we find that (a) the haze extinction profile that best matches the data is one with higher (by 40%) extinction in the atmosphere with respect to Rannou et al. (2003) down to about 30 km where a complete cut-off occurs; (b) the methane mixing ratio at Titan's surface cannot exceed 3% on a disk-average basis, yielding a maximum CH4 column abundance of 2.27 km-am in Titan's atmosphere. From the derived surface albedo spectrum in the 2.7-3.08 micron region, we bring some constraints on Titan's surface composition. The albedo in the center of the methane window varies from 0.01 to 0.08. These values, compared to others reported in the other methane windows, show a strong compatibility with the water ice spectrum in the near-infrared. Without confirming its existence from this work alone, our data then appear to be compatible with water ice. A variety of other ices, such as CO2, NH3, tholin material or hydrocarbon liquid cannot be excluded from our data, but an additional unidentified component with a signature around 2.74 micron is required to satisfy the data.},
  Keywords                 = {Titan, Satellites of Saturn, Surfaces, Satellite, Infrared observations, ISO},
  Owner                    = {pernot},
  Timestamp                = {2008.01.30},
  Url                      = {http://www.sciencedirect.com/science/article/B6WGF-4H6XM5V-1/2/79f8951b0ace1691eea65f4519cc274a}
}

@Article{Coustenis00,
  Title                    = {ISO spectroscopy of Titan},
  Author                   = {Coustenis, A. and Salama, A. and Lellouch, E. and Encrenaz, Th and Schulz, B. and Feuchtgruber, H. and Gautier, D. and Ott, S. and de Graauw, Th and Kessler, M. F.},
  Journal                  = {European Space Agency, [Special Publication] SP},
  Year                     = {2000},
  Number                   = {ISO Beyond the Peaks},
  Pages                    = {13-17},
  Volume                   = {SP-456},

  Abstract                 = {In the spectroscopic mode, Titan was obsd. by ISO in 1997 by SWS/Grating, PHT-S and CAM/CVF. The combination of these data provides Titan's spectrum from 5 to 17 and from 2.5 to 5 mm with resolving powers ranging from 40 (CAM/CVF) to 2000 (SWS). The anal. of the spectra provides information on (a) Titan's atm. structure (temp. and compn.) and (b) Titan's surface (through the emission obsd. in the 2.9-m window). In this paper we conc. on the 7 to 9 and 2.5 to 5 m regions. A temp. profile for Titan's disk is inferred from the anal. of the 7.7 mm CH4 band. The CH3D abundance is estd. to be 7.5 () * 10-6, for a D/H ratio of 9.5 () * 10-5. The 2.9 methane \"window\" on Titan is obsd. in its full shape for the first time. It shows two peaks at 2.7 and 2.8 mm, and an absorption feature at 2.75 mm, which may be the spectral signature of a surface component on Titan, possibly water ice that is mixed with tholins. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; ISO spectroscopy of Titan yields ests. of atm. CH3D abundances and temp. profiles and suggests the presence of a water ice-tholin mixt. on the Titan surface); Organic compounds Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (tholins, on Titan surface; ISO spectroscopy of Titan yields ests. of atm. CH3D abundances and temp. profiles and suggests the presence of a water ice-tholin mixt. on the Titan surface) Titan atm deuterium surface water ice},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

