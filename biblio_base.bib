@Article{Tyler81,
  Title                    = {Radio science investigations of the Saturn system with Voyager 1: preliminary results},
  Author                   = {Tyler, G. L. and Eshleman, V. R. and Anderson, J. D. and Levy, G. S. and Lindal, G. F. and Wood, G. E. and Croft, T. A.},
  Journal                  = {Science (Washington, DC, United States)},
  Year                     = {1981},
  Note                     = {CAN 94:165393 73-7 Spectra by Absorption, Emission, Reflection, or Magnetic Resonance, and Other Optical Properties Cent. Radar Astron.,Stanford Univ.,Stanford,CA,USA. Journal 0036-8075 written in English. 74-82-8; 1333-74-0; 7440-59-7; 7727-37-9 Role: OCCU (Occurrence) (in atm. of planet Saturn)},
  Number                   = {4491},
  Pages                    = {201-6},
  Volume                   = {212},

  Abstract                 = {Voyager 1 radio occultation measurements of Titan's equatorial atm. successfully probed to the surface, which is provisionally placed at a radius of 2570 km. Derived scale heights plus other exptl. and theor. results indicate that mol. N is the predominant atm. constituent. The surface pressure and temp. appear to be ~1.6 bars and 93 K, resp. The main clouds are probably CH4 ice, although some condensation of N cannot be ruled out. Solar abundance arguments suggest and the measurements allow large quantities of surface CH4 near its triple-point temp., so that the 3 phases of CH4 could play roles in the atm. and on the surface of Titan similar to those of H2O on Earth. The min. temp. is about 91 K near the 60-millibar pressure level. The measured part of the polar ionosphere of Saturn has a peak electron concn. of 2.3 * 104 cm-3 at an altitude of 2500 km above the 1-bar level in the atm., and a plasma scale height at the top of the ionosphere of 560 km. Attenuation of monochromic radiation at a wavelength of 3.6 cm propagating obliquely through Saturn's rings is consistent with traditional values for the normal optical depth of the rings, but the near-forward scattering of this radiation by the rings indicates effective scattering particles with larger than expected diams. of 10, 8, and 2 m in the A ring, the outer Cassini division, and the C ring, resp. Preliminary anal. of the radio tracking data yields new values for the masses of Saturn. Corresponding values for the mean densities of these object are 1.33 +- 0.10 and ~1.89 g/cm3. The d. of Rhea is consistent with a solar-compn. mix of anhyd. rock and volatiles, while Titan is apparently enriched in silicates relative to the solar compn. [on SciFinder (R)]},
  Keywords                 = {Silicates Role: OCCU (Occurrence) (in Titan satellite of planet Saturn); Planets (Saturn, atm. of, Voyager 1 study of) planet Saturn atm; nitrogen planet Saturn atm; methane planet Saturn atm; silicate Titan satellite Saturn},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

