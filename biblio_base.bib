@Article{Pavlov01a,
  Title                    = {UV shielding of NH3 and O2 by organic hazes in the Archean atmosphere},
  Author                   = {Pavlov, Alexander A. and Brown, Lisa L. and Kasting, James F.},
  Journal                  = {Journal of Geophysical Research, [Planets]},
  Year                     = {2001},
  Note                     = {CAN 136:154075 53-10 Mineralogical and Geological Chemistry Laboratory for Atmospheric and Space Physics,University of Colorado,Boulder,CO,USA. Journal 0148-0227 written in English. 74-82-8 (Methane); 7664-41-7 (Ammonia); 7782-44-7 (Oxygen) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in Archean atm.; UV shielding of NH3 and O2 by methane hazes in the Archean atm.)},
  Number                   = {E10},
  Pages                    = {23267-23287},
  Volume                   = {106},

  Abstract                 = {The late Archean atm. was probably rich in biol. generated CH4 and may well have contained a hydrocarbon haze layer similar to that obsd. today on Saturn's moon, Titan. Here we present a detailed model of the photochem. of haze formation in the early atm., and we examine the effects of such a haze layer on climate and UV radiation. We show that the thickness of the haze layer was limited by a neg. feedback loop: A haze optical depth of more than .apprx.0.5 in the visible would have produced a strong \"antigreenhouse effect,\" thereby cooling the surface and slowing the rate at which CH4 was produced. Given this climatic constraint on its visible optical depth, the amt. of UV shielding provided by the haze can be estd. from knowledge of the optical properties and size distribution of the haze particles. Contrary to previous studies [Sagan and Chyba, 1997], we find that when the finite size of the particles is taken into account, the amt. of UV shielding provided by the haze is small. Thus NH3 should have been rapidly photolyzed and should not have been sufficiently abundant to augment the atm. greenhouse effect. We also examine the question of whether photosynthetically generated O2 could have accumulated beneath the haze layer. For the model parameters considered here, the answer is \"no\": The upper limit on ground level O2 concns. is .apprx.10-6 atm, and a more realistic est. for pO2 during the late Archean is 10-8 atm. The stability of both O2 and NH3 is sensitive to the size distribution and optical properties of the haze particles, neither of which is well known. Further theor. and lab. work is needed to address these uncertainties. [on SciFinder (R)]},
  Keywords                 = {Atmosphere (UV shielding of NH3 and O2 by methane hazes in the Archean atm.); Climate (greenhouse and antigreenhouse effect; UV shielding of NH3 and O2 by methane hazes in the Archean atm.) atm Archean methane haze ammonia oxygen},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Pavlov01,
  Title                    = {Organic haze in Earth's early atmosphere: source of low-13C Late Archean kerogens?},
  Author                   = {Pavlov, Alexander A. and Kasting, James F. and Eigenbrode, Jennifer L. and Freeman, Katherine H.},
  Journal                  = {Geology},
  Year                     = {2001},
  Note                     = {CAN 136:219752 53-10 Mineralogical and Geological Chemistry Department of Geosciences,Pennsylvania State University,University Park,PA,USA. Journal 0091-7613 written in English. 124-38-9 (Carbon dioxide) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (in atm.; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); 14762-74-4 (Carbon-13) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in kerogen; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (methanogenesis; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment)},
  Number                   = {11},
  Pages                    = {1003-1006},
  Volume                   = {29},

  Abstract                 = {High concns. of greenhouse gases would have been required to offset low solar luminosity early in Earth's history. Enhanced CO2 levels are probably at least part of the soln., but CH4 may have played a significant role as well, particularly during the Late Archean era, 2.5-3.0 Ga, when methanogenic bacteria were almost certainly present. Indeed, biol. CH4 prodn. should have led to CO2 drawdown by way of a neg. feedback loop involving the carbonate-silicate geochem. cycle. We suggest here that the atm. CH4/CO2 ratio approached the value of .apprx.1 needed to trigger formation of Titan-like org. haze. This haze was strongly depleted in 13C relative to 12C and was produced at a rate comparable to the modern rate of org. carbon burial in marine sediments. Therefore, it could provide a novel explanation for the presence of extremely low-13C kerogens in Late Archean sediments. [on SciFinder (R)]},
  Keywords                 = {Atmosphere (Archean; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Climate (greenhouse effect; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Methanogenic bacteria (methane prodn.; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Greenhouse gases; Marine sediments (org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Kerogens Role: GPR (Geological or astronomical process), PRP (Properties), PROC (Process) (org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Aerosols (org.; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment); Optical properties (translucency; org. haze in Earth's early atm. as a source of low-13C Late Archean kerogens found in Late Archean sediment) Archean kerogen Earth atm methanogenesis org haze hydrocarbon aerosol},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

