  Author                   = {Niemann, H. B. and Atreya, S. K. and Bauer, S. J. and Biemann, K. and Block, B. and Carignan, G. R. and Donahue, T. M. and Frost, R. L. and Gautier, D. and Haberman, J. A. and Harpold, D. and Hunten, D. M. and Israel, G. and Lunine, J. I. and Mauersberger, K. and Owen, T. C. and Raulin, F. and Richards, J. E. and Way, S. H.},
  Journal                  = {Space Science Reviews},
