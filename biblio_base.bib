@Article{Hanel82,
  Title                    = {Infrared observations of the Saturnian system from Voyager 2},
  Author                   = {Hanel, R. and Conrath, B. and Flasar, F. M. and Kunde, V. and Maguire, W. and Pearl, J. and Pirraglia, J. and Samuelson, R. and Cruikshank, D. and et al.},
  Journal                  = {Science (Washington, DC, United States)},
  Year                     = {1982},
  Note                     = {CAN 96:94508 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Goddard Space Flight Cent.,NASA,Greenbelt,MD,USA. Journal 0036-8075 written in English. 74-82-8; 74-84-0; 74-85-1; 74-86-2; 74-90-8; 74-98-6; 74-99-7; 460-12-8; 1333-74-0 Role: PRP (Properties) (spectral lines of, in spectrum of Saturn system, from Voyager 2 encounter)},
  Number                   = {4532},
  Pages                    = {544-8},
  Volume                   = {215},

  Abstract                 = {During the passage of Voyage 2 through the Saturn system, IR spectral and radiometric data were obtained for Saturn, Titan, Enceladus, Tethys, Iapetus, and the rings. Combined Voyager 1 and Voyager 2 observations of temps. in the upper troposphere of Saturn indicate a seasonal asymmetry between the northern and southern hemispheres, with superposed small-scale merdional gradients. Comparison of high spatial resoln. data from the 2 hemispheres poleward of 60 Deg latitude suggests an approx. symmetry in the small-scale structure, consistent with the extension of a sym. system of zonal jets into the polar regions. Longitudinal variations of 1 to 2 K are obsd. Disk-averaged IR spectra of Titan show little change over the 9-mo interval between Voyager encounters. By combining Voyager 2 temp. measurements with ground-based geometric albedo detns., phase integrals of 0.91 +- 0.13 and 0.89 +- 0.09 were derived for Tethys and Enceladus, resp. The subsolar point temp. of dark material on Iapetus must exceed 110 K. Temps. (and IR optical depths) for the A and C rings and for the Cassini division are 69 +- 1 K (0.40 +- 0.05), 85 +- 1 K (0.10 +- 0.03), and 85 +- 2 K (0.07 +- 0.04), resp. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, IR spectrum of, from Voyage 2 encounter) IR Saturn Voyager 2},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Hanke2011,
  Title                    = {Sensitivity analysis and uncertainty calculation for dispersion corrected density functional theory.},
  Author                   = {Hanke, Felix},
  Journal                  = {J Comput Chem},
  Year                     = {2011},

  Month                    = {May},
  Number                   = {7},
  Pages                    = {1424--1430},
  Volume                   = {32},

  Abstract                 = {The precision of binding energies and distances computed with dispersion-corrected density functional theory (DFT-D) is investigated by propagation of uncertainties, yielding relative uncertainties of several percent. Sensitivity analysis is used to calculate the geometry-dependent relative importance of each input parameter for the dispersion correction. While DFT-Ds are exact at asymptotically large distances, their damping functions are shown to play a significant role in binding geometries. This is demonstrated in detail for the interlayer binding of graphite. The techniques presented allow practitioners to quickly compute error bars and to get an a posteriori estimate about the transferability of their results. They can also aid the development of future dispersion corrections.},
  Doi                      = {10.1002/jcc.21724},
  Institution              = {Surface Science Research Centre, University of Liverpool, Liverpool, United Kingdom. hanke@liverpool.ac.uk},
