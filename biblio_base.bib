@TechReport{Thompson85,
  Title                    = {Phase equilibria in nitrogen-hydrocarbon systems: applications to Titan},
  Author                   = {Thompson, W. Reid},
  Institution              = {Lab. Planet. Stud.,Cornell Univ.,Ithaca,NY,USA.},
  Year                     = {1985},
  Note                     = {CAN 104:171937 53-9 Mineralogical and Geological Chemistry Report 0379-6566 written in English. 74-98-6 Role: PRP (Properties) (systems, ethane-methane-nitrogen-, phase equil. of, cloud and ocean compns. on Titan in relation to); 7727-37-9 Role: PRP (Properties) (systems, ethane-methane-propane-, phase equil. of, cloud and ocean compns. of Titan in relation to); 74-82-8 Role: PRP (Properties) (systems, ethane-nitrogen-propane-, phase equil. of, cloud and ocean compns. on Titan in relation to); 74-84-0 Role: PRP (Properties) (systems, methane-nitrogen-propane-, phase equil. of, cloud and ocean compns. on Titan in relation to)},

  Abstract                 = {Phase equil. in the N2-CH4-C2H6-C3H8 system are modeled using an expression for activity coeffs. in multicomponent systems. The model predicts that Titan cloud droplets are liq. CH4 with 15-25% dissolved N2 (increasing with altitude), up to the (13-km, 0.7-bar, 80 Deg K) level, and solid CH4 contg. 13-26% N2 above. The pressure- and temp.-dependent distribution of CH4 and N between atm. and ocean is computed; .gtorsim.2.5% CH4 in the troposphere is necessary to condense any cloud. This implies an ocean of .ltorsim.16 mol % CH4 and .gtorsim.75 mol % C2H6, of depth .gtorsim.920 m. Increasing the partial pressure of CH4 implies a warmer climate in Titan's past. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, cloud and ocean compns. of, phase equil. in ethane-methane-nitrogen-propane systems in relation to) Titan cloud ocean compn},
