@Article{Mul1981,
  Title                    = {Merged electron-ion beam experiments. IV. Dissociative recombination for the methane group {$CH^+,...,CH_5^+$}},
  Author                   = {P. M. Mul and J. B. A. Mitchell and V. S. D'Angelo and P. Defrance and J. W. McGowan and H. R. Froelich},
