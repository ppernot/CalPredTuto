@Article{Lee00,
  Title                    = {Photochemical modeling of CH3 abundances in the outer solar system},
  Author                   = {Lee, Anthony Y. T. and Yung, Yuk L. and Moses, Julianne},
  Journal                  = {Journal of Geophysical Research, [Planets]},
  Year                     = {2000},
  Note                     = {CAN 133:298981 53-9 Mineralogical and Geological Chemistry Division of Geological and Planetary Sciences,California Institute of Technology,Pasadena,USA. Journal 0148-0227 written in English. 74-82-8 (Methane); 74-84-0 (Ethane); 74-86-2 (Ethyne); 1333-74-0 (Hydrogen); 2229-07-4 (Methyl); 2669-89-8 (Ethenyl) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in atms. of giant planets and Titan; photochem. modeling of CH3 abundances in the atms. of the giant planets and Titan)},
  Number                   = {E8},
  Pages                    = {20207-20225},
  Volume                   = {105},

  Abstract                 = {Recent measurements of Me radicals (CH3) in the upper atmospheres of Saturn and Neptune by the IR Space Observatory (ISO) provide new constraints to photochem. models of hydrocarbon chem. in the outer solar system. The derived column abundances of CH3 on Saturn above 10 mbar and Neptune above the 0.2 mbar pressure level are (2.5 - 6.0) * 1013 cm-2 and (0.7 - 2.8) * 1013 cm-2, resp. We use the updated Caltech/Jet Propulsion Lab. photochem. model, which incorporates hydrocarbon photochem., vertical mol. and bulk atm. eddy diffusion, and realistic radiative transfer modeling, to study the CH3 abundances in the upper atm. of the giant planets and Titan. We identify the key reactions that control the concns. of CH3 in the model, such as the three-body recombination reaction, CH3 + CH3 + M -> C2H6 + M. We evaluate and extrapolate the three-body rate const. of this reaction to the low-temp. limit (1.8 * 10-16 T-3.75 e-300/T, T < 300 K) and compare Me radical abundances in five atmospheres: Jupiter, Saturn, Uranus, Neptune, and Titan. The sensitivity of our models to the rate coeffs. for the reactions H + CH3 + M -> CH4 + M, H + C2H3 -> C2H2 + H2, 1CH2 + H2 -> CH3 + H, and H + C2H5 -> 2 CH3, the branching ratios of CH4 photolysis, vertical mixing in the five atmospheres, and Lyman a photon enhancement at the orbit of Neptune have all been tested. The results of our model CH3 abundances for both Saturn (5.1 * 1013 cm-2) and Neptune (2.2 * 1013 cm-2) show good agreement with ISO Short Wavelength Spectrometer measurements. Using the same chem. reaction set, our calcns. also successfully generate vertical profiles of stable hydrocarbons consistent with Voyager and ground-based measurements in these outer solar system atmospheres. Predictions of CH3 column concns. (for p ? 0.2 mbar) in the atmospheres of Jupiter (3.3 * 1013 cm-2), Uranus (2.5 * 1012 cm-2), and Titan (1.9 * 1015 cm-2) may be checked by future observations. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; photochem. modeling of CH3 abundances in the atms. of the giant planets and Titan); Planets (giant; photochem. modeling of CH3 abundances in the atms. of the giant planets and Titan); Jupiter; Neptune; Saturn; Uranus (photochem. modeling of CH3 abundances in the atms. of the giant planets and Titan) giant planet Titan atm methyl},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

