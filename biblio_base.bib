@Article{Minard00,
  Title                    = {Analytical methods for investigating structure of macromolecular solids relevant to the origin of life},
  Author                   = {Minard, Robert D. and Cody, George D. and Jones, A. Daniel},
  Journal                  = {Abstracts of Papers, 220th ACS National Meeting, Washington, DC, United States, August 20-24, 2000},
  Year                     = {2000},
  Note                     = {Department of Chemistry,Pennsylvania State University,University Park,PA,USA. Journal; Meeting Abstract written in English.},
  Pages                    = {GEOC-015},

  Abstract                 = {There are numerous examples of complex macromol. solids found in carbonaceous chondrites and IDPs or formed in simulation expts. (tholins) and the polymn. of hydrogen cyanide. These macromol. solids provide extremely challenging anal. problems. Both non-degradative methods (solid state NMR, MALDI, SIMS, XANES, etc) and degradative methods (pyrolysis/MS, pyrolysis/GC-MS, hydrolysis/LC-MS or TMAH thermochemolysis/GC-MS have been applied to some or all of these solids and provide some insight into their substructural units. As an example of the latter method, by heating HCN polymer or a tholin produced from a CH4/N2 glow discharge Titan simulation expt. with tetramethylammonium hydroxide at 250-300 DegC, concommitant bond cleavage and in situ methylation takes place yielding a large no. of products that can be sepd. and analyzed by GC-MS (Minard et. al. , 1998). Many products from both HCN polymer and the tholin were the same and imply substructural units of many types: diacyl, amino acyl, purine/pyrimidine, or triazine that are relevant to biochem. The application of this and other anal. methods to the unraveling of the structure of macromol. materials relevant to the origin of life will be presented. Minard, R.D., Hatcher, P.G., Gourley, C.R., and Matthews, C.N., 1998. Structural Investigations of Hydrogen Cyanide Polymers: New Insights Using TMAH Thermochemolysis/GC-MS, Origins of Life and the Evol. Of the Biosphere, 28:461-473. [on SciFinder (R)]},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

