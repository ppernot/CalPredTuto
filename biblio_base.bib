@Article{Vuitton03,
  Title                    = {Intrinsic lifetime of metastable excited C4H2: implications for the photochemistry of C4H2 in Titan's atmosphere},
  Author                   = {Vuitton, V. and Gee, C. and Raulin, F. and Benilan, Y. and Crepin, C. and Gazeau, M. C.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2003},
  Note                     = {CAN 140:238652 53-9 Mineralogical and Geological Chemistry Laboratoire Interuniversitaire des Systemes Atmospheriques,Univ. Paris,Creteil,Fr. Journal 0032-0633 written in English. 74-82-8 (Methane); 460-12-8 (Butadiyne) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in Titan's atm.; implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.)},
  Number                   = {13},
  Pages                    = {847-852},
  Volume                   = {51},

  Abstract                 = {A better understanding of the complex org. chem. occurring in the methane-rich atm. of Titan can be achieved by comparing observational data with the results of photochem. models. Until now, these models failed to reproduce the obsd. concns. of C4H2. This may be due to the lack of kinetic data with regard to polyenes chem. In particular, no data were available for the intrinsic lifetime of the metastable excited state of C4H2. This state, probably a triplet excited state, seems to be a precursor in the photochem. reactions occurring in the atm. of Titan. Std. matrix isolation technique assocd. with time resolved spectroscopy is well adapted to characterize metastable states. Absorption and emission spectra (phosphorescence) of C4H2 in rare gas matrixes are presented and a vibrational anal. is conducted. We also report the first exptl. intrinsic lifetime of C4H*2, measured in Ar and Kr matrixes. A value of about 100 ms in the gas phase is deduced from matrixes results (.apprx.45 ms in Ar and .apprx.20 ms in Kr). The detd. lifetime of C4H2, despite being two orders of magnitude higher than the value currently used in the models, cannot explain the discrepancy between models and observations. Nevertheless, it can account for an increase in the prodn. of heavier compds. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.); Titan (implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.); Polyenes Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (in Titan's atm; implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.); Atmosphere (of Titan; implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.); Excited triplet state; Metastable state; Phosphorescence; Photochemistry (of butadiyne; implications of the intrinsic lifetime of metastable excited C4H2 for the photochem. of C4H2 in Titan's atm.) planet Saturn Titan atm methane metastable excited butadiyne photochem},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Vuitton09a,
  Title                    = {Negative ion chemistry in {T}itan's upper atmosphere},
  Author                   = {V. Vuitton and P. Lavvas and R.V. Yelle and M. Galand and A. Wellbrock and G.R. Lewis and A.J. Coates and J.-E. Wahlund},
  Journal                  = {Planetary and Space Science},
  Year                     = {2009},
  Pages                    = { - },
  Volume                   = {In Press, Corrected Proof},

  Abstract                 = {The Electron Spectrometer (ELS), one of the sensors making up the Cassini Plasma Spectrometer (CAPS) revealed the existence of numerous negative ions in Titan's upper atmosphere. The observations at closest approach (~1000 km) show evidence for negatively charged ions up to ~10,000 amu/q, as well as two distinct peaks at 22±4 and 44±8 amu/q, and maybe a third one at 82±14 amu/q. We present the first ionospheric model of Titan including negative ion chemistry. We find that dissociative electron attachment to neutral molecules (mostly HCN) initiates the formation of negative ions. The negative charge is then transferred to more acidic molecules such as HC3N, HC5N or C4H2. Loss occurs through associative detachment with radicals (H and CH3). We attribute the three low mass peaks observed by ELS to CN-, C3N-/C4H- and C5N-. These species are the first intermediates in the formation of the even larger negative ions observed by ELS, which are most likely the precursors to the aerosols observed at lower altitudes.},
  Doi                      = {DOI: 10.1016/j.pss.2009.04.004},
  File                     = {Vuitton2009.pdf:Vuitton2009.pdf:},
  ISSN                     = {0032-0633},
  Keywords                 = {Atmospheres},
  Owner                    = {plessis},
  Timestamp                = {2009.07.02},
  Url                      = {http://www.sciencedirect.com/science/article/B6V6T-4W3HX8B-3/2/e6dcadc76fba4aa64d126341a1428426}
}

