@Article{McKay01,
  Title                    = {Physical properties of the organic aerosols and clouds on Titan},
  Author                   = {McKay, C. P. and Coustenis, A. and Samuelson, R. E. and Lemmon, M. T. and Lorenz, R. D. and Cabane, M. and Rannou, P. and Drossart, P.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2001},
  Note                     = {CAN 134:313824 53-9 Mineralogical and Geological Chemistry NASA Ames Research Center,Moffett Field,CA,USA. Journal 0032-0633 written in English. 7732-18-5 (Water) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (ice; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); 74-82-8 (Methane); 74-84-0 (Ethane); 7440-44-0 (Carbon); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (in Titan's clouds; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan)},
  Number                   = {1},
  Pages                    = {79-99},
  Volume                   = {49},

  Abstract                 = {Titan's haze is optically thick in the visible, with an optical depth at 0.5 mm of about three. The haze varies with latitude in a seasonal cycle and has a detached upper layer. Microphys. models, photochem. models, and lab. simulations all imply that the prodn. rate of the haze is in the range of (0.5-2) * 10-14g cm-2s-1. Given the rate of sedimentation, the total mass loading is about 250 mg m-2. The transparency of the haze is high for wavelengths above 1 mm because the haze material becomes almost purely scattering and the optical depth decreases with increasing wavelength. The particles in the main haze deck are probably fractal in structure with an equiv. vol. radius of 0.2 mm. The haze material is org. and, if similar to lab. tholin, has a C/N ratio in the range of 2-4 and a C/H ratio of about unity. The haze significantly affects the thermal balance of Titan, causing an antigreenhouse effect that cools the surface by 9 K. Titan's faintly banded appearance suggests strong zonal winds in the lower stratosphere. Condensate clouds of ethane or methane, if present, are thin, patchy, or transient. Stratospheric clouds of condensed nitriles and (possibly) hydrocarbons appear to be assocd. with, though not contained entirely in, the polar shadow, suggesting abundances may vary with the season. Pptg. condensate particles from the stratosphere probably act as nucleating centers for the formation and rapid growth of methane ice particles in the troposphere, where the gas phase appears to be highly supersatd. Once formed, fallout times for these hailstones are .apprx.2 h or less. Melting, and possible subsequent fragmentation of methane raindrops, should occur at .apprx.12 km and below. Almost complete evapn. should occur just above the surface. A thin residue of ethane-enriched fog particles would then slowly settle to the surface, steadily modifying an existing surface or subsurface residue of liq. hydrocarbons. The optical properties of the haze in the 1 to 3 mm spectral region and the implications for the visibility of the surface are probably the most pressing current research questions. Other key questions include the nature of the high altitude detached haze layer, altitude and seasonal changes in compn. of the haze, the role of haze particles as condensation nuclei for clouds, and the nature of any condensate clouds. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Nucleation (cloud; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Hydrocarbons; Nitriles Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (in Titan's clouds; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Fractals; Optical properties; Physical properties; Troposphere (optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Aerosols (org.; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Fog (particles; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan); Organic compounds Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (tholis; optical properties, prodn. rate, and compn. of the org. aerosols and clouds on Saturn's satellite Titan) planet Saturn Titan org aerosol cloud haze optical property},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{McKay99,
  Title                    = {Analytic solutions for the antigreenhouse effect: Titan and the early Earth},
  Author                   = {McKay, Christopher P. and Lorenz, Ralph D. and Lunine, Jonathan I.},
  Journal                  = {Icarus},
  Year                     = {1999},
  Note                     = {CAN 130:255085 53-9 Mineralogical and Geological Chemistry Space Science Division,NASA Ames Research Center,Moffett Field,CA,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 7664-41-7 (Ammonia) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (analytic solns. for the antigreenhouse effect on Saturn's satellite Titan and the early Earth)},
  Number                   = {1},
  Pages                    = {56-61},
  Volume                   = {137},

  Abstract                 = {The antigreenhouse effect results from a high altitude haze which blocks sunlight while allowing transmission of thermal IR. An antigreenhouse is obsd. on Titan and has been proposed for the early Earth. Here, we derive an analytic soln. useful for computing the atm. and surface temp. with both the greenhouse and antigreenhouse effects included. In agreement with detailed calcns., the analytic soln. predicts that a runaway greenhouse on early Titan caused by accretional heat would collapse when the flux fell to 0.9 W m-2. For the present value of the solar luminosity a runaway greenhouse collapses at a geothermal heat flux of 0.4 W m-2. We show that the antigreenhouse effect may compromise attempts to solve the faint young sun problem on the early Earth with a methane-ammonia greenhouse shielded by an org. haze, more than doubling the IR opacity needed to warm the early Earth compared to the case with no antigreenhouse. (c) 1999 Academic Press. [on SciFinder (R)]},
  Keywords                 = {Air pollution; Atmosphere; Earth; Geological heat flow; Sun; Titan (analytic solns. for the antigreenhouse effect on Saturn's satellite Titan and the early Earth); Greenhouses (anti-; analytic solns. for the antigreenhouse effect on Saturn's satellite Titan and the early Earth); Climate (greenhouse effect; analytic solns. for the antigreenhouse effect on Saturn's satellite Titan and the early Earth) planet Saturn Titan satellite Earth antigreenhouse effect},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{McKay97,
  Title                    = {Temperature lapse rate and methane in Titan's troposphere},
  Author                   = {McKay, Christopher P. and Martin, Shuleen Chau and Griffith, Caitlin A. and Keller, Richard M.},
  Journal                  = {Icarus},
  Year                     = {1997},
  Note                     = {CAN 128:77860 53-9 Mineralogical and Geological Chemistry Space Science Division,NASA Ames Research Center,Moffett Field,CA,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (temp. lapse rate and methane in Titan's troposphere)},
  Number                   = {2},
  Pages                    = {498-505},
  Volume                   = {129},

  Abstract                 = {We have reanalyzed the Voyager radio occultation data for Titan, examg. two alternative approaches to methane condensation. In one approach, methane condensation is facilitated by the presence of nitrogen because nitrogen lowers the condensation level of a methane/nitrogen mixt. The resulting enhancement in methane condensation lowers the upper limit on surface relative humidity of methane obtained from the Voyager occultation data from 0.7 to 0.6. In this case the surface relative humidity of methane lies between 0.08 and 0.6, with values close to 0.6 indicated. In the other approach, methane is allowed to become supersatd. and reaches 1.4 times satn. in the troposphere. In this case, surface humidities up to 100% are allowed by the Voyager occultation data, and thus the upper limit must be set by other considerations. If supersatn. is included, then the surface relative humidity of methane can be any value greater than 0.08-unless a deep ocean is present, in which case the surface relative humidity is limited to less than 0.85. Again, values close to 0.6 are indicated. Overall, the tropospheric lapse rate on Titan appears to be detd. by radiative equil. The lapse rate is everywhere stable against dry convection, but is unstable to moist convection. This finding is consistent with a supersatd. atm. in which condensation-and hence moist convection-is inhibited. [on SciFinder (R)]},
  Keywords                 = {Titan (temp. lapse rate and methane in Titan's troposphere) Titan troposphere temp lapse rate methane},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

