@Article{Molina-Cuberos99,
  Title                    = {Chemistry of the galactic cosmic ray induced ionosphere of Titan},
  Author                   = {Molina-Cuberos, G. J. and Lopez-Moreno, J. J. and Rodrigo, R. and Lara, L. M.},
  Journal                  = {Journal of Geophysical Research, [Planets]},
  Year                     = {1999},
  Number                   = {E9},
  Pages                    = {21997-22024},
  Volume                   = {104},

  Abstract                 = {Titan's lower ionosphere (from 1 to 400 km) has been studied with a one-dimensional ion-neutral model. In this region of the atm., galactic cosmic rays (GCRs) are the main ionization source. They penetrate to the deeper atm. and ionize the neutral constituents of Titan's atm. (mainly N2, CH4, Ar, H2, and CO) to produce N2+, N+, Ar+, CH4+, CH3+, CH2+, H2+, H+, and CO+. Fast reactions with the neutrals convert these ions into ions such as CH5+, C2H5+, and N2H+. Different pathways are proposed to obtain the ion and electron densities. The most abundant ions are cluster ions, like CH5+.CH4, HCO+.H2, and HCNH+.C2H4, and long chain hydrocarbon ions. In atmospheres very rich in N2, such as Titan's, ions like H4C7N+ and CH3CNH+ also represent an important contribution to the total pos. ion d. Three-body reactions may play an important role in the dense atm. of Titan, and special attention is devoted to them. The calcd. electron d. in the lower atm. reaches a peak of ~ 2150 cm-3 at an altitude of 90 km. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; chem. of the galactic cosmic ray induced ionosphere of Titan); Galactic cosmic ray (chem. of the galactic cosmic ray induced ionosphere of Titan) galactic cosmic ray ionosphere Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

