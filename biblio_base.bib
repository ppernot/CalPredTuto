  Abstract                 = {Life on earth was supposed to be born about 3.8 billion years ago in primordial sea, using org. compds. dissolved in seawater. Various kinds of expts. have been performed to examine how these org. compds. were abiotically synthesized in simulated primitive terrestrial and extraterrestrial environments. Miller found that amino acids were easily formed by spark discharges in a mixt. of methane, ammonia, hydrogen and water. Nowaday, however, the primitive earth atm. is believed to be less reducing, like a mixt. of carbon dioxide, carbon monoxide, nitrogen and water. It has been difficult to form org. compds. from this kind of gas mixt. with conventional energies. The authors found that amino acids could be formed from this gas mixt. if high energy protons (major constituents of cosmic rays) were used as an energy source. Hydrothermal vent environments are another possible site for abiotic formation of bioorg. compds. Org. compds. have been found in extraterrestrial environments, such as in Jupiter, Titan and comets. Amino acid precursors were formed when mixts. simulating Jovian atm., Titan atm., or cometary nuclei (aggregates of interstellar dust particles with ice mantles) were irradiated with high energy protons. Org. compds. carried by comets may have been essential sources for the first organisms on primitive earth. Planetary missions are quite important to see how chem. evolution goes in prebiotic environments. Mars is the first place out of the earth to examine the presence of living organisms and/or bioor. compds. [on SciFinder (R)]},
  Keywords                 = {Life (abiotic formation of bioorg. compds. and origins of life); Organic compounds Role: BOC (Biological occurrence), BPN (Biosynthetic preparation), BPR (Biological process), BSU (Biological study, unclassified), RCT (Reactant), SPN (Synthetic preparation), BIOL (Biological study), OCCU (Occurrence), PREP (Preparation), PROC (Process), RACT (Reactant or reagent) (biol., abiotic formation of bioorg. compds. and origins of life); Biochemistry (org., abiotic formation of bioorg. compds. and origins of life); Evolution (prebiotic, abiotic formation of bioorg. compds. and origins of life) bioorg compd abiotic formation life evolution},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Kobayashi97,
  Title                    = {Abiotic synthesis of bioorganic compounds in simulated primitive planetary environments},
  Author                   = {Kobayashi, Kensei and Kaneko, Takeo and Ponnamperuma, Cyril and Oshima, Tairo and Yanagawa, Hiroshi and Saito, Takeshi},
  Journal                  = {Nippon Kagaku Kaishi},
  Year                     = {1997},
  Note                     = {CAN 128:112129 6-7 General Biochemistry Dep. Chem., Fac. Eng.,Yokohama National Univ.,Yokohama,Japan. Journal 0369-4577 written in Japanese. 71-30-7 (Cytosine) Role: PEP (Physical, engineering or chemical process), PROC (Process) (abiotic synthesis of bioorg. compds. in simulated primitive planetary environments)},
  Number                   = {12},
  Pages                    = {823-834},

  Abstract                 = {Terrestrial organisms were supposed to be abiogenetically born in primordial sea about 4 billion years ago, using org. compds. in seawater. We performed expts. simulating the environments of primitive planets or interstellar dusts to examine possible formation of bioorg. compds. essential for the first organisms. When a gas mixt. of Titan-type, which contained methane and nitrogen, was exposed to a spark discharge, such org. compds. as hydrocarbons, \"amino acid precursors,\" and some nucleic acid bases like cytosine were formed: The amino acid precursors are compds. which give amino acids after acid hydrolysis. If a gas mixt. of carbon monoxide, nitrogen and water (\"mildly-reduced gas mixt.\") was used, spark discharges gave only trace amt. of the amino acid precursors. The same type of gas mixt. was irradiated with high energy particles, the amino acid precursors were formed in high yield, together with uracil and imidazole. Some of the amino acid precursors had mol. wt. of several hundreds, thus they might be condensates of amino acids. Amino acid precursors were also formed when a simulated ice mantle (10 K) of interstellar dust particles was irradiated with high energy particles. These results suggest that (i) cosmic ray-induced formation of bioorg. compds. was important if the primitive terrestrial atm. was mildly reduced, (ii) exogenous org. compds. delivered by bodies like comets could not be negligible, particularly if the primitive terrestrial atm. was non-reduced, and (iii) further simulation studies on abiotic syntheses of bioorg. compds. will be required to evaluate \"the RNA-world hypothesis\" and \"the Oligomer-world hypothesis\". [on SciFinder (R)]},
  Keywords                 = {Interstellar dust (abiotic synthesis of bioorg. compds. in simulated primitive planetary environments); Amino acids; Nucleic acid bases; Organic compounds Role: PEP (Physical, engineering or chemical process), PROC (Process) (abiotic synthesis of bioorg. compds. in simulated primitive planetary environments); Evolution (mol.; abiotic synthesis of bioorg. compds. in simulated primitive planetary environments) evolution interstellar dust bioorg compd formation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Kobayashi94,
  Title                    = {Formation of bio-organic compounds in planetary atmospheres by cosmic radiation},
