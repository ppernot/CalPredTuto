@Article{Schulze-Makuch02,
  Title                    = {Search parameters for the remote detection of extraterrestrial life},
  Author                   = {Schulze-Makuch, Dirk and Irwin, Louis N. and Guan, Huade},
  Journal                  = {Planetary and Space Science},
  Year                     = {2002},
  Note                     = {CAN 138:34711 6-0 General Biochemistry Department of Geological Sciences,University of Texas at El Paso,El Paso,TX,USA. Journal; General Review 0032-0633 written in English. 7704-34-9 (Sulfur) Role: BSU (Biological study, unclassified), BIOL (Biological study) (gases; search parameters for the remote detection of extraterrestrial life); 74-82-8 (Methane); 74-87-3 (Methyl chloride); 74-88-4 (Methyl iodide); 1333-74-0 (Hydrogen); 7664-41-7 (Ammonia); 7782-44-7 (Oxygen) Role: BSU (Biological study, unclassified), BIOL (Biological study) (search parameters for the remote detection of extraterrestrial life)},
  Number                   = {7-8},
  Pages                    = {675-683},
  Volume                   = {50},

  Abstract                 = {A review. Direct consequences of biol. activity (biosignatures) and alterations of the geol. environment due to biol. processes (geosignatures) are currently known only for the planet Earth. However, geoindicators remotely detectable by robotic technol. have revealed a no. of sites in the solar system where conditions compatible with the support of life may exist. By focusing on a search for energy gradients, complex chem., liqs. that may act as solvents, atmospheres, and indicators of geol. differentiation, robotic exploration of the solar system and beyond should lead to fruitful targets in the search for extraterrestrial life. An anal. of all major solar system bodies for these parameters suggests that Mars, Titan, and the Galilean satellites should be given the highest priority in the search for extraterrestrial life in our solar system. Extending them to other bodies in the solar system, however, draws attention to Io, Triton, Titania, Enceladus, and Iapetus, among others, as worthy of greater attention. [on SciFinder (R)]},
  Keywords                 = {Satellites (Enceladus; search parameters for the remote detection of extraterrestrial life); Satellites (Galilean; search parameters for the remote detection of extraterrestrial life); Satellites (Iapetus; search parameters for the remote detection of extraterrestrial life); Jupiter (Io satellite; search parameters for the remote detection of extraterrestrial life); Satellites (Titania; search parameters for the remote detection of extraterrestrial life); Neptune (Triton satellite; search parameters for the remote detection of extraterrestrial life); Satellites (Triton; search parameters for the remote detection of extraterrestrial life); Life (extraterrestrial; search parameters for the remote detection of extraterrestrial life); Energy (gradients; search parameters for the remote detection of extraterrestrial life); Robotics (robot; search parameters for the remote detection of extraterrestrial life); Atmosphere; Mars; Planets; Solar system; Solvents; Titan; Universe (search parameters for the remote detection of extraterrestrial life); Chlorophylls Role: BSU (Biological study, unclassified), BIOL (Biological study) (search parameters for the remote detection of extraterrestrial life) review extraterrestrial life robot detection},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

