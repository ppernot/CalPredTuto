@Unpublished{Null08,
  Title                    = {The Nested {D}irichlet distribution: properties and applications},
  Author                   = {Null, Brad},
  Note                     = {Workin paper, Department of Management Science and Engineering, Stanford University.},

  Month                    = {october},
  Year                     = {2008},

  Abstract                 = {In this paper we present a method of nesting Dirichlet distributions, which ultimately gen-
eralizes the Generalized Dirichlet distribution. We denote the resulting underlying distribution
a nested Dirichlet distribution. We begin by providing some basic intuition for understanding
the distribution as well as presenting some useful notation, the joint probability distribution
function, and formulae for several properties of the distribution, such as the covariance matrix
for all variables. We also discuss the close relationship of the distribution with the Generalized
Dirichlet, show how to ¯t the distribution given multinomial data using Maximum Likelihood
Estimators, and present an analysis of the accuracy and bias of these estimators using Cramer-
Rao bounds.
Subsequently we evaluate a data mining method to estimate the nesting structure of this
distribution using simulated multinomial data. In our example, this method generally discovers
the correct nesting structure. Further, forecasting using this ¯tted distribution (given data
simulated from a nested Dirichlet underlying distribution) improve predictive power over the
Dirichlet twice as much as the Generalized Dirichlet does.},
  File                     = {Null2008.pdf:Null2008.pdf:},
  Owner                    = {plessis},
  Timestamp                = {2009.10.02}
}

