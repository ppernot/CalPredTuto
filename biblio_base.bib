@Article{Cook2001,
  Title                    = {Vacuum ultraviolet photochemistry of methane, silane and germane},
  Author                   = {Cook, P. A. and Ashfold, M. N. R. and Jee, Y. J. and Jung, K. H. and Harich, S. and Yang, X. M.},
  Journal                  = {Physical Chemistry Chemical Physics},
  Year                     = {2001},
  Number                   = {{10}},
  Pages                    = {1848-1860},
  Volume                   = {3},

  Abstract                 = {{The photochemistry of jet-cooled CH4, SiH4 and GeH4 molecules following excitation at the Lyman-alpha wavelength (121.6 nm) has been investigated by high resolution photofragment translational spectroscopy methods. Complementary ab initio calculations of selected portions of the potential energy surfaces for the various components of the T-1(2) and T-3(2) excited states arising from the 3sa(1)<-- 1t(2) electron promotion are presented in the case of CH4. The form of the H atom recoil velocity distribution arising in the 121.6 nm photolysis of CH4 is rationalised in terms of initial excitation to both the 2(1)A' and 1(1)A{''} excited states (Jahn-Teller components of the degenerate T-1(2) state), followed by a range of decay mechanisms. CH4(2(1)A') molecules can decay adiabatically, via sequential extension of first one, then a second, C-H bond with eventual formation of two H atoms and CH2((a) over tilde (1)A(1)) products, or after internal conversion (IC) to the ground state. The H+CH3((X) over tilde) products resulting from the IC process display a recoil velocity distribution characterised by an anisotropy parameter beta similar to +2, implying that the fragmentation involves irreversible extension of the C-H bond along which the transition dipole points at the instant of photon absorption. Fragmentation of CH4(1(1)A{''}) molecules to H+CH3((X) over tilde) products proceeds via intersystem crossing to the lowest (3)A' potential energy surface. The recoil anisotropy of these products (beta similar to -0.45) implies that this radiationless process also occurs on a timescale that is rapid compared to the parent rotational period. Both single H-C bond fission channels may yield CH3((X) over tilde) products with such high levels of internal excitation that they are unstable with respect to further unimolecular decay; any H atoms that result from this secondary decay must contribute to the observed yield of slow H atoms with beta similar to0. All H atoms resulting from Lyman-alpha photolysis of both SiH4 and GeH4 have (low) kinetic energies and little or no recoil anisotropy, compatible with their being formed via three body fragmentation to, primarily, H+H+SiH2/GeH2((X) over tilde (1)A(1)) products. Faster H atoms are evident in the total kinetic energy release (TKER) spectra obtained following 157.6 nm photoexcitation of SiH4, but the power dependence of this fast H atom signal implies that these arise as a result of a two photon process involving initial formation of SiH2+H-2 products and subsequent photolysis of the nascent silylene fragments.}},
  ISSN                     = {{1463-9076}},
  Owner                    = {ber},
  Timestamp                = {2010.07.11},
  Unique-id                = {{ISI:000168392700009}}
}

@Article{Cottin01,
  Title                    = {Experimental and theoretical studies on the gas/solid/gas transformation cycle in extraterrestrial environments},
  Author                   = {Cottin, Herve and Gazeau, Marie-Claire and Chaquin, Patrick and Raulin, Francois and Benilan, Yves},
  Journal                  = {Journal of Geophysical Research, [Planets]},
  Year                     = {2001},
  Number                   = {E12},
  Pages                    = {33325-33332},
  Volume                   = {106},

  Abstract                 = {The ubiquity of mol. material in the universe, from hydrogen to complex org. matter, is the result of intermixed physicochem. processes that have occurred throughout history. In particular, the gas/solid/gas phase transformation cycle plays a key role in chem. evolution of org. matter from the interstellar medium to planetary systems. This paper focuses on two examples that are representative of the diversity of environments where such transformations occur in the Solar System: (1) the photolytic evolution from gaseous to solid material in methane contg. planetary atmospheres and (2) the degrdn. of high mol. wt. compds. into gas phase mols. in comets. We are currently developing two programs which couple exptl. and theor. studies. The aim of this research is to provide data necessary to build models in order to better understand (1) the photochem. evolution of Titan's atm., through a lab. program to det. quant. spectroscopic data on long carbon chain mols. (polyynes) obtained in the SCOOP program (French acronym for Spectroscopy of Org. Compds. Oriented for Planetol.), and (2) the extended sources in comets, through a lab. program of quant. studies of photochem. and thermal degrdn. processes on relevant polymers (e.g., Polyoxymethylene) by the SEMAPhOrE Cometaire program (French acronym for Exptl. Simulation and Modeling Applied to Org. Chem. in Cometary Environment). We report here studies of polyoxymethylene degrdn. that suggest this is a plausible explanation for the formaldehyde extended sources obsd. in several comets. [on SciFinder (R)]},
  Keywords                 = {Comets (photo- and thermo-degrdn. of polyoxymethylene could generate the formaldehyde obsd. in cometary comae); Titan (simulating photochem. evolution of methane in Titan' atm.) comet formadehyde decompn polyoxymethylene; Titan atm methane photochem},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

