@Article{Bakes02,
  Title                    = {Photoelectric charging of submicron aerosols and macromolecules in the Titan haze},
  Author                   = {Bakes, E. L. O. and McKay, Christopher P. and Bauschlicher, Charles W., Jr.},
  Journal                  = {Icarus},
  Year                     = {2002},
  Number                   = {2},
  Pages                    = {464-475},
  Volume                   = {157},

  Abstract                 = {We quantify the charge states of submicrometer aerosols and arom. macromols. in Titan's org. haze. The aerosol charge is balanced between the recombination of pos. ions with the aerosol plus the ejection of electrons from the aerosol via the UV-driven photoelec. effect and the recombination of electrons with the aerosol. During the day, the dominant charge state for submicrometer aerosols is pos. Macromols. composed of fewer than 32 carbon atoms with low electron affinities (<1.0 eV) are neutral, while the rest are mainly neutral and neg. charged with a small fraction (.apprx.10%) becoming pos. charged at higher (>=300 km) altitudes. At night, Titan's aerosol population becomes uniformly neutral and neg. charged. The time taken for a nighttime aerosol to change from being neg. charged to its most probable daytime pos. charge is on the order of a few seconds for the largest submicrometer aerosols, while macromols. tend to persist in an anionic charge state for one to several Earth days. Charging strongly influences aerosol agglomeration via Coulomb attraction and may account for the seasonal variations in the albedo of the Titan haze at midrange (.apprx.200-250 km) altitudes. Enhanced agglomeration may also efficiently produce a source of condensation nuclei for the daily rainout of methane. In addn., the difference in aerosol charge between Titan's day and night (or summer and winter) phases will produce dramatically different chemistries which must be accounted for in future photochem. models. Finally, if there are PAH-like macromols. in the Titan haze, Cassini Huygens should be able to observe these charge differences, with neutral macromols. emitting strongly at 3.3 and 11.2 mm, cationic macromols. emitting between 6.2 and 8.6 mm, and anionic macromols. emitting in both IR spectral regions. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; effect of aerosol agglomeration via Coulomb attractions on the photoelec. charging of submicron aerosols and macromols. in the haze of Titan satellite); Nucleation (cloud; effect of aerosol agglomeration via Coulomb attractions on the photoelec. charging of submicron aerosols and macromols. in the haze of Titan satellite); Albedo; Atmospheric aerosols; Titan (effect of aerosol agglomeration via Coulomb attractions on the photoelec. charging of submicron aerosols and macromols. in the haze of Titan satellite); Agglomeration (of aerosol; effect of aerosol agglomeration via Coulomb attractions on the photoelec. charging of submicron aerosols and macromols. in the haze of Titan satellite) Titan satellite atm submicron aerosol macromol photoelec charge state},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

