@Article{Singh96,
  Title                    = {Absorption coefficients for the 727 nm band of methane at 77 K determined by intracavity laser spectroscopy},
  Author                   = {Singh, Kuldip and O'Brien, James J.},
  Journal                  = {Astrophysics and Space Science},
  Year                     = {1996},
  Note                     = {CAN 125:201163 53-9 Mineralogical and Geological Chemistry Department Chemistry,University Missouri,St. Louis,MO,USA. Journal 0004-640X written in English. 74-82-8 (Methane) Role: PRP (Properties) (absorption coeffs. for the 727 nm band of methane at 77 K detd. by intracavity laser spectroscopy for use in interpretation of spectral data of outer planets and satellites)},
  Number                   = {1},
  Pages                    = {97-109},
  Volume                   = {236},

  Abstract                 = {Methane spectral features in the visible to near-IR region are prominent in the spectra of the outer planets but lab. data for the appropriate methane conditions are required to interpret the observational data. By use of the intracavity laser spectroscopy technique, a moderately high resoln. (>= 500,000) absorption spectrum of the 727 nm band of methane at 77 K is obtained. The methane absorption bands in the visible to near-IR region are very weak, but intracavity laser spectroscopy provides sufficient sensitivity to perform the measurements and to ext. quant. data for methane at low temps. Absorption coeffs. are detd. and are reported as avs. at one .ANG. intervals throughout the region 7127-7420 .ANG.. By integrating over the band, an intensity of 753 cm-1 km-1 am-1 is obtained. The results compare well with previous low resoln. measurements on methane at room temp., with gas phase results calcd. using the absorption spectrum of liq. methane, and with absorption coeffs. derived from methane features obsd. in the spectra of the outer planets and Titan. [on SciFinder (R)]},
