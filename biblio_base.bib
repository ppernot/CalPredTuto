@Article{Pietrogrande01,
  Title                    = {Analysis of complex mixtures recovered from space missions. Statistical approach to the study of Titan atmosphere analogues (tholins)},
  Author                   = {Pietrogrande, M. C. and Coll, P. and Sternberg, R. and Szopa, C. and Navarro-Gonzalez, R. and Vidal-Madjar, C. and Dondi, F.},
  Journal                  = {Journal of Chromatography, A},
  Year                     = {2001},
  Note                     = {CAN 136:234979 53-9 Mineralogical and Geological Chemistry Department of Chemistry,University of Ferrara,Ferrara,Italy. Journal 0021-9673 written in English. 60-35-5 (Acetamide); 71-43-2 (Benzene); 74-82-8 (Methane); 74-84-0 (Ethane); 74-85-1 (Ethene); 74-90-8 (Hydrocyanic acid); 74-99-7 (Propine); 75-05-8 (Acetonitrile); 75-12-7 (Formamide); 75-28-5 (2-Methylpropane); 78-78-4 (2-Methylbutane); 78-82-0 (2-Methyl-propanenitrile); 78-93-3 (Butanone); 100-42-5 (Styrene); 100-47-0 (Benzonitrile); 106-98-9 (1-Butene); 106-99-0 (Butadiene); 107-01-7 (2-Butene); 107-12-0 (Propanenitrile); 107-13-1 (2-Propenenitrile); 108-88-3 (Toluene); 109-68-2 (2-Pentene); 109-75-1 (3-Butenenitrile); 109-97-7 (1H-Pyrrole); 110-86-1 (Pyridine); 115-07-1 (1-Propene); 124-38-9 (Carbon dioxide); 126-98-7 (2-Methylpropenenitrile); 289-95-2 (Pyrimidine); 460-19-5 (Ethanedinitrile); 463-49-0 (Propadiene); 542-92-7 (1,3-Cyclopentadiene); 591-95-7 (1,2-Pentadiene); 591-96-8 (2,3-Pentadiene); 3438-46-8 (4-Methylpyrimidine); 5053-43-0 (2-Methylpyrimidine); 7664-41-7 (Ammonia); 7732-18-5 (Water); 208666-46-0 (Butyne) Role: ANT (Analyte), ANST (Analytical study) (statistical study of Titan atm. tholins using GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation)},
  Number                   = {1-2},
  Pages                    = {69-77},
  Volume                   = {939},

  Abstract                 = {To study Titan, the largest moon of Saturn, lab. simulation expts. have been performed to obtain analogs of Titan's aerosols (named tholins) using different energy sources. Tholins, which have been demonstrated to represent aerosols in Titan's haze layers, are a complex mixt. resulting from the chem. evolution of several hydrocarbons and nitriles. Chromatog. anal. yields complex chromatograms which require the use of math. procedures to ext. all the information contained therein. Two different chemometric approaches (the Fourier anal. approach and a statistical model of peak overlapping) were successfully applied to the pyrolysis-GC-MS chromatogram of a tholin sample. Fundamental information on the mixt.'s chem. compn. (no. of components, m) and on the sepn. system performance (sepn. efficiency, s) can be easily estd.; the excellent correspondence between the data calcd. by the two independent procedures proves the reliability of the statistical approaches in characterizing a tholin chromatogram. Moreover, the plot of autocorrelation function contains, in a simplified form, all the information on the retention pattern; retention recursivities can be easily singled out and related to specific mol. structure variations. Therefore, the autocorrelation function (ACF) plot constitutes a simplified fingerprint of the pyrolysis products of tholins, which can be used as a powerful tool to characterize a tholin sample. [on SciFinder (R)]},
  Keywords                 = {Titan (atm., aerosol; statistical study of Titan atm. tholins using GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation); Statistical analysis (in study of Titan atm. tholins with use of GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation); Atmospheric aerosols (on Titan satellite; statistical study of Titan atm. tholins using GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation); Air analysis; Simulation and Modeling (statistical study of Titan atm. tholins using GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation); Organic compounds Role: ANT (Analyte), ANST (Analytical study) (tholins; statistical study of Titan atm. tholins using GC-mass spectrometry chromatogram of pyrolized tholin samples from lab. simulation) tholin atm aerosol Titan},
