@Article{Loveday03,
  Title                    = {High-pressure neutron diffraction and models of Titan},
  Author                   = {Loveday, J. S. and Nelmes, R. J.},
  Journal                  = {High Pressure Research},
  Year                     = {2003},
  Note                     = {CAN 141:382332 53-9 Mineralogical and Geological Chemistry School of Physics and Centre for Science at Extreme Conditions,University of Edinburgh,Edinburgh,UK. Journal 0895-7959 written in English. 14476-19-8 (Methane hydrate); 178115-93-0 (Ammonia monohydrate) Role: PRP (Properties) (high-pressure neutron diffraction study of ammonia monohydrate and methane hydrate for modeling of Titan)},
