@Article{Fura2002,
  Title                    = {Small carbon clusters ({C}$_n^0$, {C}$_n^+$, {C}$_n^-$) from acyclic and cyclic precursors: Neutralization-reionization and theory},
  Author                   = {Aberra Fura and Frantisek Turecek and Fred W. McLafferty},
