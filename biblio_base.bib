@Article{Trafton74,
  Title                    = {Titan. Unidentified strong absorptions in the photometric infrared},
  Author                   = {Trafton, L.},
  Journal                  = {Icarus},
  Year                     = {1974},
  Note                     = {CAN 80:139315 73-7 Spectra by Absorption, Emission, Reflection, or Magnetic Resonance, and Other Optical Properties McDonald Obs.,Univ. Texas,Austin,TX,USA. Journal 0019-1035 written in English. 74-82-8 Role: PRP (Properties) (spectral bands of, in ir spectrum of atm. of planet Saturn's satellite Titan)},
  Number                   = {2},
  Pages                    = {175-87},
  Volume                   = {21},

  Abstract                 = {The morphol. of Titan's ir spectral features reveals a pattern of differences with respect to Saturn's bands. Titan's bands exhibit a progressive washing out with increasing wavelength relative to the corresponding bands in Saturn's spectrum. Unidentified absorption features of apparently gaseous origin appear in the long-wavelength wing of Titan's 1-m CH4 band complex with considerably greater strength. Greater absorption is also obsd. in the 1.1 m region. Not all of the phenomena can be explained by enhanced CH4 absorption, so Titan's spectrum implies the existence of another spectroscopically active gas. Several candidates for this gas are briefly considered. In a no. of ways, Titan's far ir spectral features approx. those in the spectrum of Uranus more closely than those in the spectrum of Saturn. This is further support for the concept of an optically deep atm. for Titan. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, ir spectrum of Titan satellite of) Titan IR atm methane; Saturn Titan atm IR},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Trafton72,
  Title                    = {Bulk composition of Titan's atmosphere},
  Author                   = {Trafton, L.},
  Journal                  = {Astrophysical Journal},
  Year                     = {1972},
  Note                     = {CAN 77:82028 73-7 Spectra by Absorption, Emission, Reflection, or Magnetic Resonance, and Other Optical Properties McDonald Obs.,Univ. Texas,Fort Davis,TX,USA. Journal 0004-637X written in English. 74-82-8 Role: OCCU (Occurrence) (in planet Saturn Titan satellite)},
  Number                   = {1)(Pt. 1},
  Pages                    = {295-306},
  Volume                   = {175},

  Abstract                 = {The CH4 bands in Titan's spectrum are satd. and consequently, either the CH4 abundance is nearly an order of magnitude greater than is generally believed or else CH4 is a minor constituent in this atm. Consideration of the phys. constraints for Titan's atm. leads to a model which describes the bulk compn. in terms of observable parameters. Intermediate-resoln. photometric scans of both Saturn and Titan, including scans of the Q-branch of Titan's 3n3 CH4 band, constrain these parameters in such a way that the model indicates the presence of another important atm. gas, namely, another bulk constituent or a significant thermal opacity. The simplest interpretations of the published observations are not all compatible; further progress in detg. the compn. and state of Titan's atm. requires addnl. observations to eliminate the present ambiguities. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, compn. of atm. of Titan satellite of) Titan atm compn methane; planet Saturn satellite compn},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

