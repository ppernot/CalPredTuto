@Article{Tanguy90a,
  Title                    = {Stratospheric profile of HCN on Titan from millimeter observations},
  Author                   = {Tanguy, Loic and Bezard, Bruno and Marten, Andre and Gautier, Daniel and Gerard, Eric and Paubert, Gabriel and Lecacheux, Alain},
