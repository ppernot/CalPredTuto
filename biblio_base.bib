@Article{Hunt83,
  Title                    = {Collision-induced absorption in the far infrared spectrum of Titan},
  Author                   = {Hunt, J. L. and Poll, J. D. and Goorvitch, D. and Tipping, R. H.},
  Journal                  = {Icarus},
  Year                     = {1983},
  Note                     = {CAN 99:79620 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Dep. Phys.,Univ. Guelph,Guelph,ON,Can. Journal 0019-1035 written in English. 74-82-8; 7727-37-9 Role: PRP (Properties) (collision-induced IR spectrum of gas mixt. contg., for Saturn satellite Titan)},
  Number                   = {1},
  Pages                    = {63-72},
  Volume                   = {55},

  Abstract                 = {The effects of collision-induced absorption on the far-IR spectrum of Titan were investigated. Results for the temp. range of 70 to 120 K are reported. These are used as input data for a simple atm. model to compute the far IR radiance, brightness temp., and spectral limb function. This source of opacity alone is not capable of explaining the Voyager results. When the collision-induced CH4 is included, the results are in closer agreement in the range between 200 and 300 cm-1, suggesting that a more complete treatment of collision-induced absorption including particularly CH4-N2, N2-H2, and H2-H2 results, may provide sufficient opacity to reduce or obviate the need for opacities due to clouds or aerosols in order to explain the obsd. spectra. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, collision-induced far-IR spectrum of Titan satellite of) collision induced IR Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@TechReport{Hunten78,
  Title                    = {A Titan atmosphere with a surface temperature of 200 K},
  Author                   = {Hunten, Donald M.},
  Institution              = {Dep. Planetary Sci. Lunar,Univ. Arizona,Tucson,AZ,USA.},
  Year                     = {1978},
  Note                     = {CAN 92:45108 53-9 Mineralogical and Geological Chemistry Report 0191-7811 written in English. 74-82-8; 1333-74-0; 7727-37-9 Role: OCCU (Occurrence) (in Titan atm.)},

  Abstract                 = {The .apprx.200 K brightness temp. detd. at 3 mm wavelength by Ulich, B. L., et al. (1978), suggests an atm. model for Titan with a 21-bar surface pressure and CH4 clouds at 100-120 km altitude. The visual limb is .apprx.200 km. The CH4 mixing ratio is 0.25% above the clouds and 7% below; the dominant gas is assumed to be N2. Thermal opacity results from pressure-induced absorption in N2, H2 (0.5%) and some cloud particles; unit opacity is reached at 600 millibars, 110 km from the surface. The radius of the solid body in this model is 2700 km, in reasonable agreement with 2600 km obtained if the d. is the same as that of Ganymede and Callisto. Deeper atmospheres can be obtained if the temp. gradient is subadiabatic or if greater CH4 abundance is assumed. [on SciFinder (R)]},
  Keywords                 = {Simulation model (of Titan atm.); Planets (Saturn, Titan, atm. of, high-d. model of) methane Titan atm model; hydrogen Titan atm model; nitrogen Titan atm model},
