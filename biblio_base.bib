@Article{Stansberry89,
  Title                    = {Upper limits on possible photochemical hazes on Pluto},
  Author                   = {Stansberry, John A. and Lunine, Jonathan I. and Tomasko, Martin G.},
  Journal                  = {Geophysical Research Letters},
  Year                     = {1989},
  Note                     = {CAN 114:66318 53-9 Mineralogical and Geological Chemistry Dep. Planet. Sci.,Univ. Arizona,Tucson,AZ,USA. Journal 0094-8276 written in English. 74-82-8 (Methane) Role: RCT (Reactant), RACT (Reactant or reagent) (photolysis of, in atm. of Pluto, formation of haze layer in relation to)},
  Number                   = {11},
  Pages                    = {1221-4},
  Volume                   = {16},

  Abstract                 = {The possibility that aerosols generated through CH4 photolysis could be responsible for the haze layer on Pluto (Elliot, J. L. et al., 1989) is examd. A comprehensive model of aerosol prodn., particle growth, sedimentation and condensation is applied to the atm. of Pluto using pressures, temps., and compn. derived from the stellar occultation and other data. Two atm. models proposed in the literature, one from Elliot et al. [1989], and one from W. B. Hubbard et al. [1989], as well as a range of optical properties for the particles were tested. To produce a haze with unity tangential optical depth at 1174 km, an aerosol mass prodn. rate equal to twice the total CH4 dissocn. rate due to solar UV expected for Pluto was used with an assumption that the particles produced were 10 times more absorbing than those in other hazes in the outer solar system. The possibility of condensation in the lower atm. was considered but did not result in distinctly different haze optical depths. If a photochem. haze on Pluto was responsible for the occultation light curve measured by Elliot et al., operation of a photochem. system different from those on Titan, Uranus, or Neptune is indicated. Alternatives include near-surface condensation processes driven by circulation, and the possibility that the occultation light curve can be explained in its entirety by temp. effects as proposed by W.B. Hubbard et al. [1989]. [on SciFinder (R)]},
  Keywords                 = {Planets (Pluto, atm. of, photochem. hazes in, aerosol formation from methane photolysis in relation to, modeling of) Pluto atm haze methane aerosol},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

