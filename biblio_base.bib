@Article{Chang79,
  Title                    = {Organic chemistry on Titan},
  Author                   = {Chang, Sherwood and Scattergood, Thomas and Aronowitz, Sheldon and Flores, Jose},
  Journal                  = {Reviews of Geophysics and Space Physics},
  Year                     = {1979},
  Number                   = {8},
  Pages                    = {1923-33},
  Volume                   = {17},

  Abstract                 = {Features taken from various models of Titan's atm. are combined in a working composite model that provides environmental constraints within which different pathways for org. chem. synthesis are assessed. Exptl. results and theor. modeling studies suggest that the org. chem. of the satellite is dominated by 2 atm. processes: photochem. and energetic particle bombardment. Photochem. reactions of CH4 in the upper atm. can account for the presence of C2 hydrocarbons. Reactions initiated in various levels of the atm. by cosmic ray, Saturn wind, and solar-wind particle bombardment of a CH4-N2 atm. mixt. can account for the UV-visible absorbing stratospheric haze, the reddish appearance of the satellite, and some of the C2 hydrocarbons. In the lower atm., photochem. processes will be important if surface temps. are sufficiently high for gaseous NH3 and CH4; if 0.1% of the incident UV light from 1600-2270 .ANG. reaches the lower atm., these reactions will be capable of producing org. matter at a rate comparable to or higher than that resulting from particle-initiated reactions. Elec. discharges are highly improbable on Titan; if they occurred at all, they would be restricted to the lower atm. and clouds. Their yield of org. matter might approach that of hot H atom reactions if the conversion of solar to elec. discharge energy on Titan was as efficient as that on earth. [on SciFinder (R)]},
  Keywords                 = {Alkanes Role: FORM (Formation, nonpreparative) (formation of, in Titan atm.); Organic matter (formation of, on Titan); Planets (Saturn, Titan, org. chem. of) Saturn satellite Titan org chem; hydrocarbon formation atm Titan; org matter formation Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Chanover03,
  Title                    = {Probing Titan's lower atmosphere with acousto-optic tuning},
  Author                   = {Chanover, N. J. and Anderson, C. M. and McKay, C. P. and Rannou, P. and Glenar, D. A. and Hillman, J. J. and Blass, W. E.},
  Journal                  = {Icarus},
  Year                     = {2003},
  Number                   = {1},
  Pages                    = {150-163},
  Volume                   = {163},

  Abstract                 = {Narrow-band images of Titan were obtained in Nov. 1999 with the NASA/GSFC- built acousto-optic imaging spectrometer (AImS) camera. This instrument utilizes a tunable filter element that was used within the 500- to 1050-nm range, coupled to a CCD camera system. The images were taken with the Mount Wilson 2.54-m (100 in.) Hooker telescope, which is equipped with a natural guide star adaptive optics system. We obsd. Titan at 830 and 890 nm and at a series of wavelengths across the 940-nm window in Titan's atm. where the methane opacity is relatively low. We detd. the abs. reflectivity (I/F) of Titan and fit the values at 940 nm to a Minnaert function at Titan's equator and at -30 Deg latitude (closer to the subsolar point) and obtained av. values for the Minnaert limb-darkening slope, k, of 0.661 +- 0.007 and 0.775 +- 0.018, resp. Comparison with models suggests that the equatorial value of k is consistent with rain removal of haze in the lower atm. The higher value of k at -30 Deg is consistent with the southern hemisphere being brighter than the equator. However, the fits are not unique. The data and models at 890 are consistent with no limb brightening or darkening at this wavelength either at the equator or at -30 Deg. [on SciFinder (R)]},
  Keywords                 = {Satellites (Titan; acousto-optic tuning of Titan images at methane-transparent wavelenghts 830-940 nm indicates a relatively haze-free atm. below 60-70 km but inconclusive evidence of haze albedo asymmetry); Titan (acousto-optic tuning of Titan images at methane-transparent wavelenghts 830-940 nm indicates a relatively haze-free atm. below 60-70 km but inconclusive evidence of haze albedo asymmetry); IR reflectance spectra (near-IR, of Titan; acousto-optic tuning of Titan images at methane-transparent wavelenghts 830-940 nm indicates a relatively haze-free atm. below 60-70 km but inconclusive evidence of haze albedo asymmetry) Titan troposphere haze structure IR reflectance},
