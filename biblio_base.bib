@Article{Kok07,
  Title                    = {Oxygen compounds in Titan's stratosphere as observed by Cassini CIRS},
  Author                   = {de Kok, R. and Irwin, P.G.J. and Teanby, N.A. and Lellouch, E. and Bezard, B. and Vinatier, S. and Nixon, C.A. and Fletcher, L. and Howett, C. and Calcutt, S.B. and Bowles, N.E. and Flasar, F.M. and Taylor, F.W.},
  Journal                  = {Icarus},
  Year                     = {2007},

  Month                    = feb,
  Number                   = {2},
  Pages                    = {354--363},
  Volume                   = {186},

  Abstract                 = {We have investigated the abundances of Titan's stratospheric oxygen compounds using 0.5 cm-1 resolution spectra from the Composite Infrared Spectrometer on the Cassini orbiter. The CO abundance was derived for several observations of far-infrared nadir spectra, taken at a range of latitudes (75[degree sign] S-35[degree sign] N) and emission angles (0[degree sign]-60[degree sign]), using rotational lines that have not been analysed before the arrival of Cassini at Saturn. The derived volume mixing ratios for the different observations are mutually consistent regardless of latitude. The weighted mean CO volume mixing ratio is 47+/-8 ppm if CO is assumed to be uniform with latitude. H2O could not be detected and an upper limit of 0.9 ppb was determined. CO2 abundances derived from mid-infrared nadir spectra show no significant latitudinal variations, with typical values of 16+/-2 ppb. Mid-infrared limb spectra at 55[degree sign] S were used to constrain the vertical profile of CO2 for the first time. A vertical CO2 profile that is constant above the condensation level at a volume mixing ratio of 15 ppb reproduces the limb spectra very well below 200 km. This is consistent with the long chemical lifetime of CO2 in Titan's stratosphere. Above 200 km the CO2 volume mixing ratio is not well constrained and an increase with altitude cannot be ruled out there.},
  Keywords                 = {Titan, Atmospheres, composition},
  Owner                    = {pernot},
  Timestamp                = {2008.01.30},
  Url                      = {http://www.sciencedirect.com/science/article/B6WGF-4MCWB1C-2/2/d617d325d9166f5d045588bf44c659f3}
}

