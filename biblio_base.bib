@Article{Hamberg07,
  Title                    = {Experimental determination of dissociative recombination reaction pathways and absolute reaction cross-sections of {CH$_2$OH$^+$, CD$_2$OD$^+$ and CD$_2$OD$_2^+$}},
  Author                   = {Hamberg, M. and Geppert, W. D. and Thomas, R. D. and Zhaunerchyk, V. and \"Osterdahl, F. and Ehlerding, A. and Kaminska, M. and Semaniak, J. and Ugglas, M. af and K\"allberg, A. and Paal, A. and Simonsson, A. and Larsson, M.},
  Journal                  = {Molecular Physics: An International Journal at the Interface Between Chemistry and Physics},
  Year                     = {2007},
  Pages                    = {899--906},
  Volume                   = {105},

  Abstract                 = {Measurements of the cross-sections and branching ratios of the dissociative recombination of the ions CH2OH+, CD2OD+ and CD2OD2+ have been performed at the CRYRING storage ring located in Stockholm, Sweden. Evaluation of the data yielded reaction rate coefficients of: 7.0 times 10-7(T/300)-0.78 cm3 mol-1 s-1 for CH2OH+; 7.5 times 10-7(T/300)-0.70 cm3 mol-1 s-1 for CD2OD+ and 1.51 times 10-6(T/300)-0.66 cm3 mol-1 s-1 for CD2OD2+. Calculation of the branching ratios for CH2OH+ and its deuterated isotopologue gave the following results for the DR reaction channels involving C-O bond fissure: H2O + CH (2.2%) and CH2 + OH (5.5%) in the reaction of CH2OH+ as well as D2O + CD (5%) and CD2 + OD (18%) for the dissociative recombination of CD2OD+. The remainder of the reaction flux kept the C-O bond intact: 92% for CH2OH+ and 77% for CD2OD+, respectively. Other recent measurements on the CH3OH2+ ion indicate dominating bond breaking between the heavy atoms, which is in contrast to this experiment. For CD2OD2+ CO-bond breaking was observed for 43% of the reaction flux.
view references (41)},
