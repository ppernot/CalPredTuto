@Article{Lunine99,
  Title                    = {On the volatile inventory of Titan from isotopic abundances in nitrogen and methane},
  Author                   = {Lunine, Jonathan I. and Yung, Yuk L. and Lorenz, Ralph D.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1999},
  Note                     = {CAN 132:183319 53-9 Mineralogical and Geological Chemistry Reparto di Planetologia,CNR-Istituto di Astrofisica Spaziale,Rome,Italy. Journal 0032-0633 written in English. 74-82-8 (Methane); 676-49-3 (Methane-d) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (reconstruction of abundance of volatiles in Titan's atm. from published N- and H-isotope data)},
  Number                   = {10/11},
  Pages                    = {1291-1303},
  Volume                   = {47},

  Abstract                 = {Recently published nitrogen and hydrogen isotopic data were analyzed to constrain the initial volatile abundances on Saturn's giant moon Titan. The nitrogen data are interpreted in terms of a model of non-thermal escape processes that lead to enhancement in the heavier isotope. The authors show that these data do not, in fact, strongly constrain the abundance of nitrogen present in Titan's early atm., and that a wide range of initial atm. masses (all larger than the present value) can yield the measured enhancement. The enrichment in deuterated methane is now much better detd. than it was when Pinto et al. (1986) first proposed a photochem. mechanism to preferentially retain the deuterium. A simple linear theory is developed to provide a more reliable est. of the relative dissocn. rates of normal and deuterated methane. The improved data and models are used to compute initial methane reservoirs consistent with the obsd. enhancement. The result of this anal. agrees with an independent est. for the initial methane abundance based solely on the present-day rate of photolysis and an assumption of steady state. This consistency in reservoir size is necessary but not sufficient to infer that methane photolysis has proceeded steadily over the age of the solar system to produce large quantities of less volatile orgs. The anal. indicates an epoch of early atm. escape of nitrogen, followed by a later addn. of methane by outgassing from the interior. The results also suggest that Titan's volatile inventory came in part or largely from a circum-Saturnian disk of material more reducing than the surrounding solar nebula. Many of the ambiguities inherent in the present anal. can be resolved through Cassini-Huygens data and a program of lab. studies on isotopic and mol. exchange processes. The value of, and interest in, the Cassini-Huygens data can be greatly enhanced if such a program were undertaken prior to the prime phase of the mission. [on SciFinder (R)]},
  Keywords                 = {Simulation and Modeling; Titan (of processes affecting the abundance of volatiles in Titan's atm. and reconstruction of compn. of the initial atm. on the basis of N- and H-isotope data) hydrogen nitrogen isotope Titan atm reconstruction initial volatile abundance; methane nitrogen abundance initial atm Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

