@Article{Encrenaz97,
  Title                    = {Infrared observations of planetary atmospheres},
  Author                   = {Encrenaz, T.},
  Journal                  = {Astrophysics and Space Science Library},
  Year                     = {1997},
  Number                   = {Infrared Space Interferometry: Astrophysics \& the Study of Earth-Like Planets},
  Pages                    = {13-23},
  Volume                   = {215},

  Abstract                 = {IR spectra of planetary atmospheres show a large variety of features. They are dominated by the signatures of their most spectroscopically active constituents: CO2 in the case of the terrestrial planets, and CH4 in the case of the giant planets. Minor species are also detectable, in particular: CO and H2O in the terrestrial planets with, in addn., O3 in the Earth atm.; NH3 and PH3 in Jupiter and Saturn; several hydrocarbons in all giant planets and Titan. To search for spectral signatures in exoplanets, the 6-15 mm range is esp. appropriate. Assuming that all sources of noise (apart from the detector noise) can be completely removed, the required integration time for detecting an Earth-like planet at 5 parsec (assuming the performances of the ISO detectors) is about two weeks for a 5-m telescope (R=100, S/N=5). Improving the sensitivity of the detectors would reduce these integration times accordingly. [on SciFinder (R)]},
  Keywords                 = {Earth; IR spectra; Jupiter; Saturn; Titan (IR observations of planetary atmospheres); Water vapor (atm. gas; IR observations of planetary atmospheres); Hydrocarbons Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (atm. gas; IR observations of planetary atmospheres); Planets (giant; IR observations of planetary atmospheres) planetary atm IR observation},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Encrenaz94,
