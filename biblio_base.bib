}

@Article{Kossacki96,
  Title                    = {Hiding Titan's ocean: densification and hydrocarbon storage in an icy regolith},
  Author                   = {Kossacki, Konrad J. and Lorenz, Ralph D.},
  Journal                  = {Planetary and Space Science},
  Year                     = {1996},
  Note                     = {CAN 126:77692 53-9 Mineralogical and Geological Chemistry Institute of Geophysics,University of Warsaw,Warsaw,Pol. Journal 0032-0633 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (hiding Titan's ocean as explained by the densification and hydrocarbon storage in an icy regolith)},
  Number                   = {9},
  Pages                    = {1029-1037},
  Volume                   = {44},

  Abstract                 = {The problem of regolith densification is examd. with application to Titan, with the objective of establishing the pore vol. in the regolith. A large pore vol. could store the large vol. of hydrocarbons required to exist on Titan's surface by photochem. models, forming a \"buried ocean\", a surface model more compatible with observational constraints than the global ocean model favored in the last decade. A sophisticated coupled model of methane diffusion and regolith densification is presented. It is found that several hundred meters equiv. \"ocean\" depth can be concealed in the regolith. Further, methane diffusion through the regolith is fast enough to maintain the atm. methane concn. against photolysis. \"Hydrothermal\" methane circulation may play a significant role in surface chem. and heat transport. [on SciFinder (R)]},
  Keywords                 = {Atmosphere; Hydrothermal fluids; Titan (hiding Titan's ocean as explained by the densification and hydrocarbon storage in an icy regolith); Hydrocarbons Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (hiding Titan's ocean as explained by the densification and hydrocarbon storage in an icy regolith) hydrocarbon storage regolith planet Saturn Titan},
