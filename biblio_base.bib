@Article{McKay89,
  Title                    = {The thermal structure of Titan's atmosphere},
  Author                   = {McKay, Christopher P. and Pollack, James B. and Courtin, Regis},
  Journal                  = {Icarus},
  Year                     = {1989},
  Note                     = {CAN 111:118446 53-9 Mineralogical and Geological Chemistry Space Sci. Div.,NASA,Moffett Field,CA,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 74-84-0 (Ethane); 74-86-2 (Ethyne); 1333-74-0 (Hydrogen); 7727-37-9 (Nitrogen) Role: OCCU (Occurrence) (in atm. of Titan, thermal structure in relation to)},
  Number                   = {1},
  Pages                    = {23-53},
  Volume                   = {80},

  Abstract                 = {A radiative-convective model is developed of the thermal structure of Titan's atm. The model computes the solar and IR radiation in a series of spectral intervals with vertical resoln. Sources of opacity in the visible and near-IR include stratospheric haze particles, CH4 cloud particles, and gaseous CH4; IR sources of opacity in the thermal IR include the pressure-induced opacity of N2, CH4, and H2, the permitted transitions of C2H2 and C2H6, and particulate opacity. The haze properties were detd. with a simple microphysics model. The model contains a min. of free parameters; an attempt was made to det. these by fits to independent data sets. Gas and haze opacity alone, with the temps. fixed by Voyager observations, produces a model that is within a few percent of radiative convective balance everywhere in the atm. In a self-consistent computation of temps., model calcn. for the surface temp. is, in general, colder than the obsd. value by 5-10 DegK. The presence or absence of CH4 condensation clouds only slightly alters the results. Good agreement can be obtained by adjusting the parameters in the model. The model parameters in these optimized cases are typically within 15% of the baseline values and within the limits allowed by observations. The most important factors controlling Titan's thermal structure are absorption of sunlight by the stratospheric haze and the pressure-induced gas opacity in the IR. Within the uncertainties of the model, these effects can explain the obsd. temp. profile. Condensation clouds play a minor role, if any. [on SciFinder (R)]},
  Keywords                 = {Opacity (pressure-induced gas, of Titan's atm., thermal structure in relation to); Planets (Saturn, Titan satellite, atm. of, thermal structure of); Haze (atm., stratospheric, sunlight absorption by, Titan's thermal structure in relation to) Titan satellite atm thermal structure; Saturn Titan atm thermal structure},
