@Article{Atreya06,
  Title                    = {Titan's methane cycle},
  Author                   = {Atreya, Sushil K. and Adams, Elena Y. and Niemann, Hasso B. and Demick-Montelara, Jaime E. and Owen, Tobias C. and Fulchignoni, Marcello and Ferri, Francesca and Wilson, Eric H.},
  Journal                  = {Planetary and Space Science},
  Year                     = {2006},

  Month                    = oct,
  Number                   = {12},
  Pages                    = {1177--1187},
  Volume                   = {54},

  Abstract                 = {Methane is key to sustaining Titan's thick nitrogen atmosphere. However, methane is destroyed and converted to heavier hydrocarbons irreversibly on a relatively short timescale of approximately 10-100 million years. Without the warming provided by CH4-generated hydrocarbon hazes in the stratosphere and the pressure induced opacity in the infrared, particularly by CH4-N2 and H2-N2 collisions in the troposphere, the atmosphere could be gradually reduced to as low as tens of millibar pressure. An understanding of the source-sink cycle of methane is thus crucial to the evolutionary history of Titan and its atmosphere. In this paper we propose that a complex photochemical-meteorological-hydrogeochemical cycle of methane operates on Titan. We further suggest that although photochemistry leads to the loss of methane from the atmosphere, conversion to a global ocean of ethane is unlikely. The behavior of methane in the troposphere and the surface, as measured by the Cassini-Huygens gas chromatograph mass spectrometer, together with evidence of cryovolcanism reported by the Cassini visual and infrared mapping spectrometer, represents a "methalogical" cycle on Titan, somewhat akin to the hydrological cycle on Earth. In the absence of net loss to the interior, it would represent a closed cycle. However, a source is still needed to replenish the methane lost to photolysis. A hydrogeochemical source deep in the interior of Titan holds promise. It is well known that in serpentinization, hydration of ultramafic silicates in terrestrial oceans produces H2(aq), whose reaction with carbon grains or carbon dioxide in the crustal pores produces methane gas. Appropriate geological, thermal, and pressure conditions could have existed in and below Titan's purported water-ammonia ocean for "low-temperature" serpentinization to occur in Titan's accretionary heating phase. On the other hand, impacts could trigger the process at high temperatures. In either instance, storage of methane as a stable clathrate-hydrate in Titan's interior for later release to the atmosphere is quite plausible. There is also some likelihood that the production of methane on Titan by serpentinization is a gradual and continuous on-going process.},
  Booktitle                = {Surfaces and Atmospheres of the Outer Planets, their Satellites and Ring Systems from Cassini-Huygens Data},
  Keywords                 = {Titan, Methane, Serpentinization, Origin, Meteorology, Methalogical cycle},
  Owner                    = {pernot},
  Timestamp                = {2008.01.30},
  Url                      = {http://www.sciencedirect.com/science/article/B6V6T-4KGPPJ1-1/2/4da062f32b59d7d0456c9d3a527f86ff}
}

@Article{Atreya78,
  Title                    = {Evolution of a nitrogen atmosphere on Titan},
  Author                   = {Atreya, Sushil K. and Donahue, Thomas M. and Kuhn, William R.},
  Journal                  = {Science (Washington, DC, United States)},
  Year                     = {1978},
  Number                   = {4356},
  Pages                    = {611-13},
  Volume                   = {201},

  Abstract                 = {Photochem. calcns. indicate that if NH3 outgassed from Titan it should have been converted to a dense N2 atm. during the lifetime of the satellite. A crucial step in the process involves a gas phase reaction of N2H4 with H. The most favorable conditions for this step would be the intermediate prodn. of a CH4-H2 greenhouse capable of raising the gas temp. to 150 K. Subsequently, .apprx.20 bars of N2 could have evolved. The pressure-induced opacity of 20 bars of N2 should suffice to explain the recently measured 200 K surface temp. Unlike the situation of Jupiter, NH3 is not recycled on Titan by reactions involving N2 or N2H4. This may explain the failure of recent attempts to detect NH3 in the upper atm. of Titan. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan, atm. evolution on, photochem. in study of) nitrogen atm evolution Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Atreya99,
  Title                    = {ISO observations of C2H2 on Uranus and CH3 on Saturn: implications for atmospheric vertical mixing in the Voyager and ISO epochs, and a call for relevant laboratory measurements},
  Author                   = {Atreya, S. K. and Edgington, S. G. and Encrenaz, Th and Feuchtgruber, H.},
  Journal                  = {European Space Agency, [Special Publication] SP},
  Year                     = {1999},
  Number                   = {Universe as Seen by ISO, Vol. 1},
  Pages                    = {149-152},
  Volume                   = {SP-427},

  Abstract                 = {In Oct. 1996, ISO/SWS recorded the spectra of Uranus in the 7-16.5 mm. range and detected the spectral signatures of acetylene (C2H2). Interpretation of the C2H2 d. profile with photochem. models leads to a value of (5 - 10) * 103 cm2s-1 for the eddy diffusion coeff. (Kh) at the Uranus homopause, which for these limits of Kh corresponds to 354 km (0.037 mb) and 390 km (0.02 mb) above the 1-bar level. The eddy coeff. derived from these globally averaged observations is found to be in agreement with the values obtained by Voyager for the equatorial-low latitudes ten years earlier. The ISO-Voyager consistency in Kh indicates that whatever little manifestable internal energy Uranus possesses may still be adequate and responsible for controlling the behavior of vertical mixing in its visible atm., and that the dynamical behavior of the upper atm. is fairly uniform over the planet. In Dec. 1997, Me radicals (CH3) were also detected by ISO/SWS in the atm. of Saturn. The derived stratospheric column abundance of CH3, (1.5-7.5) * 1013 cm-2, is about a factor of 10 smaller than the predictions of methane photochem. models, however. The discrepancy could be explained by one of two means: either the value of the eddy diffusion coeff. used in the models - the Voyager value - is too high by at least a factor of 100, or the rate const. for the self-reaction loss of CH3 is too low by at least a factor of 10. It is argued that Kh could not possibly be reduced so drastically from the Voyager epoch. Instead, we believe the source of the discrepancy between the ISO derived CH3 and the model calcns. lies in the poor knowledge of lab. chem. kinetics data on certain CH3 loss reactions, which has serious implications for the interpretation of observations of atmospheres of the giant planets and Titan. [on SciFinder (R)]},
  Keywords                 = {Saturn; Uranus (IR Space Observatory observations of C2H2 on Uranus and CH3 on Saturn) acetylene atm Uranus; methyl radical atm Saturn},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Auerbach77,
