@Article{Coll95,
  Title                    = {Organic chemistry in Titan's atmosphere: New data from laboratory simulations at low temperature},
  Author                   = {Coll, P. and Coscia, D. and Gazeau, M. C. and Vanssay, E. de and Guillemin, J. C. and Raulin, F.},
  Journal                  = {Advances in Space Research},
  Year                     = {1995},
  Number                   = {2},
  Pages                    = {93-103},
  Volume                   = {16},

  Abstract                 = {Many expts. have already been carried out to simulate org. chem. on Titan, the largest satellite of Saturn. They can provide fruitful information on the nature of minor org. constituents likely to be present in Titan's atm., both in gas and aerosol phases. Indeed, all the org. compds. but one already detected in Titan's atm. have been identified in simulation expts. The exception, C4N2, as well as other compds. expected in Titan from theor. modeling, such as other N-orgs., and polyynes, first of all C6H2, have never been detected in exptl. simulation thus far. All these compds. are thermally unstable, and the temp. conditions used during the simulation expts. were not appropriate. We have recently started a new program of simulation expts. with temp. conditions close to that of Titan's environment. It also uses dedicated anal. techniques and procedures compatible with the anal. of orgs. only stable at low temps., as well solid products of low stability in the presence of O2 and H2O. Spark discharge of N2-CH4 gas mixts. was carried out at low temp. in the range 100-150 K. Products were analyzed by FTIR, GC, and GC-MS techniques. GC-peaks were identified by their mass spectrum, and, in most cases, by comparison of the retention time and mass spectrum with std. ones. In Titan simulation expts., C6H2 and HC5N were detected. Their abundance is a few percent relative to C4H2 and HC3N, resp. Preliminary data on the solid products indicate an elemental compn. corresponding to (H11C11N)n. These results open new prospects in the modeling of Titan's haze making. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, org. chem. in Titan's atm., based on data from lab. simulations at low temp.) planet Saturn Titan atm org chem},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Coll98,
  Title                    = {Review and latest results of laboratory investigations of Titan's aerosols},
