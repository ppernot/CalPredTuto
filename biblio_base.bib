}

@Article{Thompson94,
  Title                    = {The Titan haze revisited: Magnetospheric energy sources and quantitative tholin yields},
  Author                   = {Thompson, W. Reid and McDonald, Gene D. and Sagan, Carl},
  Journal                  = {Icarus},
  Year                     = {1994},
  Note                     = {CAN 122:244517 53-9 Mineralogical and Geological Chemistry Laboratory Planetary Studies,Cornell University,Ithaca,NY,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (haze in the atm. of Titan with magnetospheric energy sources and quant. tholin yields)},
  Number                   = {2},
  Pages                    = {376-81},
  Volume                   = {112},

  Abstract                 = {Lab. measurements are made of the radiation yields of complex org. solids produced from N2/CH4 gas mixts. contg. 10 or 0.1% CH4. These tholins resemble org. aerosols produced in the atmospheres of Titan, Pluto, and Triton. The tholin yields are large compared to the total yield of gaseous products: nominally, 13 (C + N)/100 eV for Titan tholin and 2.1 (C + N)/100 eV for Triton tholin. High-energy magnetospheric electrons responsible for tholin prodn. represent a class distinct from the plasma electrons considered in models of Titan's airglow. Electrons with E > 20 keV provide an energy flux .apprx.1 * 10-2 erg cm-2 sec-1, implying from our measured tholin yields a mass flux of 0.5 to 4.0 * 10-14 g cm-2 sec-1 of tholin. The corresponding thickness of the tholin sedimentary column accumulated over 4 Gyr on Titan's surface is 4-30 m. This figure is in agreement with required mass fluxes computed from recent radiative transfer and sedimentation models. If, however, these results, derived from expts. at .apprx.2 mb, are applied to lower pressure levels toward peak auroral electron energy deposition and scaled with pressure as the gas-phase org. yields, the derived tholin mass flux is at least an order of magnitude less. We attribute this difference to the fact that tholin synthesis occurs well below the level of max. electron energy deposition and to possible contributions to tholins from UV-derived C2-hydrocarbons. The Titan tholin, produced by magnetospheric electrons, is alone sufficient to supply at least a significant fraction of Titan's haze; this result is consistent with the fact that the optical properties of Titan tholin, among all proposed materials, are best at reproducing Titan's geometric albedo spectrum from near UV to mid-IR in light-scattering models. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, haze in the atm. of Titan with magnetospheric energy sources and quant. tholin yields); Haze (atm., haze in the atm. of Titan with magnetospheric energy sources and quant. tholin yields); Organic compounds Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (tholins, haze in the atm. of Titan with magnetospheric energy sources and quant. tholin yields) planet Titan haze magnetosphere energy source},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Thompson92a,
  Title                    = {Organic chemistry on Titan - surface interactions},
  Author                   = {Thompson, W. Reid and Sagan, Carl},
  Journal                  = {European Space Agency, [Special Publication], SP},
  Year                     = {1992},
  Note                     = {CAN 118:172828 53-9 Mineralogical and Geological Chemistry Lab. Planet. Stud.,Cornell Univ.,Ithaca,NY,USA. Journal 0379-6566 written in English.},
  Number                   = {Proc. - Symp. Titan, 1991},
  Pages                    = {167-76},
  Volume                   = {ESA SP-338},

  Abstract                 = {Over geol. time, substantial org. sediment layers - 10-100 m of C2H2, HCN and tholins, plus other hydrocarbons and nitriles - have accumulated on Titan. These sediments are not pristine, but have been subjected to impact processing. Impacts till the surface, producing a .apprx.400 m deep regolith of .apprx.20% orgs. mixed or interbedded with H2O ice, and produce transient crater floor pools of liq. H2O (or H2O + NH3) lasting .apprx.1000 yr, allowing some aq. org. chem. at T .ltorsim. 273 K to occur. Furthermore, decompression of impact-shocked H2O ice against the 1.5 bar surface pressure results in substantial amts. (.gtorsim.10%) of liq. and should produce extensive ejecta blankets. For blanket-to-crater diam. ratios similar to those seen on Mars, .apprx.60-90% of Titan's surface, over its history, would have been exposed to liq. H2O for times of .apprx.minutes (for entrainment-cooled slurry) to .apprx.200 yr (for .apprx.100 m-deep flows or ponds of liq. ejecta). Aq. ejecta entrain and dissolve large amts. of org.-bearing regolith, resulting in high concns. of nitriles and other reactive species. Reactions among these aq. orgs. - which readily yield amino acids, purines, pyrimidines, aldehydes, etc. in lab. simulations - will occur sufficiently rapidly at .apprx.273 K to produce large amts. of org. compds. of high prebiol. significance. The presence of condensing agents (C2N2, HCN tetramer, etc.) along with a natural concg. mechanism makes polymn. of amino acids and other species likely. Because of these impact-mediated aq. alteration processes, Titan's surface is a natural lab. for chem. evolution - more directly relevant to the origin of life on Earth than has been previously recognized. [on SciFinder (R)]},
  Keywords                 = {Organic chemistry (of Titan's atm., effect of cratering and impact processes on, life origin in relation to); Life (origin of, org. chem. of Titan's atm. in relation to); Planets (Saturn, Titan satellite, atm. of, org. chem. of, effect of cratering and impact processes on, life origin in relation to) Titan atm org chem life origin},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Thompson84,
  Title                    = {Titan: far-infrared and microwave remote sensing of methane clouds and organic haze},
  Author                   = {Thompson, W. Reid and Sagan, Carl},
  Journal                  = {Icarus},
  Year                     = {1984},
  Note                     = {CAN 102:36067 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Lab. Planet. Stud.,Cornell Univ.,Ithaca,NY,USA. Journal 0019-1035 written in English. 74-82-8; 1333-74-0; 7727-37-9 Role: OCCU (Occurrence) (in planet Saturn Titan satellite atm.)},
  Number                   = {2},
  Pages                    = {236-59},
  Volume                   = {60},

  Abstract                 = {Titan's surface and plausible atm. thermal opacity sources, gaseous N, CH4, and H, CH4 cloud, and org. haze are sufficient to match available Earth-based and Voyager observations of Titan's thermal emission spectrum. Dominant sources of thermal emission are the surface for wavelengths l .gtorsim. 1 cm, atm. N2 for 1 cm .gtorsim. l .gtorsim. 200 mm, condensed and gaseous CH4 for 200 mm .gtorsim. l .gtorsim.20 mm, and mol. bands and org. haze for l .ltorsim. 20 mm. Matching computed spectra to the obsd. Voyager IRIS spectra at 7.3 and 52.7 Deg emission angles yields the following abundance and locations of opacity sources: CH4 clouds: 0.1 g-cm-2 at a planetocentric radius of 2610-2625 km, 0.3 g-cm-2 at 2590-2610 km, total 0.4 +- 0.1 g-cm-2 above 2590 km; org. haze: 4 +- 2 * 10-6 g cm-2 above 2750 km; tropospheric H2: 0.3 +- 0.1 mol%. This is the 1st quant. est. of the column d. of condensed CH4 (or CH4/C2H6) on Titan. Max. transparency in the middle to far IR occurs at 19 mm where the atm. vertical absorption optical depth is .simeq.0.6. A particle radius .hivin.r .ltorsim. 2 mm in the upper portion of the CH4 clouds is indicated by the apparent absence of scattering effects. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite of, far-IR and microwave remote sensing of methane clouds and org. haze in atm. of) planet Saturn Titan IR microwave},
