@Article{Vuitton01,
  Title                    = {IR and UV spectroscopic data for polyynes: predictions for long carbon chain compounds in Titan's atmosphere},
  Author                   = {Vuitton, V. and Scemama, A. and Gazeau, M. C. and Chaquin, P. and Benilan, Y.},
  Journal                  = {Advances in Space Research},
  Year                     = {2001},
  Note                     = {CAN 135:245196 53-9 Mineralogical and Geological Chemistry Laboratoire Interuniversitaire des Systemes Atmospheriques (LISA),Creteil,Fr. Journal 0273-1177 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), PEP (Physical, engineering or chemical process), RCT (Reactant), OCCU (Occurrence), PROC (Process), RACT (Reactant or reagent) (in Titan's atm.; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); 74-86-2 (Ethyne); 460-12-8 (1,3-Butadiyne); 3161-99-7 (1,3,5-Hexatriyne); 6165-96-4 (1,3,5,7-Octatetrayne) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (in Titan's atm.; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); 7440-44-0D (Carbon) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (long-chain, in Titan's atm.; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data)},
  Number                   = {2},
  Pages                    = {283-288},
  Volume                   = {27},

  Abstract                 = {A better understanding of the complex org. chem. occurring in the methane-rich atm. of Titan can be achieved via the comparison of observations with results obtained by theor. models. Available observations are still few but their anal. requires the knowledge of a large set of data, namely frequencies and abs. band intensities. Cross sections are also needed to develop the chem. schemes of photochem. models, in particular the schemes leading to the formation of haze particles visible on Titan. Unfortunately, some of these parameters are not well known, esp. if one takes into account the extreme phys. conditions of the studied object. This lack of data is particularly enhanced for polyynes because these compds. are highly unstable at the usual pressure and temp. conditions of a lab. and therefore are very difficult to study. We have developed UV and IR studies, coupling exptl. and theor. approaches, in order to extrapolate the parameters available for short polyynes to longer carbon chains. In the mid-UV range, when the length of the chain increases, the absorption system of polyynes is shifted to longer wavelength and its oscillator strength increases linearly. In the IR range, with the increase of the no. of carbon bonds, the positions of the CCC and CCH bending modes shift to lower energy, the latest converging rapidly to a fixed value of 620.5 cm-1 for an infinite length polyyne. Implications for detection and evolution of polyynes in Titan's atm. are emphasized. [on SciFinder (R)]},
  Keywords                 = {Bending (CCH, vibration modes, of C compds. in Titan's atm.; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); Hydrocarbons Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (C2-4, in Titan's atm.; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); Satellites (Titan; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); IR spectra (long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); Simulation and Modeling; UV and visible spectra (long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data obtained from exptl. studies); Oscillator strength (of polyynes; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data); Alkynes Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (polyalkynes; long-chain carbon compds. and polyynes in methane-rich atm. of Titan satellite, based on IR and UV spectroscopic data) Titan satellite Saturn atm polyyne carbon chain compd methane},
  Owner                    = {pernot},
