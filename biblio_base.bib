@Article{Anicich04,
  Title                    = {A SIFT ion-molecule study of some reactions in Titan's atmosphere. reactions of {N}$^+$, {N}$_2^+$, and {HCN}$^+$ with {CH}$_4$, {C}$_2${H}$_2$, and {C}$_2${H}$_4$},
