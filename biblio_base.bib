@Article{Toon88,
  Title                    = {Methane rain on Titan},
  Author                   = {Toon, Owen B. and McKay, Christopher P. and Courtin, Regis and Ackerman, Thomas P.},
  Journal                  = {Icarus},
  Year                     = {1988},
  Note                     = {CAN 109:101367 73-9 Optical, Electron, and Mass Spectroscopy and Other Related Properties Ames Res. Cent.,NASA,Moffett Field,CA,USA. Journal 0019-1035 written in English. 1333-74-0 (Hydrogen); 7727-37-9 (Nitrogen) Role: PRP (Properties) (absorption of, in IR spectra of satellite Titan); 74-82-8 (Methane); 74-84-0 (Ethane) Role: OCCU (Occurrence) (rain contg., on satellite Titan)},
  Number                   = {2},
  Pages                    = {255-84},
  Volume                   = {75},

  Abstract                 = {A new set of absorption coeffs. for CH4, H, and N, and n of haze and CH4 clouds were used in a multiple scattering radiative transfer model to reexamine the Voyager IR interferometer spectra of Titan. H composes 0.2-0.6% of the atm. The CH4 relative humidity at the surface may be >60%. CH4 clouds probably exist at altitudes <10-.apprx.30 km. The cloud particle radius is probably >50 mm so that the particles should be classified as rain. The optical depth of the clouds that are present is typically of the order of 2-5 in both the IR and the visible. The clouds are probably patchy. The clouds have only a small effect on the IR spectrum of Titan, and probably are not significant for visible radiation either. The large particle sizes and low cloud optical depths on Titan relative to Earth can be understood if Titan's high altitude haze is the major source of condensation nuclei. Then particle growth is not limited by low supersatn. until raindrop sizes are reached. H and the high altitude haze play an important role in the IR interferometer spectra 500-600 cm-1. Latitudinal variations in the brightness temp. at these wavenos. may be due primarily to obsd. variations in the temp. of the haze layer, rather than variations in the temp. of the surface. The presence of Ar is not required to interpret the IR interferometer spectra. [on SciFinder (R)]},
  Keywords                 = {Planets (Saturn, Titan satellite, methane rain on); Organic compounds Role: OCCU (Occurrence) (tholins, rain contg., on satellite Titan) methane rain planetary satellite Titan},
