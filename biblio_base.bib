@Article{Griffith98,
  Title                    = {Transient clouds in Titan's lower atmosphere},
  Author                   = {Griffith, Caitlin A. and Owen, Tobias and Miller, Gary A. and Geballe, Thomas},
  Journal                  = {Nature (London)},
  Year                     = {1998},
  Note                     = {CAN 129:318898 53-9 Mineralogical and Geological Chemistry Dep. Physics and Astronomy,Northern Arizona Univ.,Flagstaff,AZ,USA. Journal 0028-0836 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (methane-condensation transient clouds in Titan's lower atm.)},
  Number                   = {6702},
  Pages                    = {575-578},
  Volume                   = {395},

  Abstract                 = {The 1980 encounter by the Voyager 1 spacecraft with Titan, Saturn's largest moon, revealed the presence of a thick atm. contg. nitrogen and methane (1.4 and .apprx.0.05 bar, resp.). Methane was found to be nearly satd. at Titan's tropopause, which, with other considerations, led to the hypothesis that Titan might experience a methane analog of Earth's vigorous hydrol. cycle, with clouds, rain and seas. Yet recent analyses of Voyager data indicate large areas of supersatd. methane, more indicative of dry and stagnant conditions. A resoln. to this apparent contradiction requires observations of Titan's lower atm., which was hidden from the Voyager cameras by the photochem. haze (or smog) in Titan's stratosphere. The authors report near-IR spectroscopic observations of Titan within four narrow spectral windows where the satellite's atm. is ostensibly transparent. The authors detected pronounced flux enhancements that indicate the presence of reflective methane condensation clouds in the troposphere. These clouds occur at a relatively low altitude (15 +- km), at low latitudes, and appear to cover .apprx.9 per cent of Titan's disk. [on SciFinder (R)]},
  Keywords                 = {Clouds; Titan (methane-condensation transient clouds in Titan's lower atm.); IR reflectance spectra (of surface of Titan satellite suggesting presence of methane-condensation clouds in lower atm.) Titan satellite methane cloud lower atm},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Griffith91,
  Title                    = {Titan's surface and troposphere, investigated with ground-based, near-infrared observations},
  Author                   = {Griffith, Caitlin A. and Owen, Tobias and Wagener, Richard},
  Journal                  = {Icarus},
  Year                     = {1991},
  Note                     = {CAN 116:88083 53-9 Mineralogical and Geological Chemistry Phys. Dep.,State Univ. New York,Stony Brook,NY,USA. Journal 0019-1035 written in English. 7732-18-5 (Water) Role: OCCU (Occurrence) (dirty, in surface of Titan satellite, near-IR reflection spectra in relation to); 74-82-8 (Methane) Role: OCCU (Occurrence) (in cloud layer of Titan satellite, near-IR spectral study of); 1333-74-0 (Hydrogen) Role: OCCU (Occurrence) (in troposphere of Titan satellite)},
  Number                   = {2},
  Pages                    = {362-78},
  Volume                   = {93},

  Abstract                 = {New observations of Titan's near-IR spectrum (4000-5000 cm-1) combined with points taken from U. Fink and H.P. Larson's (1979) spectrum (4000-12500 cm-1) provide information on Titan's haze, possible clouds, surface albedo, and atm. abundance of H2. In the near-IR, the main features in Titan's spectrum result from absorption of solar radiation by CH4. The strength of this absorption varies considerably with wavelength, allowing study of various atm. levels down to the surface itself, by choosing specific wavelengths for anal. At 4715 cm-1, the pressure-induced S(1) fundamental band of H2 lies in the wings of CH4 bands. Based on current values for the CH4 line parameters, Titan's spectrum can be best interpreted with a vol. mixing ratio of H2 between 0.5-1.0%. The existence of an optically thin CH4 cloud layer is indicated. The optical depths detd. for Titan's haze and clouds are small enough to allow sensing of the surface of Titan at 4900, 6250, and 7700 cm-1. The most plausible interpretation of the albedos detd. at these wavenumbers suggests a surface dominated by \"dirty\" water ice. A global C2H6 ocean is not compatible with these albedos. [on SciFinder (R)]},
  Keywords                 = {Albedo (of surface of Titan satellite, ground-based near-iR reflection spectral studies in relation to); Planets (Saturn, Titan satellite, surface and troposphere of, ground-based near-IR reflection spectral study in relation to) Titan surface troposphere near IR spectrum; hydrogen abundance atm Titan satellite; methane cloud layer Titan satellite; water ice surface Titan satellite},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Griffiths1987,
  Title                    = {Experimental evidence for the stability of the {NH$_4^.$, H$_3$O$^.$, H$_3$S$^.$, and H$_3^.$} radicals by neutralization-reionization spectroscopy},
  Author                   = {W. J. Griffiths and F. M. Harris and J. H. Beynon},
