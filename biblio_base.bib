@Article{Lebonnois2005,
  Title                    = {Benzene and aerosol production in {T}itan and {J}upiter's atmospheres: a sensitivity study},
  Author                   = {S. Lebonnois},
  Journal                  = {Planetary and Space Science},
  Year                     = {2005},
  Pages                    = {486 - 497},
  Volume                   = {53},

  Abstract                 = {Benzene has recently been observed in the atmosphere of Jupiter, Saturn and also Titan. This compound is required as a precursor for larger aromatic species (PAHs) that may be part of aerosol particles. Several photochemical models have tried to reproduce the observed quantities of benzene in the atmospheres of Jupiter (both low- and high-latitudes regions), Saturn and Titan. In this present work, we have conducted a sensitivity study of benzene and PAHs formation, using similar photochemical schemes both for Titan and Jupiter (low-latitudes conditions). Two different photochemical schemes are used, for which the modeled composition fairly agrees with observational constraints, both for Jupiter and Titan. Some disagreements are specific to each atmospheric case, which may point to needed improvements, especially in kinetic data involved in the corresponding chemical cycles. The observed benzene mole fraction in Titan's stratosphere is reproduced by the model, but in the case of Jupiter, low-latitudes benzene abundance is only 3% of the observed column density, which may indicate a possible influence of latitudinal transport, since abundance of benzene is much higher in auroral regions. Though, the photochemical scheme of C6 compounds at temperature and pressure conditions of planetary atmospheres is still very uncertain. Several variations are therefore done on key reactions in benzene production. These variations show that benzene abundance is mainly sensitive to reactions that may affect the propargyl radical. The effect of aerosol production on hydrocarbons composition is also tested, as well as possible heterogenous recombination of atomic hydrogen in the case of Titan. PAHs are a major pathway for aerosol production in both models. The mass production profiles for aerosols are discussed for both Titan and Jupiter. Total production mass fluxes are roughly three times the one expected by observational constraints in both cases. Such comparative studies are useful to bring more constraints on photochemical models.},
  Doi                      = {DOI: 10.1016/j.pss.2004.11.004},
  File                     = {:home/plessis/article/reference/Lebonnois2005.pdf:},
  ISSN                     = {0032-0633},
  Keywords                 = {Benzene},
  Owner                    = {plessis},
  Timestamp                = {2009.07.01},
  Url                      = {http://www.sciencedirect.com/science/article/B6V6T-4FF9HCJ-3/2/8594742b45a9fad4140837f73e33c07d}
}

@Article{Lebonnois2003,
  Title                    = {Atomic and molecular hydrogen budget in {T}itan's atmosphere},
  Author                   = {S. Lebonnois and E.L.O. Bakes and C.P. McKay},
  Journal                  = {Icarus},
  Year                     = {2003},
  Pages                    = {474--485},
  Volume                   = {161},

  Abstract                 = {Using a one-dimensional model, we investigate the hydrogen budget and escape to space in Titan's atmosphere. Our goal is to study in detail the distributions and fluxes of atomic and molecular hydrogen in the model, while identifying sources of qualitative and quantitative uncertainties. Our study confirms that the escape of atomic and molecular hydrogen to space is limited by the diffusion through the homopause level. The H distribution and flux inside the atmosphere are very sensitive to the eddy diffusion coefficient used above altitude 600 km. We chose a high value of this coefficient 1 × 108 cm2 s-1 and a homopause level around altitude 900 km. We find that H flows down significantly from the production region above 500 km to the region [300-500] km, where it recombines into H2. Production of both H and H2 also occurs in the stratosphere, mostly from photodissociation of acetylene. The only available observational data to be compared are the escape rate of H deduced from Pioneer 11 and IUE observations of the H torus 1-3 × 109 cm-2 s-1 and the latest retrieved value of the H2 mole fraction in the stratosphere: (1.1 ± 0.1) × 10-3. Our results for both of these values are at least 50-100% higher, though the uncertainties within the chemical schemes and other aspects of the model are large. The chemical conversion from H to H2 is essentially done through catalytic cycles using acetylene and diacetylene. We have studied the role of this diacetylene cycle, for which the associated reaction rates are poorly known. We find that it mostly affects C4 species and benzene in the lower atmosphere, rather than the H profile and the hydrogen budget. We have introduced the heterogenous recombination of hydrogen on the surface of aerosol particles in the stratosphere, and this appears to be a significant process, comparable to the chemical processes. It has a major influence on the H distribution, and consequently on several other species, especially C3H4, C4H2 and C6H6. Therefore, this heterogenous process should be taken into account when trying to understand the stratospheric distribution of these hydrocarbons.},
  Doi                      = {DOI: 10.1016/S0019-1035(02)00039-8},
  File                     = {:home/plessis/article/reference/Lebonnois2003.pdf:},
  ISSN                     = {0019-1035},
  Keywords                 = {Titan},
  Owner                    = {plessis},
  Timestamp                = {2009.07.01},
  Url                      = {http://www.sciencedirect.com/science/article/B6WGF-47TWYWG-2/2/cee8f2fc3d5dbfd51d917f292fe9ef4e}
}

@Article{Lebonnois2002,
  Title                    = {Transition from gaseous compounds to aerosols in Titan's atmosphere},
  Author                   = {Lebonnois, Sebastien and Bakes, E. L. O. and McKay, Christopher P.},
  Journal                  = {Icarus},
  Year                     = {2002},
  Number                   = {2},
  Pages                    = {505-517},
  Volume                   = {159},

  Abstract                 = {We investigate the chem. transition of simple mols. like C2H2 and HCN into aerosol particles in the context of Titan's atm. Expts. that synthesize analogs (tholins) for these aerosols can help illuminate and constrain these polymn. mechanisms. Using information available from these expts., we suggest chem. pathways that can link simple mols. to macromols., which will be the precursors to aerosol particles: polymers of acetylene and cyanoacetylene, polycyclic aroms., polymers of HCN and other nitriles, and polyynes. Although our goal here is not to build a detailed kinetic model for this transition, we propose parameterizations to est. the prodn. rates of these macromols., their C/N and C/H ratios, and the loss of parent mols. (C2H2, HCN, HC3N and other nitriles, and C6H6) from the gas phase to the haze. We use a one-dimensional photochem. model of Titan's atm. to est. the formation rate of precursor macromols. We find a prodn. zone slightly lower than 200 km altitude with a total prodn. rate of 4 * 10-14 g cm-2 s-1 and a C/N .simeq. 4. These results are compared with exptl. data, and to microphys. model requirements. The Cassini/Huygens mission will bring a detailed picture of the haze distribution and properties, which will be a great challenge for our understanding of these chem. processes. [on SciFinder (R)]},
  Keywords                 = {Polycyclic compounds Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (arom.; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Titan (atm. of; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Atmosphere; Atmospheric aerosols (chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Nitriles Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Polymerization (mechanisms of; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Alkynes Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (polyalkynes; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Aromatic compounds Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (polycyclic; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan); Organic compounds Role: GFM (Geological or astronomical formation), GOC (Geological or astronomical occurrence), FORM (Formation, nonpreparative), OCCU (Occurrence) (tholins, formation of; chem. transition of simple gaseous mols. (C2H2, HCN) to aerosol particles in the atm. of Saturn's satellite Titan) Saturn satellite Titan atm gaseous compd aerosol tholin cyanoacetylene},
