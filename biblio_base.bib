@Article{Trafton75,
  Title                    = {Near-infrared spectrophotometry of {T}itan},
  Author                   = {L. M. Trafton},
  Journal                  = {Icarus},
  Year                     = {1975},
  Number                   = {4},
  Pages                    = {443--453},
  Volume                   = {24},

  Abstract                 = {Detailed analysis of the R(5) manifold of Titan's 3[nu]3 CH4 band confirms that the column abundance of Titan's spectroscopically visible atmosphere is greater than 1.6 kmamagats. This agrees with the value estimated from the strength of Titan's 3[nu]3 CH4 Q branch and is at least 25 times the value for the column abundance of Mars' atmosphere. Moreover, the enhanced strength of the weaker CH4 lines in Titan's spectrum relative to Saturn's spectrum suggests that CH4 constitutes a significant fraction of this bulk. Recently discovered strong, unidentified absorptions in Titan's spectrum at 1.05-1.06 [mu]m have been compared with laboratory spectra of a number of gases including CH4, C2H4, C2H6, and C3H8 with negative results. These comparisons, however, have not excluded the possibility that these features arise from a very large quantity of CH4 or from an isotope of CH4. The fundamental transition of the responsible molecule may affect the interpretation of Titan's 8-14 [mu]m spectrum since its wavelength may lie in this window. Comparison with Uranus' spectrum suggests that the visible abundance of this molecule in Titan's atmosphere may be much greater than in Uranus' relatively clear, deep atmosphere. Spectra of features at [lambda]8150.7 and [lambda]8272.7 attributed possibly to H2 have been obtained at high resolution also during the apparitions of 1971, 1972, and 1973. These are presented for comparison with the results of the 1970 apparition. The existence of the [lambda]8150.7 feature is established definitively but further observations are needed to establish whether the [lambda]8272.7 feature exists beyond doubt.},
  Doi                      = {DOI: 10.1016/0019-1035(75)90062-7},
  File                     = {Trafton1975.pdf:Trafton1975.pdf:},
  ISSN                     = {0019-1035},
  Owner                    = {plessis},
  Timestamp                = {2010.05.12},
  Url                      = {http://www.sciencedirect.com/science/article/B6WGF-4731494-168/2/f8d33436839925792fd5bf11a48b4178}
}

@Article{Trainer04,
  Title                    = {Chemical composition of Titan's haze: are PAHs present?},
  Author                   = {Trainer, Melissa G. and Pavlov, Alexander A. and Jimenez, Jose L. and McKay, Christopher P. and Worsnop, Douglas R. and Toon, Owen B. and Tolbert, Margaret A.},
  Journal                  = {Geophysical Research Letters},
  Year                     = {2004},
  Note                     = {53 Mineralogical and Geological Chemistry Department of Chemistry and Biochemistry and Cooperative Institute for Research in Environmental Sciences,University of Colorado at Boulder,Boulder,CO,USA. Journal 0094-8276 written in English.},
  Number                   = {17},
  Pages                    = {L17S08/1-L17S08/4},
  Volume                   = {31},

  Abstract                 = {Previous lab. studies of haze aerosols analogous to those in Titan's atm. have shown evidence of the formation of polycyclic arom. hydrocarbons (PAHs). If present, PAHs may provide a mechanism for both particle formation and nitrogen incorporation. We have conducted new expts. simulating Titan haze prodn. using an Aerosol Mass Spectrometer (AMS). The AMS allows us to examine the chem. structure of the haze particles under a variety of starting conditions in real-time and without collection. Our results for particles produced from a mixt. of 10% CH4 in N2 are consistent with a large, fraction of aroms., including specific m/z peaks likely due to PAHs. However, at lower concns. of CH4 (1% and lower), the mass fraction of PAHs greatly diminishes, and an aliph. pathway dominates. Haze contg. sediments may trace the history of methane on Titan through their compn. The implications for Titan haze and for observations from the Huygens probe are discussed. [on SciFinder (R)]},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Tran03a,
  Title                    = {The photochemical formation of a Titan haze analog. Structural analysis by x-ray photoelectron and infrared spectroscopy},
  Author                   = {Tran, Buu N. and Ferris, James P. and Chera, John J.},
  Journal                  = {Icarus},
  Year                     = {2003},
  Note                     = {CAN 139:39331 53-9 Mineralogical and Geological Chemistry Department of Chemistry,Rensselaer Polytechnic Institute,Troy,NY,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 74-85-1 (Ethene); 74-86-2 (Ethyne); 1070-71-9 (2-Propynenitrile); 1333-74-0 (Hydrogen); 7727-37-9 (Nitrogen) Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy)},
  Number                   = {1},
  Pages                    = {114-124},
  Volume                   = {162},

  Abstract                 = {The photochem. flow reactor has been modified to minimize the incorporation of oxygen and other impurities in the photoproducts. A mixt. of gases that approx. their mixing ratios on Titan (N2, CH4, H2, C2H2, C2H4, and HC3N) (0.98, 0.018, 0.002, 3.5 * 10-4, 3 * 10-4, 1.7 * 10-5, resp.) was irradiated in the flow photochem. reactor using a 185-nm source to give a Titan haze analog as a solid product. XPS gave a compn. of 93.3% C, 5.3% N, and 1.4% O. Of the 93.3% carbon, high-resoln. XPS revealed that 81.2% was present as C-H, C-C, and C:C groups, 12.1% may be C-O, C-N, C:N, -C.tplbond.N, and/or C.tplbond.N groups, 5.3% as a -C.tplbond.N group. The peak for N was sym. and was assigned to the -C.tplbond.N while that for oxygen was assigned to the C:O and/or the C-O group. Some of these assignments were confirmed by FTIR spectroscopy. The polymeric product had a C:N ratio of 17.6, which is significantly greater than that for Titan haze analogs prepd. in discharge reactions. When the polymer was exposed to air for seven days the oxygen content increased by 6% along with an increase in the IR absorption at 1710 cm-1 assigned to the C:O group of a ketone. The oxidn. is attributed to the reaction of oxygen with free radicals trapped in the polymer matrix. It is proposed that the photochem. initiation of Titan haze formation from compds. formed from starting materials formed high in Titan's atm. is a more plausible model than haze formed in reactions initiated by solely by discharges. These data will be helpful in the interpretation of the data returned from the Huygens probe of the Cassini mission. [on SciFinder (R)]},
  Keywords                 = {IR spectroscopy (Fourier-transform; photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy); Satellites (Titan; photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy); Reactors (flow, photochem.; photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy); IR absorption; Photoelectrons; Titan; X-ray photoelectron spectra (photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy); Ketones Role: GOC (Geological or astronomical occurrence), PRP (Properties), OCCU (Occurrence) (photochem. simulated generation of an analog of Titan haze from a mixt. contg. methane, hydrogen, oxygen, nitrogen, and acetylene with structural anal. by x-ray photoelectron and IR spectroscopy) Titan satellite haze formation photochem nitrogen methane hydrogen oxygen},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

@Article{Tran08,
  Title                    = {Titan's atmospheric chemistry: Photolysis of gas mixtures containing hydrogen cyanide and carbon monoxide at 185 and 254 nm},
  Author                   = {Tran, Buu N. and Force, Michael and Briggs, Robert G. and Ferris, James P. and Persans, Peter and Chera, John J.},
  Journal                  = {Icarus},
  Year                     = {2008},

  Month                    = jan,
  Number                   = {1},
  Pages                    = {224--232},
  Volume                   = {193},

  Abstract                 = {The formation of organic compounds in the atmosphere of Titan is an ongoing process of the generation of complex organics from the simplest hydrocarbon, methane. Solar radiation and magnetosphere electrons are the main energy sources that drive the reactions in Titan's atmosphere. Since energy from solar radiation is 200 times greater than that from magnetosphere electrons, we have investigated the products formed by the action of UV radiation (185 and 254 nm) on a mixture of gases containing nitrogen, methane, hydrogen, acetylene, ethylene, and cyanoacetylene, the basic gas mixture (BGM) that simulates aspects of Titan's atmosphere using a flow reactor [Tran, B.N., Ferris, J.P., Chera, J.J., 2003a. Icarus 162, 114-124; Tran, B.N., Joseph, J.C., Force, M., Briggs, R.G., Vuitton, V., Ferris, J.P., 2005. Icarus 177, 106-115]. The present research extends these studies by the addition of carbon monoxide and hydrogen cyanide to the BGM. Quantum yields for the loss of reactants and the formation of volatile products were determined and compared with those measured in the absence of the hydrogen cyanide and carbon monoxide. The GCMS analyses of the volatile photolysis products from the BGM, with added hydrogen cyanide, had a composition similar to that of the BGM while the photolysis products of the BGM with added carbon monoxide contained many oxygenated compounds. The infrared spectrum of the corresponding solid product revealed the absorption band of a ketone group, which was probably formed from the reaction of carbon monoxide with the free radicals generated by photolysis of acetylene and ethylene. Of particular interest was the observation that the addition of HCN to the gas mixture only resulted in a very small change in the C/N ratio and in the intensity of the CN frequency at 2210 cm-1 in the infrared spectrum suggesting that little HCN is incorporated into the haze analog. The C/N ratio of the haze analogs was found to be in the 10-12 range. The UV spectra of the solid products formed when HCN or CO added to the BGM is similar to the UV absorption formed from the BGM alone. This result is consistent with absence of additional UV chromophores to the solid product when these mixtures are photolyzed. The following photoproducts, which were not starting materials in our photochemical studies, have been observed on Titan: acetonitrile, benzene, diacetylene, ethane, propene, propane, and propyne.},
  File                     = {Tran08.pdf:Tran08.pdf:PDF},
  Keywords                 = {Titan, Photochemistry, Atmospheres, chemistry, Satellites, composition, Organic chemistry},
  Owner                    = {pernot},
  Timestamp                = {2007.12.14},
  Url                      = {http://www.sciencedirect.com/science/article/B6WGF-4PTMXYR-1/2/0f4c212fd71f90b40fd6d1bc78e18b25}
}

@Article{Tran03,
  Title                    = {Simulation of Titan haze formation using a photochemical flow reactor. The optical constants of the polymer},
  Author                   = {Tran, Buu N. and Joseph, Jeffrey C. and Ferris, James P. and Persans, Peter D. and Chera, John J.},
  Journal                  = {Icarus},
  Year                     = {2003},
  Note                     = {CAN 140:185013 53-9 Mineralogical and Geological Chemistry Department of Chemistry,Rensselaer Polytechnic Institute,Troy,NY,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 74-85-1 (Ethylene); 74-86-2 (Ethyne); 1070-71-9 (Cyanoacetylene) Role: GOC (Geological or astronomical occurrence), RCT (Reactant), OCCU (Occurrence), RACT (Reactant or reagent) (component mixts.; simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy)},
  Number                   = {2},
  Pages                    = {379-390},
  Volume                   = {165},

  Abstract                 = {Solar UV is the principal energy source impinging the atm. of Titan while the energy from the electrons in Saturn's magnetosphere is less than 0.5% of the UV light. Titan haze analogs were prepd. by the photolysis of a mixt. of gases that simulate the compn. of its atm. (nitrogen, methane, hydrogen, acetylene, ethylene, and cyanoacetylene). The real (n) and imaginary (k) parts of the complex refractive index of haze analogs formed from four different gas mixts. were calcd. from the spectral properties of the solid polymer in UV-visible, near IR and IR wavelength spectral regions. The value of n was const. at 1.6+-0.1 throughout the 0.2-2.5 mm region. The variation of k with wavelength for the values derived for Titan has a lower error than the abs. values of k so the more significant comparisons are with the slopes of the k(l) plots in the UV-VIS region. Three of the photochem. Titan haze analogs had slopes comparable to those derived for Titan from the Voyager data (Rages and Pollack, 1980, Icarus 41, 119-130; McKay and Toon, 1992, in: Proceedings of the Symposium on Titan, in: ESA SP, Vol. 338, pp. 185-190). The slopes of the k(l) plots for haze analogs prepd. by spark discharge (Khare et al., 1984, Icarus 60, 127-137) and plasma discharge (Ramirez et al., 2002, Icarus 156, 515-529) were also comparable to Titan's. These findings show that the k(l) plots do not differentiate between different lab. simulations of atm. chem. on Titan in the UV-VIS near IR region (0.2-2.5 m). There is a large difference between the k(l) in the IR between the haze analogs prepd. photochem. and analogs prepd. using a plasma discharges (Khare et al., 1984, Icarus 60, 127-137; Coll et al., 1999, Planet. Space Sci. 47, 1331-1340; Khare et al., 2002, Icarus 160,172-182). The C/N ratio in the haze analog prepd. by discharges is in the 2-11 range while that of the photochem. analogs is in the 18-24 range. The use of discharges and UV light for initiating the chem. in Titan's atm. is discussed. [on SciFinder (R)]},
  Keywords                 = {Titan (atm.; simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy); Refractive index (of polymers; simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy); Electric discharge (prodn. of polymers; simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy); Polymers Role: CPS (Chemical process), GPR (Geological or astronomical process), PEP (Physical, engineering or chemical process), PRP (Properties), PROC (Process) (refractive index; simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy); Photochemistry; Simulation and Modeling; Solar UV radiation; Titan (simulation of Titan haze formation using a photochem. flow reactor and detn. of optical consts. of polymers produced by solar UV energy) photochem Titan haze simulation spectroscopy refractive index; elec discharge polymer solid haze Titan tholin solar UV},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

