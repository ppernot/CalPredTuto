@Article{Karkoschka98,
  Title                    = {Methane, ammonia, and temperature measurements of the Jovian planets and Titan from CCD-spectrophotometry},
  Author                   = {Karkoschka, Erich},
  Journal                  = {Icarus},
  Year                     = {1998},
  Note                     = {CAN 129:247996 53-9 Mineralogical and Geological Chemistry Lunar and Planetary Laboratory,University of Arizona,Tucson,AZ,USA. Journal 0019-1035 written in English. 74-82-8 (Methane); 7664-41-7 (Ammonia) Role: GOC (Geological or astronomical occurrence), OCCU (Occurrence) (methane, ammonia, and temp. measurements of Jovian planets and Titan from CCD-spectrophotometry)},
  Number                   = {1},
  Pages                    = {134-146},
  Volume                   = {133},

  Abstract                 = {Full-disk albedo spectra of the Jovian planets and Titan were derived from observations at the European Southern Observatory in July 1995. The spectra extend from 300-1050 nm wavelength. The spectral resoln. is 0.4 nm between 520 and 995 nm, and 1 nm elsewhere. The accuracy of the albedo calibration is 4%. UBV magnitudes were also detd. Raman scattering was quantified for each planet. CH4 and NH3 bands are at 0.4 nm spectral resoln., including a new band at 930 nm wavelength which is probably due to NH3. Maps of the absorptions variation across the disks of Jupiter and Saturn are displayed. Saturn's spectrum is undisturbed by light from its rings due to the edge-on geometry during the observations. The albedo of Uranus near 1 mm wavelength has dropped almost 10% between 1993 and 1995, while there was no change in the UV. The signature of light from Titan's surface yielded a path length of 4 km-am of CH4 in Titan's atm. The temp. dependence of the width of the 890-nm CH4 band was used to measure temp. variations at three altitude levels, resulting in the first temp. maps of Jupiter and Saturn based on reflected sunlight. Jupiter displays a banded temp. structure with some discrete features of a few Kelvin amplitude. Saturn's north-south temp. asymmetry has reversed since the Voyager observations. (c) 1998 Academic Press. [on SciFinder (R)]},
  Keywords                 = {Jupiter; Neptune; Planets; Saturn; Titan; Uranus (atm. of; methane, ammonia, and temp. measurements of Jovian planets and Titan from CCD-spectrophotometry); Temperature (methane, ammonia, and temp. measurements of Jovian planets and Titan from CCD-spectrophotometry) Jovian planet Titan atm compn spectrophotometry; methane ammonia atm Jupiter Saturn Titan; Uranus Neptune atm methane ammonia spectrophotometry},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

