@Article{Lemmon02,
  Title                    = {Methane abundance on Titan, measured by the Space Telescope Imaging Spectrograph},
  Author                   = {Lemmon, Mark T. and Smith, Peter H. and Lorenz, Ralph D.},
  Journal                  = {Icarus},
  Year                     = {2002},
  Note                     = {CAN 138:190928 53-9 Mineralogical and Geological Chemistry Department of Atmospheric Sciences,Texas A\&M University,College Station,TX,USA. Journal 0019-1035 written in English. 74-82-8 (Methane) Role: GOC (Geological or astronomical occurrence), GPR (Geological or astronomical process), OCCU (Occurrence), PROC (Process) (methane abundance on Titan, measured by the Space Telescope Imaging Spectrograph)},
  Number                   = {2},
  Pages                    = {375-385},
  Volume                   = {160},

  Abstract                 = {Although methane is the dominant absorber in Titan's reflection spectrum, the amt. of methane in the atm. has only been detd. to an order of magnitude. We analyzed spectra from the Space Telescope Imaging Spectrograph, looking at both a bright surface region (700-km radius) and a dark surface region. The difference between the spectra of the two regions is attributed to light that has scattered off the surface, and therefore made a round-trip through all of Titan's methane. Considering only absorption, the shape of the difference spectrum provides an upper limit on methane abundance of 3.5 km-am. Modeling the multiple scattering in the atm. further constrains the methane abundance to 2.63+-0.17 km-am. In the absence of supersatn. and with a simplified methane vertical profile, this corresponds to a surface methane-mole fraction near 3.8% and a relative humidity of 0.32. With supersatn. near the tropopause, the surface methane mole fraction could be as low as 3%. [on SciFinder (R)]},
  Keywords                 = {Titan (methane abundance on Titan, measured by the Space Telescope Imaging Spectrograph) methane Titan},
  Owner                    = {pernot},
  Timestamp                = {2007.11.16}
}

